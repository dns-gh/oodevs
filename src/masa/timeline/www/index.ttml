<!DOCTYPE html>
<html lang="en">
    <head>
        <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1"/>
        <title>Timeline</title>
        <meta name="description" content=""/>
        <link rel="shortcut icon" href="favicon.ico" />
        <link rel="stylesheet" type="text/css" href="lib/bootstrap.min.css"/>
        <link rel="stylesheet" type="text/css" href="lib/font-awesome.min.css"/>
        <link rel="stylesheet" type="text/css" href="lib/datepicker.css"/>
        <link rel="stylesheet" type="text/css" href="css/style.css"/>
        <!--[if IE ]>
        <link rel="stylesheet" type="text/css" href="css/ie.css" />
        <![endif]-->
    </head>
    <body>
        <div id="session_container" class="container-fluid hide">
            <div class="sub_bar">
                <div id="session"></div>
                <div id="settings"></div>
                <div id="range_popovers"></div>
            </div>
        </div>
        <div id="session_errors" class="container hide">
            <div class="span12">
                <div class="row" id="errors">
                </div>
            </div>
        </div>
        <script type="text/template" id="error_template">
            <div class="alert alert-error">
                <strong>{{brief}}</strong>
                {{data}}
            </div>
        </script>
        <script type="text/template" id="tick_template">
            <div{{#if major}} class="major"{{/if}}>{{top}}</div>
            {{#if bottom}}
            <div>{{bottom}}</div>
            {{/if}}
        </script>
        <script type="text/template" id="event_template">
            {{#if has_html}}
            <div class="event_box">
                <div class="event {{color}}">
                    {{#if has_checkbox}}
                        {{#if has_done}}
                        <i class="checker icon-{{icon}} icon-white"></i>
                        {{else}}
                        <i class="checker icon-{{icon}} icon-white"></i>
                        {{/if}}
                    {{else}}
                        <i class="icon-bookmark icon-white"></i>
                    {{/if}}
                    {{#if has_error}}
                    <i class="event_error icon-warning-sign"></i>
                    {{/if}}
            {{/if}}
                    {{#if has_time}}
                    {{begin}}
                    {{#if end}}
                    <i class="icon-angle-right icon-white"></i>
                    {{end}}
                    {{/if}}
                    <span class="vertical_separator"></span>
                    {{/if}}
                    {{brief}}
            {{#if has_html}}
                </div>
            </div>
            {{/if}}
        </script>
        <script type="text/template" id="event_settings_template">
            <div class="modal fade">
                <div class="modal-header">
                    <a class="close" data-dismiss="modal">Ã—</a>
                    <h4>{{header}}</h4>
                </div>
                <form class="modal-body form-horizontal">
                    [[ template "input_text" mk "id" "evt_name" "label" "{{i18n 'Name'}}" "value" "{{name}}" ]]
                    [[ template "input_checkbox" mk "id" "evt_done" "label" "{{i18n 'Mark as Done'}}" "value" "{{#bool done}} checked='checked'{{/bool}}" ]]
                    [[ template "input_datetime" mk "id" "evt_begin" "label" "{{i18n 'Start'}}" "value" "value='{{begin}}'" ]]
                    [[ template "input_datetime" mk "id" "evt_end" "label" "{{i18n 'End'}}" "value" "{{end}}" "check" "{{#bool has_end}} checked='checked'{{/bool}}" ]]
                    [[ template "input_textarea" mk "id" "evt_info" "label" "{{i18n 'Description'}}" "value" "{{info}}" ]]
                </form>
                <div class="modal-footer">
                    <a class="btn btn-inverse apply">{{accept}}</a>
                    <a class="btn btn-inverse" data-dismiss="modal">{{reject}}</a>
                </div>
            </div>
        </script>
        <script type="text/javascript" src="lib/jquery[[       if .debug ]][[ else ]].min[[ end ]].js"></script>
        <script type="text/javascript" src="lib/json2[[        if .debug ]][[ else ]].min[[ end ]].js"></script>
        <script type="text/javascript" src="lib/underscore[[   if .debug ]][[ else ]].min[[ end ]].js"></script>
        <script type="text/javascript" src="lib/backbone[[     if .debug ]][[ else ]].min[[ end ]].js"></script>
        <script type="text/javascript" src="lib/handlebars[[   if .debug ]][[ else ]].min[[ end ]].js"></script>
        <script type="text/javascript" src="lib/moment[[       if .debug ]][[ else ]].min[[ end ]].js"></script>
        <script type="text/javascript" src="lib/d3.v3[[        if .debug ]][[ else ]].min[[ end ]].js"></script>
        <script type="text/javascript" src="lib/bootstrap[[    if .debug ]][[ else ]].min[[ end ]].js"></script>
        <script type="text/javascript" src="lib/datepicker[[   if .debug ]][[ else ]].min[[ end ]].js"></script>
        <script type="text/javascript" src="js/i18n[[          if .debug ]][[ else ]].min[[ end ]].js"></script>
        <script type="text/javascript" src="js/core[[          if .debug ]][[ else ]].min[[ end ]].js"></script>
    </body>
</html>
