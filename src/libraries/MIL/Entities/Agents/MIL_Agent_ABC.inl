// Created: NLD 2004-09-13
// -----------------------------------------------------------------------------
inline
bool MIL_Agent_ABC::operator==( const MIL_Agent_ABC& rhs ) const
{
    return nID_ == rhs.nID_;
}

// -----------------------------------------------------------------------------
// Name: MIL_Agent_ABC::operator!=
// Created: NLD 2004-09-13
// -----------------------------------------------------------------------------
inline
bool MIL_Agent_ABC::operator!=( const MIL_Agent_ABC& rhs ) const
{
    return nID_ != rhs.nID_;
}