<?xml version="1.0" encoding="UTF-8"?>
<!-- edited with XMLSpy v2005 sp2 U (http://www.altova.com) by  () -->
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified" attributeFormDefault="unqualified">
	<xs:element name="orbat">
		<xs:annotation>
			<xs:documentation xml:lang="en">Root element, describes the order of battle.</xs:documentation>
			<xs:documentation xml:lang="fr">Element racine, décrit l'ordre de bataille.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="sides">
					<xs:annotation>
						<xs:documentation xml:lang="en">Defines the list of sides.</xs:documentation>
						<xs:documentation xml:lang="fr">Définit la liste des camps.</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:sequence>
							<xs:element ref="side" minOccurs="0" maxOccurs="unbounded"/>
						</xs:sequence>
					</xs:complexType>
					<!-- Constraints -->
					<xs:key name="sideKey">
						<xs:annotation>
							<xs:documentation xml:lang="en">Defines key for side.</xs:documentation>
							<xs:documentation xml:lang="fr">Définit une clé pour le camp.</xs:documentation>
						</xs:annotation>
						<xs:selector xpath="side"/>
						<xs:field xpath="@id"/>
					</xs:key>
					<xs:unique name="knowledgeGroupIdUnicity">
						<xs:annotation>
							<xs:documentation xml:lang="en">Checks unicity of knowledge group identifiers over whole ORBAT.</xs:documentation>
							<xs:documentation xml:lang="fr">Vérifie l'unicité de l'identifiant du groupe de connaissance au sein de l'ordre de bataille.</xs:documentation>
						</xs:annotation>
						<xs:selector xpath="side/communication/knowledge-group"/>
						<xs:field xpath="@id"/>
					</xs:unique>
					<xs:unique name="objectIdUnicity">
						<xs:annotation>
							<xs:documentation xml:lang="en">Checks unicity of object identifiers over whole ORBAT.</xs:documentation>
							<xs:documentation xml:lang="fr"> Vérifie l'unicité de l'identifiant d'objet au sein de l'ordre de bataille.</xs:documentation>
						</xs:annotation>
						<xs:selector xpath="side/objects/object"/>
						<xs:field xpath="@id"/>
					</xs:unique>
					<xs:unique name="populationIdUnicity">
						<xs:annotation>
							<xs:documentation xml:lang="en">Checks unicity of population identifiers over whole ORBAT.</xs:documentation>
							<xs:documentation xml:lang="fr"> Vérifie l'unicité de l'identifiant de population au sein de l'ordre de bataille.</xs:documentation>
						</xs:annotation>
						<xs:selector xpath="side/populations/population"/>
						<xs:field xpath="@id"/>
					</xs:unique>
					<xs:unique name="automatIdUnicity">
						<xs:annotation>
							<xs:documentation xml:lang="en">Checks unicity of automat identifiers over whole ORBAT.</xs:documentation>
							<xs:documentation xml:lang="fr"> Vérifie l'unicité de l'identifiant d'automate au sein de l'ordre de bataille.</xs:documentation>
						</xs:annotation>
						<xs:selector xpath=".//formation/automat|.//automat/automat"/>
						<xs:field xpath="@id"/>
					</xs:unique>
					<xs:unique name="unitIdUnicity">
						<xs:annotation>
							<xs:documentation xml:lang="en">Checks unicity of unit identifiers over whole ORBAT.</xs:documentation>
							<xs:documentation xml:lang="fr"> Vérifie l'unicité de l'identifiant d'unité au sein de l'ordre de bataille.</xs:documentation>
						</xs:annotation>
						<xs:selector xpath=".//automat/unit"/>
						<xs:field xpath="@id"/>
					</xs:unique>
					<xs:unique name="intelligenceIdUnicity">
						<xs:annotation>
							<xs:documentation xml:lang="en">Checks unicity of intelligence identifiers over whole ORBAT.</xs:documentation>
							<xs:documentation xml:lang="fr"> Vérifie l'unicité de l'identifiant de l'intelligence au sein de l'ordre de bataille.</xs:documentation>
						</xs:annotation>
						<xs:selector xpath=".//intelligence"/>
						<xs:field xpath="@id"/>
					</xs:unique>
				</xs:element>
				<xs:element name="diplomacies">
					<xs:annotation>
						<xs:documentation xml:lang="en">Defines diplomacy relationships between sides.</xs:documentation>
						<xs:documentation xml:lang="fr">Définit les relations diplomatiques entre les camps.</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:sequence>
							<xs:element name="side" type="diplomacyRelationshipType" minOccurs="0" maxOccurs="unbounded">
								<xs:unique name="relationshipTargetUnicity">
									<xs:annotation>
										<xs:documentation xml:lang="en">Checks that diplomacy is only defined once for each target side.</xs:documentation>
										<xs:documentation xml:lang="fr">Vérifie que la diplomatie est unique pour chacun des autres camps.</xs:documentation>
									</xs:annotation>
									<xs:selector xpath="relationship"/>
									<xs:field xpath="@side"/>
								</xs:unique>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
					<xs:unique name="relationshipSourceUnicity">
						<xs:annotation>
							<xs:documentation xml:lang="en"> Checks that diplomacy is only defined once for each source side.</xs:documentation>
							<xs:documentation xml:lang="fr"> Vérifie que la diplomatie est unique pour chaque camp.</xs:documentation>
						</xs:annotation>
						<xs:selector xpath="side"/>
						<xs:field xpath="@id"/>
					</xs:unique>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
		<xs:keyref name="relationshipSideKeyRef" refer="sideKey">
			<xs:annotation>
				<xs:documentation xml:lang="en">Checks that referenced side exists.</xs:documentation>
				<xs:documentation xml:lang="fr">Vérifie que le camp désigné existe.</xs:documentation>
			</xs:annotation>
			<xs:selector xpath="diplomacies/side"/>
			<xs:field xpath="@id"/>
		</xs:keyref>
		<xs:keyref name="diplomacyRelationshipSideKeyRef" refer="sideKey">
			<xs:annotation>
				<xs:documentation xml:lang="en">Checks that relationshup side exists.</xs:documentation>
				<xs:documentation xml:lang="fr">Vérifie que le camp auquel on affecte une diplomatie existe.</xs:documentation>
			</xs:annotation>
			<xs:selector xpath="diplomacies/side/relationship"/>
			<xs:field xpath="@side"/>
		</xs:keyref>
	</xs:element>
	<xs:complexType name="entityType">
		<xs:annotation>
			<xs:documentation xml:lang="en">Base type for entities.</xs:documentation>
			<xs:documentation xml:lang="fr">Type de base pour les entités.</xs:documentation>
		</xs:annotation>
		<xs:attribute name="id" type="xs:nonNegativeInteger" use="required">
			<xs:annotation>
				<xs:documentation xml:lang="en">Defines unique identifier for internal references.</xs:documentation>
				<xs:documentation xml:lang="fr">Définit un identifiant unique pour les références internes.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="name" type="xs:string" use="required">
			<xs:annotation>
				<xs:documentation xml:lang="en">Defines display name of entity.</xs:documentation>
				<xs:documentation xml:lang="fr">Définit un nom pour l'affichage de l'entité.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<xs:element name="side">
		<xs:annotation>
			<xs:documentation xml:lang="en">Defines side composition.</xs:documentation>
			<xs:documentation xml:lang="fr">Définit la composition du camp.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:complexContent>
				<xs:extension base="entityType">
					<xs:all>
						<xs:element name="communication">
							<xs:annotation>
								<xs:documentation xml:lang="en">Defines communication hierarchy.</xs:documentation>
								<xs:documentation xml:lang="fr">Définit la hiérarchie de communication.</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:sequence>
									<xs:element ref="knowledge-group" minOccurs="0" maxOccurs="unbounded"/>
								</xs:sequence>
							</xs:complexType>
							<xs:key name="knowledgeGroupKey">
								<xs:annotation>
									<xs:documentation xml:lang="en">Defines key for knowledge group.</xs:documentation>
									<xs:documentation xml:lang="fr">Définit une clé pour le groupe de connaissance.</xs:documentation>
								</xs:annotation>
								<xs:selector xpath="knowledge-group"/>
								<xs:field xpath="@id"/>
							</xs:key>
						</xs:element>
						<xs:element name="logistic">
							<xs:annotation>
								<xs:documentation xml:lang="en">Defines logistics hierarchy.</xs:documentation>
								<xs:documentation xml:lang="fr">Définit une hiérarchie logistique.</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:sequence>
									<xs:element name="automat" type="logisticRelationshipType" minOccurs="0" maxOccurs="unbounded"/>
								</xs:sequence>
							</xs:complexType>
							<xs:unique name="logisticRelationhipIdUnicity">
								<xs:selector xpath="automat"/>
								<xs:field xpath="@id"/>
							</xs:unique>
						</xs:element>
						<xs:element name="objects">
							<xs:annotation>
								<xs:documentation xml:lang="en">Defines objects belonging to side.</xs:documentation>
								<xs:documentation xml:lang="fr">Définit les objets appartenant au camp.</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:sequence>
									<xs:element ref="object" minOccurs="0" maxOccurs="unbounded"/>
								</xs:sequence>
							</xs:complexType>
						</xs:element>
						<xs:element name="populations">
							<xs:annotation>
								<xs:documentation xml:lang="en">Defines populations belonging to side.</xs:documentation>
								<xs:documentation xml:lang="fr">Définit les populations appartenant au camp.</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:sequence>
									<xs:element ref="population" minOccurs="0" maxOccurs="unbounded"/>
								</xs:sequence>
							</xs:complexType>
						</xs:element>
						<xs:element name="tactical">
							<xs:annotation>
								<xs:documentation xml:lang="en">Defines tactical hierarchy (formations, automats and units).</xs:documentation>
								<xs:documentation xml:lang="fr">Définit la hiérarchie tactique (formations, automates et unités).</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:sequence>
									<xs:element ref="formation" minOccurs="0" maxOccurs="unbounded"/>
								</xs:sequence>
							</xs:complexType>
							<xs:key name="automatKey">
								<xs:annotation>
									<xs:documentation xml:lang="en">Defines key for automat.</xs:documentation>
									<xs:documentation xml:lang="fr">Définit une clé pour l'automate.</xs:documentation>
								</xs:annotation>
								<xs:selector xpath=".//formation/automat|.//automat/automat"/>
								<xs:field xpath="@id"/>
							</xs:key>
						</xs:element>
					</xs:all>
					<xs:attribute name="type" type="karmaType" use="required">
						<xs:annotation>
							<xs:documentation xml:lang="en">Defines side type (karma).</xs:documentation>
							<xs:documentation xml:lang="fr">Définit le type de camp (karma).</xs:documentation>
						</xs:annotation>
					</xs:attribute>
				</xs:extension>
			</xs:complexContent>
		</xs:complexType>
		<!-- knowledge group constraints -->
		<xs:keyref name="knowledgeGroupKeyRef" refer="knowledgeGroupKey">
			<xs:annotation>
				<xs:documentation xml:lang="en">Checks that knowledge group exists in same side.</xs:documentation>
				<xs:documentation xml:lang="fr">Vérifie que le groupe de connaissance existe dans le même camp.</xs:documentation>
			</xs:annotation>
			<xs:selector xpath=".//formation/automat|.//automat/automat"/>
			<xs:field xpath="@knowledge-group"/>
		</xs:keyref>
		<!-- objects constraints -->
		<xs:keyref name="objectTc2KeyRef" refer="automatKey">
			<xs:annotation>
				<xs:documentation xml:lang="en">Checks that referenced automat exists in same side.</xs:documentation>
				<xs:documentation xml:lang="fr">Vérifie que l'automate existe dans le même camp.</xs:documentation>
			</xs:annotation>
			<xs:selector xpath="objects/object/attributes/tc2"/>
			<xs:field xpath="@id"/>
		</xs:keyref>
		<!-- logistics constraints -->
		<xs:keyref name="logisticSubordinateKeyRef" refer="automatKey">
			<xs:annotation>
				<xs:documentation xml:lang="en">Checks existence of source and target logistic automats referenced in links.</xs:documentation>
				<xs:documentation xml:lang="fr">Vérifie l'existence de l'automate source et cible du lien logistique.</xs:documentation>
			</xs:annotation>
			<xs:selector xpath="logistic/automat|logistic/automat/subordinate"/>
			<xs:field xpath="@id"/>
		</xs:keyref>
	</xs:element>
	<xs:simpleType name="karmaType">
		<xs:annotation>
			<xs:documentation xml:lang="en">Defines side orientation.</xs:documentation>
			<xs:documentation xml:lang="fr">Définit l'orientation du camp.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="friend"/>
			<xs:enumeration value="enemy"/>
			<xs:enumeration value="neutral"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:element name="knowledge-group">
		<xs:annotation>
			<xs:documentation xml:lang="en">Defines a knowledge/communication group.</xs:documentation>
			<xs:documentation xml:lang="fr">Définit un groupe de connaissance.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:complexContent>
				<xs:extension base="entityType">
					<xs:attribute name="type" type="xs:string" use="required">
						<xs:annotation>
							<xs:documentation xml:lang="en">Defines the type of knowledge group, references "Communications.xml".</xs:documentation>
							<xs:documentation xml:lang="fr">Définit le type de groupe de connaissance. Renvoie vers "Communications.xml".</xs:documentation>
						</xs:annotation>
					</xs:attribute>
				</xs:extension>
			</xs:complexContent>
		</xs:complexType>
	</xs:element>
	<xs:complexType name="logisticRelationshipType">
		<xs:annotation>
			<xs:documentation xml:lang="en">Defines logistic link between an automat and its logistic subordinates.</xs:documentation>
			<xs:documentation xml:lang="fr">Définit le lien logistique entre un automate et ses subordonnés logistiques.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="subordinate" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation xml:lang="en">Defines link nature and parameters for specified logistic subordinate.</xs:documentation>
					<xs:documentation xml:lang="fr">Définit la nature du lien et les paramètres pour le subordonné logistique.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="quotas" type="resourcesType" minOccurs="0">
							<xs:annotation>
								<xs:documentation xml:lang="en">Defines quotas granted to subordinate by logistic superior.</xs:documentation>
								<xs:documentation xml:lang="fr">Définit les quotas alloués au subordonné par son supérieur logistique.</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
					<xs:attribute name="id" type="xs:nonNegativeInteger" use="required">
						<xs:annotation>
							<xs:documentation xml:lang="en">Identifies the subordinate (automat).</xs:documentation>
							<xs:documentation xml:lang="fr">Identifiant de l'automate subordonné</xs:documentation>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute name="link" type="logisticLinkType" use="required">
						<xs:annotation>
							<xs:documentation xml:lang="en">Defines the type of logistic link to be configured.</xs:documentation>
							<xs:documentation xml:lang="fr">Définit le type du lien logistique à configurer.</xs:documentation>
						</xs:annotation>
					</xs:attribute>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
		<xs:attribute name="id" type="xs:nonNegativeInteger" use="required">
			<xs:annotation>
				<xs:documentation xml:lang="en">Identifies the automat for which to define logistic links to.</xs:documentation>
				<xs:documentation xml:lang="fr">Identifiant de l'automate recevant le lien logistique.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<xs:simpleType name="logisticLinkType">
		<xs:annotation>
			<xs:documentation xml:lang="en">Defines types of logistic links.</xs:documentation>
			<xs:documentation xml:lang="fr">Définit les types de liens logistiques.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="maintenance"/>
			<xs:enumeration value="medical"/>
			<xs:enumeration value="supply"/>
			<xs:enumeration value="tc2"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:element name="object">
		<xs:annotation>
			<xs:documentation xml:lang="en">Defines a tactical object.</xs:documentation>
			<xs:documentation xml:lang="fr">Définit un objet tactique.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:complexContent>
				<xs:extension base="entityType">
					<xs:all>
						<xs:element name="shape" type="shapeType">
							<xs:annotation>
								<xs:documentation xml:lang="en">Defines the location and shape of the object.</xs:documentation>
								<xs:documentation xml:lang="fr">Définit la géolocalisation et la forme de l'objet.</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="attributes">
							<xs:annotation>
								<xs:documentation xml:lang="en">Defines specific object attributes.</xs:documentation>
								<xs:documentation xml:lang="fr">Définit les attributs spécifiques à l'objet.</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:all>
									<xs:element name="activity-time" minOccurs="0">
										<xs:annotation>
											<xs:documentation xml:lang="en">Defines attributes related to object activity time.</xs:documentation>
											<xs:documentation xml:lang="fr">Définit les attributs relatifs au temps d'activité de l'objet.</xs:documentation>
										</xs:annotation>
										<xs:complexType>
											<xs:attribute name="value" type="xs:nonNegativeInteger">
												<xs:annotation>
													<xs:documentation xml:lang="en">Defines the activity time in seconds.</xs:documentation>
													<xs:documentation xml:lang="fr">Définit la durée d'activation en secondes.</xs:documentation>
												</xs:annotation>
											</xs:attribute>
										</xs:complexType>
									</xs:element>
									<xs:element name="obstacle" minOccurs="0">
										<xs:annotation>
											<xs:documentation xml:lang="en">Defines attributes related to obstacle capacities of the object.</xs:documentation>
											<xs:documentation xml:lang="fr">Définit les attributs relatifs aux capacités de l'objet à faire obstacle.</xs:documentation>
										</xs:annotation>
										<xs:complexType>
											<xs:attribute name="activated" type="xs:boolean" use="required">
												<xs:annotation>
													<xs:appinfo>Not read by simulation.</xs:appinfo>
													<xs:documentation xml:lang="en">Defines whether the obstacle is initially activated or not.</xs:documentation>
													<xs:documentation xml:lang="fr">Définit si l'obstacle est initialement actif.</xs:documentation>
												</xs:annotation>
											</xs:attribute>
											<xs:attribute name="type" type="obstacleType" use="required">
												<xs:annotation>
													<xs:documentation xml:lang="en">Defines the type of obstacle.</xs:documentation>
													<xs:documentation xml:lang="fr">Définit le type d'obstacle.</xs:documentation>
												</xs:annotation>
											</xs:attribute>
										</xs:complexType>
									</xs:element>
									<xs:element name="tc2" minOccurs="0">
										<xs:annotation>
											<xs:documentation xml:lang="en">Defines attributes related to logistic superior (automat TC2).</xs:documentation>
											<xs:documentation xml:lang="fr">Définit les attributs concernant le supérieur logistique (automate TC2).</xs:documentation>
										</xs:annotation>
										<xs:complexType>
											<xs:attribute name="id" type="xs:nonNegativeInteger" use="required">
												<xs:annotation>
													<xs:documentation xml:lang="en">Defines reference to the logistic superior (TC2 automat).</xs:documentation>
													<xs:documentation xml:lang="fr">Identifiant du supérieur logistique (automate TC2).</xs:documentation>
												</xs:annotation>
											</xs:attribute>
										</xs:complexType>
									</xs:element>
								</xs:all>
							</xs:complexType>
						</xs:element>
					</xs:all>
					<xs:attribute name="type" type="objectType" use="required">
						<xs:annotation>
							<xs:documentation xml:lang="en">Defines the type of the object. This is a reference to an existing object type from "Objects.xml".</xs:documentation>
							<xs:documentation xml:lang="fr">Définit le type d'objet tactique. Renvoie vers "Objects.xml".</xs:documentation>
						</xs:annotation>
					</xs:attribute>
				</xs:extension>
			</xs:complexContent>
		</xs:complexType>
	</xs:element>
	<xs:simpleType name="objectType">
		<xs:annotation>
			<xs:documentation xml:lang="en">Defines the list of available object types. $$$TODO: import list from "ObjectNames.xml" or "Objects.xsd".</xs:documentation>
			<xs:documentation xml:lang="fr">Liste des types d'objets possibles.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="mines"/>
			<xs:enumeration value="mined area (linear)"/>
			<xs:enumeration value="mined area (scattered)"/>
			<xs:enumeration value="anti tank obstacle"/>
			<xs:enumeration value="abattis"/>
			<xs:enumeration value="barricade"/>
			<xs:enumeration value="landslide"/>
			<xs:enumeration value="road destruction"/>
			<xs:enumeration value="bridge destruction"/>
			<xs:enumeration value="continuous pontoon bridge"/>
			<xs:enumeration value="firing post"/>
			<xs:enumeration value="safe zone"/>
			<xs:enumeration value="gun artillery deployment area"/>
			<xs:enumeration value="cobra deployment area"/>
			<xs:enumeration value="mrls deployment area"/>
			<xs:enumeration value="crossing site"/>
			<xs:enumeration value="nbc cloud"/>
			<xs:enumeration value="logistic support area"/>
			<xs:enumeration value="decontamination site"/>
			<xs:enumeration value="jamming area"/>
			<xs:enumeration value="rota area"/>
			<xs:enumeration value="nbc zone"/>
			<xs:enumeration value="landing area"/>
			<xs:enumeration value="trail"/>
			<xs:enumeration value="platform"/>
			<xs:enumeration value="mobility enhanced area"/>
			<xs:enumeration value="landing area (helicopter)"/>
			<xs:enumeration value="logistic area"/>
			<xs:enumeration value="logistic route"/>
			<xs:enumeration value="prisoners camp"/>
			<xs:enumeration value="refugees camp"/>
			<xs:enumeration value="checkpoint"/>
			<xs:enumeration value="dropping area"/>
			<xs:enumeration value="movement forbidden area"/>
			<xs:enumeration value="fire forbidden area"/>
			<xs:enumeration value="mortar deployment area"/>
			<xs:enumeration value="discontinuous pontoon bridge"/>
			<xs:enumeration value="installation"/>
			<xs:enumeration value="fire"/>
			<xs:enumeration value="emergency shelter"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="obstacleType">
		<xs:annotation>
			<xs:documentation xml:lang="en">Defines obstacle types.</xs:documentation>
			<xs:documentation xml:lang="fr">Liste des types d'obstacles possibles.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="preliminary">
				<xs:annotation>
					<xs:documentation xml:lang="en">Defines a preliminary obstacle.</xs:documentation>
					<xs:documentation xml:lang="fr">Pour les obstacles activés dès le début de la simulation.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="reserved">
				<xs:annotation>
					<xs:documentation xml:lang="en">Defines a reserved obstacle, which is initially disabled and must be activated.</xs:documentation>
					<xs:documentation xml:lang="fr">Pour les obstacles initialement inactifs et nécessitant d'être activés.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>
	<xs:element name="population">
		<xs:annotation>
			<xs:documentation xml:lang="en">Defines a population.</xs:documentation>
			<xs:documentation xml:lang="fr">Définit une population.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:complexContent>
				<xs:extension base="entityType">
					<xs:attribute name="attitude" type="populationAttitudeType" use="required">
						<xs:annotation>
							<xs:documentation xml:lang="en">Defines the initial population attitude.</xs:documentation>
							<xs:documentation xml:lang="fr">Définit l'attitude initiale de la population.</xs:documentation>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute name="humans" type="xs:positiveInteger" use="required">
						<xs:annotation>
							<xs:documentation xml:lang="en">Defines the initial number of people composing the population.</xs:documentation>
							<xs:documentation xml:lang="fr">Définit le nombre initial d'individus formant la population.</xs:documentation>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute name="position" type="utmCoordinatesType" use="required">
						<xs:annotation>
							<xs:documentation xml:lang="en">Defines the initial position of the center of the population concentration.</xs:documentation>
							<xs:documentation xml:lang="fr">Définit la géolocalisation initiale du centre de la population.</xs:documentation>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute name="type" type="xs:string" use="required">
						<xs:annotation>
							<xs:documentation xml:lang="en">Defines the type of population. References an existing population type from "Populations.xml".</xs:documentation>
							<xs:documentation xml:lang="fr">Définit le type de la population. Renvoie vers "Populations.xml".</xs:documentation>
						</xs:annotation>
					</xs:attribute>
				</xs:extension>
			</xs:complexContent>
		</xs:complexType>
	</xs:element>
	<xs:simpleType name="utmCoordinatesType">
		<xs:annotation>
			<xs:documentation xml:lang="en">Defines Universal Transverse Mercator geographic coordinates.</xs:documentation>
			<xs:documentation xml:lang="fr"> Format des coordonnées UTM (Universal Transverse Mercator).</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:pattern value="\d{2}[A-Z]{3}\d{8}(\d{2})?"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="populationAttitudeType">
		<xs:annotation>
			<xs:documentation xml:lang="en">Defines the different population attitudes.</xs:documentation>
			<xs:documentation xml:lang="fr">Définit les attitudes possibles d'une population.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="calme"/>
			<xs:enumeration value="agitee"/>
			<xs:enumeration value="excitee"/>
			<xs:enumeration value="agressive"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:complexType name="diplomacyRelationshipType">
		<xs:annotation>
			<xs:documentation xml:lang="en">Defines the diplomatical relationships between one side and the others.</xs:documentation>
			<xs:documentation xml:lang="fr">Définit les relations diplomatiques entre un camp et les autres.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="relationship" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation xml:lang="en">Defines a diplomatical relationship between two sides.</xs:documentation>
					<xs:documentation xml:lang="fr">Définit la relation diplomatique entre deux camps.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:attribute name="side" type="xs:nonNegativeInteger" use="required">
						<xs:annotation>
							<xs:documentation xml:lang="en">Defines the side with whom to define relationship.</xs:documentation>
							<xs:documentation xml:lang="fr">Désigne le camp avec qui la relation diplomatique est définie.</xs:documentation>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute name="diplomacy" type="affinityType" use="required">
						<xs:annotation>
							<xs:documentation xml:lang="en">Defines the kind of affinity between the two sides considered.</xs:documentation>
							<xs:documentation xml:lang="fr">Définit le type d'affinité entre les deux camps considérés.</xs:documentation>
						</xs:annotation>
					</xs:attribute>
				</xs:complexType>
				<xs:unique name="diplomacyRelationshipDefinitionUnicity">
					<xs:annotation>
						<xs:documentation xml:lang="en">Checks that relationship are only defined once.</xs:documentation>
						<xs:documentation xml:lang="fr">Vérifie que les relations diplomatiques sont définies une seule foi.</xs:documentation>
					</xs:annotation>
					<xs:selector xpath="."/>
					<xs:field xpath="@side"/>
				</xs:unique>
			</xs:element>
		</xs:sequence>
		<xs:attribute name="id" type="xs:nonNegativeInteger" use="required">
			<xs:annotation>
				<xs:documentation xml:lang="en">Defines the identifier of the side whose relationships are to be defined.</xs:documentation>
				<xs:documentation xml:lang="fr"> Définit l'identifiant du camp avec qui la relation diplomatique est définie.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<xs:simpleType name="affinityType">
		<xs:annotation>
			<xs:documentation xml:lang="en">Defines the list of available affinity types.</xs:documentation>
			<xs:documentation xml:lang="fr">Définit la liste des types d'affinitépossibles.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="friend"/>
			<xs:enumeration value="enemy"/>
			<xs:enumeration value="neutral"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:complexType name="shapeType">
		<xs:annotation>
			<xs:documentation xml:lang="en">Defines a geometrical shape.</xs:documentation>
			<xs:documentation xml:lang="fr">Définit une forme géométrique.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="points">
				<xs:annotation>
					<xs:documentation xml:lang="en">Defines a list of points.</xs:documentation>
					<xs:documentation xml:lang="fr">Définit une liste de points.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="point" type="utmCoordinatesType" maxOccurs="unbounded">
							<xs:annotation>
								<xs:documentation xml:lang="en">Defines a point using UTM coordinates.</xs:documentation>
								<xs:documentation xml:lang="fr">Géolocalisation d'un point en coordonnées UTM.</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
		<xs:attribute name="type"/>
	</xs:complexType>
	<xs:complexType name="resourcesType">
		<xs:annotation>
			<xs:documentation xml:lang="en">Defines a list of resources with associated quantity.</xs:documentation>
			<xs:documentation xml:lang="fr">Définit la liste des ressources avec les quantités associées.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="dotation" minOccurs="0" maxOccurs="unbounded">
				<xs:complexType>
					<xs:annotation>
						<xs:documentation xml:lang="en">Defines a resource reference and associated quantity.</xs:documentation>
						<xs:documentation xml:lang="fr">Définit une ressource et la quantité associée.</xs:documentation>
					</xs:annotation>
					<xs:attribute name="name" type="xs:string" use="required">
						<xs:annotation>
							<xs:documentation xml:lang="en">Defines the name of the resources. References existing resource from "Resources.xml".</xs:documentation>
							<xs:documentation xml:lang="fr">Définit le nom de la ressource. Renvoie vers "Resources.xml".</xs:documentation>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute name="quantity" type="xs:positiveInteger" use="required">
						<xs:annotation>
							<xs:documentation xml:lang="en">Defines the quantity of the considered resource.</xs:documentation>
							<xs:documentation xml:lang="fr">Définit la quantitéde la ressource associée.</xs:documentation>
						</xs:annotation>
					</xs:attribute>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:element name="formation">
		<xs:annotation>
			<xs:documentation xml:lang="en">Defines a formation, which is an organisational entity containing formations and automats.</xs:documentation>
			<xs:documentation xml:lang="fr">Définit une formation, c'est-à-dire un ensemble d'automates et d'autres formations permettant ainsi de décrire l'organisation hiérarchique des unités.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:complexContent>
				<xs:extension base="entityType">
					<xs:sequence>
						<xs:element ref="automat" minOccurs="0" maxOccurs="unbounded"/>
						<xs:element ref="formation" minOccurs="0" maxOccurs="unbounded"/>
						<xs:element name="lima" type="tacticalLineType" minOccurs="0" maxOccurs="unbounded"/>
						<xs:element name="limit" type="tacticalLineType" minOccurs="0" maxOccurs="unbounded"/>
						<xs:element ref="intelligence" minOccurs="0" maxOccurs="unbounded"/>
					</xs:sequence>
					<xs:attribute name="level" type="hierarchicalLevelType" use="required">
						<xs:annotation>
							<xs:documentation xml:lang="en">Defines the hierarchical level of the formation.</xs:documentation>
							<xs:documentation xml:lang="fr">Définit le niveau hiérarchique de la formation.</xs:documentation>
						</xs:annotation>
					</xs:attribute>
				</xs:extension>
			</xs:complexContent>
		</xs:complexType>
	</xs:element>
	<xs:simpleType name="hierarchicalLevelType">
		<xs:annotation>
			<xs:documentation xml:lang="en">Defines the list of military hierarchical level.</xs:documentation>
			<xs:documentation xml:lang="fr">Définit la liste des niveaux hiérarchiques d'une armée.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="o"/>
			<xs:enumeration value="oo"/>
			<xs:enumeration value="ooo"/>
			<xs:enumeration value="i"/>
			<xs:enumeration value="ii"/>
			<xs:enumeration value="iii"/>
			<xs:enumeration value="x"/>
			<xs:enumeration value="xx"/>
			<xs:enumeration value="xxx"/>
			<xs:enumeration value="xxxx"/>
			<xs:enumeration value="xxxxx"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:element name="automat">
		<xs:annotation>
			<xs:documentation xml:lang="en">Defines an automat.</xs:documentation>
			<xs:documentation xml:lang="fr">Définit un automate.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:complexContent>
				<xs:extension base="entityType">
					<xs:sequence>
						<xs:element ref="unit" minOccurs="0" maxOccurs="unbounded"/>
						<xs:element ref="automat" minOccurs="0" maxOccurs="unbounded"/>
						<xs:element name="lima" type="tacticalLineType" minOccurs="0" maxOccurs="unbounded"/>
						<xs:element name="limit" type="tacticalLineType" minOccurs="0" maxOccurs="unbounded"/>
						<xs:element ref="intelligence" minOccurs="0" maxOccurs="unbounded"/>
					</xs:sequence>
					<xs:attribute name="engaged" type="xs:boolean" use="required">
						<xs:annotation>
							<xs:documentation xml:lang="en">Defines whether automat is initially engaged or not.</xs:documentation>
							<xs:documentation xml:lang="fr">Définit si un automate est initialement embrayé ou non.</xs:documentation>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute name="knowledge-group" type="xs:nonNegativeInteger" use="required">
						<xs:annotation>
							<xs:documentation xml:lang="en">Defines reference to existing knowledge group which automat belongs to.</xs:documentation>
							<xs:documentation xml:lang="fr">Désigne le groupe de connaissance auquel l'automate appartient.</xs:documentation>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute name="type" type="xs:string" use="required">
						<xs:annotation>
							<xs:documentation xml:lang="en">Defines automat type. References type defined in "Automat.xml".</xs:documentation>
							<xs:documentation xml:lang="fr">Définit le type d'automate. Renvoie vers "Automat.xml".</xs:documentation>
						</xs:annotation>
					</xs:attribute>
				</xs:extension>
			</xs:complexContent>
		</xs:complexType>
		<!--xs:unique name="commandPostUnicity">
			<xs:annotation>
				<xs:documentation xml:lang="en">Checks that there is exactly one command post unit.</xs:documentation>
				<xs:documentation xml:lang="fr">Vérifie qu'il existe une et une seule unité ayant le poste de commande.</xs:documentation>
			</xs:annotation>
			<xs:selector xpath="unit"></xs:selector>
			<xs:field xpath="@command-post"></xs:field>
		</xs:unique-->
	</xs:element>
	<xs:element name="unit">
		<xs:annotation>
			<xs:documentation xml:lang="en">Defines a unit.</xs:documentation>
			<xs:documentation xml:lang="fr">Définit une unité.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:complexContent>
				<xs:extension base="entityType">
					<xs:all>
						<xs:element name="equipments" minOccurs="0">
							<xs:annotation>
								<xs:appinfo>Not implemented in preparation and simulation.</xs:appinfo>
								<xs:documentation xml:lang="en">Defines overloaded equipment status.</xs:documentation>
								<xs:documentation xml:lang="fr">Redéfinit l'état initial de l'équipement.</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:sequence>
									<xs:element name="equipment" minOccurs="0">
										<xs:annotation>
											<xs:documentation xml:lang="en">Defines overloaded status for specified equipment.</xs:documentation>
											<xs:documentation xml:lang="fr">Redéfinit l'état initial de l'équipement spécifié.</xs:documentation>
										</xs:annotation>
										<xs:complexType>
											<xs:attribute name="available" type="xs:nonNegativeInteger">
												<xs:annotation>
													<xs:documentation xml:lang="en">Defines the new number of available equipments.</xs:documentation>
													<xs:documentation xml:lang="fr">Redéfinit la quantité totale de l'équipement.</xs:documentation>
												</xs:annotation>
											</xs:attribute>
											<xs:attribute name="fixable" type="xs:nonNegativeInteger">
												<xs:annotation>
													<xs:documentation xml:lang="en">Defines the new number of fixable equipments.</xs:documentation>
													<xs:documentation xml:lang="fr">Redéfinit la quantité initiale d'équipement à réparer.</xs:documentation>
												</xs:annotation>
											</xs:attribute>
											<xs:attribute name="unavailable" type="xs:nonNegativeInteger">
												<xs:annotation>
													<xs:documentation xml:lang="en">Defines the new number of unavailable equipments.</xs:documentation>
													<xs:documentation xml:lang="fr"> Redéfinit la quantité initiale d'équipement irrécupérable.</xs:documentation>
												</xs:annotation>
											</xs:attribute>
											<xs:attribute name="type" type="xs:string" use="required">
												<xs:annotation>
													<xs:documentation xml:lang="en">Defines the type of equipment to define overloaded status for.</xs:documentation>
													<xs:documentation xml:lang="fr">Définit le type d'équipement dont on va redéfinir l'état initial.</xs:documentation>
												</xs:annotation>
											</xs:attribute>
										</xs:complexType>
									</xs:element>
								</xs:sequence>
							</xs:complexType>
						</xs:element>
						<xs:element name="human-factors" minOccurs="0">
							<xs:annotation>
								<xs:appinfo>Not implemented in preparation and simulation.</xs:appinfo>
								<xs:documentation xml:lang="en">Defines overloaded human factors.</xs:documentation>
								<xs:documentation xml:lang="fr">Redéfinit le facteur humain initial.</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:all>
									<xs:element name="tiredness" type="tirednessType">
										<xs:annotation>
											<xs:documentation xml:lang="en">Defines overloaded tiredness human factor.</xs:documentation>
											<xs:documentation xml:lang="fr">Redéfinit le facteur humain de la fatigue.</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="morale" type="moraleType">
										<xs:annotation>
											<xs:documentation xml:lang="en">Defines overloaded morale human factor.</xs:documentation>
											<xs:documentation xml:lang="fr"> Redéfinit le facteur humain du moral.</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="experience" type="experienceType">
										<xs:annotation>
											<xs:documentation xml:lang="en">Defines overloaded experience human factor.</xs:documentation>
											<xs:documentation xml:lang="fr">Redéfinit le facteur humain de l'expérience.</xs:documentation>
										</xs:annotation>
									</xs:element>
								</xs:all>
							</xs:complexType>
						</xs:element>
						<xs:element name="humans" minOccurs="0">
							<xs:annotation>
								<xs:appinfo>Not implemented in preparation and simulation.</xs:appinfo>
								<xs:documentation xml:lang="en">Defines overloaded human components.</xs:documentation>
								<xs:documentation xml:lang="fr">Redéfinit l'effectif humain.</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:sequence>
									<xs:element name="human">
										<xs:complexType>
											<xs:attribute name="state" type="humanStateType" use="required">
												<xs:annotation>
													<xs:documentation xml:lang="en">Defines overloaded human state for specified count of human components.</xs:documentation>
													<xs:documentation xml:lang="fr">Redéfinit l'état de l'effectif humain.</xs:documentation>
												</xs:annotation>
											</xs:attribute>
											<xs:attribute name="officer" type="xs:nonNegativeInteger" use="required">
												<xs:annotation>
													<xs:documentation xml:lang="en">Defines "state" as the overloaded state for the specified amount of officers.</xs:documentation>
													<xs:documentation xml:lang="fr">Redéfinit l'état du nombre d'officiers donné.</xs:documentation>
												</xs:annotation>
											</xs:attribute>
											<xs:attribute name="noncommissioned-officer" type="xs:nonNegativeInteger" use="required">
												<xs:annotation>
													<xs:documentation xml:lang="en">Defines "state" as the overloaded state for the specified amount of non-commissioned officers.</xs:documentation>
													<xs:documentation xml:lang="fr"> Redéfinit l'état du nombre de sous-officiers donné.</xs:documentation>
												</xs:annotation>
											</xs:attribute>
											<xs:attribute name="private" type="xs:nonNegativeInteger" use="required">
												<xs:annotation>
													<xs:documentation xml:lang="en">Defines "state" as the overloaded state for the specified amount of privates.</xs:documentation>
													<xs:documentation xml:lang="fr">Redéfinit l'état du nombre de soldat donné.</xs:documentation>
												</xs:annotation>
											</xs:attribute>
										</xs:complexType>
									</xs:element>
								</xs:sequence>
							</xs:complexType>
						</xs:element>
						<xs:element name="stocks" type="resourcesType" minOccurs="0">
							<xs:annotation>
								<xs:documentation xml:lang="en">Defines additional resource stocks.</xs:documentation>
								<xs:documentation xml:lang="fr">Définit un stock additionnel de ressources.</xs:documentation>
							</xs:annotation>
							<xs:unique name="stocksResourceUnicity">
								<xs:annotation>
									<xs:documentation xml:lang="en">Checks that resources only appears once in list.</xs:documentation>
									<xs:documentation xml:lang="fr">Vérifie que la liste de ressources ne comporte pas de doublons.</xs:documentation>
								</xs:annotation>
								<xs:selector xpath="dotation"/>
								<xs:field xpath="@name"/>
							</xs:unique>
						</xs:element>
						<xs:element name="dotations" type="resourcesType" minOccurs="0">
							<xs:annotation>
								<xs:documentation xml:lang="en">Defines overloaded additional initial resources.</xs:documentation>
								<xs:documentation xml:lang="fr">Redéfinit les ressources additionnelles initiales.</xs:documentation>
							</xs:annotation>
							<xs:unique name="dotationsResourceUnicity">
								<xs:annotation>
									<xs:documentation xml:lang="en">Checks that resources only appears once in list.</xs:documentation>
									<xs:documentation xml:lang="fr"> Vérifie que la liste de ressources ne comporte pas de doublons.</xs:documentation>
								</xs:annotation>
								<xs:selector xpath="dotation"/>
								<xs:field xpath="@name"/>
							</xs:unique>
						</xs:element>
					</xs:all>
					<xs:attribute name="type" type="xs:string" use="required">
						<xs:annotation>
							<xs:documentation xml:lang="en">Defines unit type. Reference type from "Units.xml".</xs:documentation>
							<xs:documentation xml:lang="fr">Définit le type d'unité. Renvoie vers "Units.xml".</xs:documentation>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute name="position" type="utmCoordinatesType" use="required">
						<xs:annotation>
							<xs:documentation xml:lang="en">Defines unit position as UTM coordinates.</xs:documentation>
							<xs:documentation xml:lang="fr"> Géolocalisation de l'unité en coordonnées UTM.</xs:documentation>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute name="command-post" type="xs:boolean" default="false">
						<xs:annotation>
							<xs:documentation xml:lang="en">Defines whether unit stands as a command-post.</xs:documentation>
							<xs:documentation xml:lang="fr">Vrai si l'unité est un poste de commande.</xs:documentation>
						</xs:annotation>
					</xs:attribute>
				</xs:extension>
			</xs:complexContent>
		</xs:complexType>
		<xs:key name="unitKey">
			<xs:annotation>
				<xs:documentation xml:lang="en">Defines key for unit.</xs:documentation>
				<xs:documentation xml:lang="fr">Définit un identifiant pour l'unité.</xs:documentation>
			</xs:annotation>
			<xs:selector xpath="."/>
			<xs:field xpath="@id"/>
		</xs:key>
	</xs:element>
	<xs:simpleType name="tirednessType">
		<xs:annotation>
			<xs:documentation xml:lang="en">Defines list of tiredness 		</xs:documentation>
			<xs:documentation xml:lang="fr">Définit la liste des degrés de fatigue possible.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="normal"/>
			<xs:enumeration value="fatigue"/>
			<xs:enumeration value="epuise"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="moraleType">
		<xs:annotation>
			<xs:documentation xml:lang="en">Defines list of morale states.</xs:documentation>
			<xs:documentation xml:lang="fr">Définit la liste des niveaux caractérisant le moral des troupes.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="fanatique"/>
			<xs:enumeration value="bon"/>
			<xs:enumeration value="moyen"/>
			<xs:enumeration value="mauvais"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="experienceType">
		<xs:annotation>
			<xs:documentation xml:lang="en">Defines list of experience states.</xs:documentation>
			<xs:documentation xml:lang="fr">Définit la liste des degrés d'expérience possibles.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="veteran"/>
			<xs:enumeration value="experimente"/>
			<xs:enumeration value="conscrit"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="humanStateType">
		<xs:annotation>
			<xs:documentation xml:lang="en">Defines list of human states.</xs:documentation>
			<xs:documentation xml:lang="fr">Définit la liste des niveaux caractérisant la santé d'un être humain.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="non blesse"/>
			<xs:enumeration value="mort"/>
			<xs:enumeration value="blesse urgence 1"/>
			<xs:enumeration value="blesse urgence 2"/>
			<xs:enumeration value="blesse urgence 3"/>
			<xs:enumeration value="blesse urgence extreme"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:complexType name="tacticalLineType">
		<xs:annotation>
			<xs:documentation xml:lang="en">Defines a tactical line.</xs:documentation>
			<xs:documentation xml:lang="fr">Définit une ligne tactique.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="point" type="utmCoordinatesType" minOccurs="2" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation xml:lang="en">Defines the list of points constituting the line geometry.</xs:documentation>
					<xs:documentation xml:lang="fr">Définit la liste des points caractérisant une ligne brisée.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
		<xs:attribute name="name" type="xs:string" use="required">
			<xs:annotation>
				<xs:documentation xml:lang="en">Defines the name identifying the tactical line. Does not have to be unique.</xs:documentation>
				<xs:documentation xml:lang="fr">Définit le nom de la ligne tactique. Il n'y a pas de contrainte d'unicité.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<xs:simpleType name="app6SymbolType">
		<xs:annotation>
			<xs:documentation xml:lang="en">Defines a symbol string conforming to MIL2525B/APP6 symbology.</xs:documentation>
			<xs:documentation xml:lang="fr">Format des symboles MIL2525B/APP6.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:maxLength value="15" fixed="true"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:element name="intelligence">
		<xs:annotation>
			<xs:documentation xml:lang="en">Defines an intelligence.</xs:documentation>
			<xs:documentation xml:lang="fr">Définit une intelligence.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:complexContent>
				<xs:extension base="entityType">
					<xs:attribute name="embarked" type="xs:boolean" use="required">
						<xs:annotation>
							<xs:documentation xml:lang="en">Defines whether intelligence is performing embarked fight.</xs:documentation>
							<xs:documentation xml:lang="fr">Vrai si une intelligence est en mesure d'effectuer un combat embarqué.</xs:documentation>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute name="karma" type="karmaType" use="required">
						<xs:annotation>
							<xs:documentation xml:lang="en">Defines the karma of the intelligence.</xs:documentation>
							<xs:documentation xml:lang="fr">Définit le karma de l'intelligence.</xs:documentation>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute name="level" type="hierarchicalLevelType" use="required">
						<xs:annotation>
							<xs:documentation xml:lang="en">Defines the hierarchical level of the intelligence.</xs:documentation>
							<xs:documentation xml:lang="fr">Définit le niveau hiérarchique de l'intelligence.</xs:documentation>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute name="nature" type="app6SymbolType" use="required">
						<xs:annotation>
							<xs:documentation xml:lang="en">Defines the nature of the intelligence following APP6 symbology.</xs:documentation>
							<xs:documentation xml:lang="fr">Défnit la nature de l'intelligence suivant la symbologie APP6.</xs:documentation>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute name="position" type="utmCoordinatesType" use="required">
						<xs:annotation>
							<xs:documentation xml:lang="en">Defines the position of the intelligence as UTM coordinates.</xs:documentation>
							<xs:documentation xml:lang="fr">Définit la géolocalisation de l'intelligence en coordonnées UTM.</xs:documentation>
						</xs:annotation>
					</xs:attribute>
				</xs:extension>
			</xs:complexContent>
		</xs:complexType>
	</xs:element>
</xs:schema>
