<?xml version="1.0" encoding="UTF-8"?>
<objects xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
	<!-- liste d'objets -->
	<real-objects>
		<!-- type => name ? -->
		<object type="pont flottant discontinu" default-consumption-mode="EnTravaux" dangerous="false" 
					can-be-maneuver-obstacle="false" can-be-developed="false" can-be-bypassed="false" avoid-distance="500" 
					default-speed="1" default-bypassed-speed="1" max-interaction-height="0" max-animating-units="0" unit-speed-impact-mode="AuPlusLent" max-unit-percentage-speed="10" population-density="1.2">
			<!-- Liste de scores pas obligatoire-->
			<sensible-positions>
				<sensible-position terrain="departementale" value="3"/>
				<sensible-position terrain="carrefour" value="10"/>
			</sensible-positions>
			<!-- Liste de dotations -->
			<dotations>
				<!-- a revoir! -->
				<construction dotation="explosif classique" category="explosif" count="300"/>
				<valorization dotation="mine classique" category="mine" count="7"/>
			</dotations>
			<!-- Non obligatoire -->
			<unit-attritions>
				<unit-attrition protection="CharLourd" destruction="0.3" repairable-with-evacuation="0.2" repairable-without-evacuation="0.15"/>
				<unit-attrition protection="CharMoyen" destruction="0.3" repairable-with-evacuation="0.2" repairable-without-evacuation="0.15"/>
			</unit-attritions>
			<!-- Non obligatoire -->
			<population-attrition surface="3" ph="0.2"/>
		</object>
	</real-objects>
	
	<virtual-objects>
		<object type="controle de zone">
			<shot-percentage-per-human-per-hectare volume="Moyen" percentage="100"/>
			<shot-percentage-per-human-per-hectare volume="Petit" percentage="10"/>
		</object>
	</virtual-objects>
</objects>
