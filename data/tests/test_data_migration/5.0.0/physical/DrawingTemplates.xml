                <g font-family="Arial" font-size="0.25" fill="currentColor">
                    <g transform="matrix( 1 0 0 -1 0 0 )">
                        <text x="-0.1" y="0.1">LZ</text>
                    </g>
                </g>
            </marker>
        </template>
        <template code="G*MPORP---****X" name="Roadblock I (planned)" type="point">
