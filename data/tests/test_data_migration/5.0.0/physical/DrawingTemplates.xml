                    <line x2="1"/>
                    <g font-family="Arial" font-size="0.08" fill="currentColor">
                        <g transform="matrix( 1 0 0 -1 0 0 )">
                            <text x="0.4" y="-0.02">SR</text>
                        </g>
                    </g>
                </g>
            </segment>
        </template>
        <template code="G*MPNL----****X" name="NBC cloud" type="polygon">
