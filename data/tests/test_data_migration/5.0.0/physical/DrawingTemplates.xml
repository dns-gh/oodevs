        <template code="G*MPOADU--****X" name="Anti-tank ditch (under construction)" type="polygon">
            <segment unit="%">
                <g fill="none" stroke="currentColor" stroke-width="3px">
                    <path d="M0,0 L0.16,0.2 L0.33,0 L0.5,0.2 L0.66,0 L0.83,0.2 L1,0 z"/>
                </g>
            </segment>
        </template>
        <template code="G*MPOADC--****X" name="Anti-tank ditch (complete)" type="polygon">
