                    <path d="M0,0 L0.16,0.2 L0.33,0 L0.5,0.2 L0.66,0 L0.83,0.2 L1,0 z"/>
                </g>
            </segment>
        </template>
        <template code="G*MPOAR---****X" name="Anti-tank ditch (reinforced with mines)" type="polygon">
            <segment unit="%">
                <g fill="currentColor">
                    <path d="M0,0 L0.16,0.2 L0.33,0 L0.5,0.2 L0.66,0 L0.83,0.2 L1,0 z"/>
                    <circle cx="0" cy="0.15" r="0.05"/>
                    <circle cx="0.33" cy="0.15" r="0.05"/>
                    <circle cx="0.66" cy="0.15" r="0.05"/>
                </g>
