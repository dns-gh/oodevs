            </marker-start>
        </template>
        <template code="G*MPNDP---****X" name="Decontamination Point" type="polygon">
            <segment>
                <line stroke='currentColor' stroke-width='4px' x2='1'/>
            </segment>
            <marker-start>
                <g stroke='currentColor' stroke-width='3px'>
                    <polygon fill="#ffffff" fill-opacity="0.5" points="0,0 -250,300 -250,1000 250,1000 250,300"/>
                    <line x1='-250' y1='300' x2='250' y2='300'/>
                    <g font-family="Arial" font-size="220" fill="currentColor">
                        <g transform="matrix( 1 0 0 -1 0 0 )">
                            <text x="-200" y="-800">DCN</text>
                        </g>
                    </g>
                </g>
            </marker-start>
        </template>
        <template code="G*ROTA*********" name="ROTA Point" type="point">
            <segment/>
            <marker-start>
                <g stroke='currentColor' stroke-width='3px'>
                    <polygon fill="#ffffff" fill-opacity="0.5" points="0,0 -250,300 -250,1000 250,1000 250,300"/>
                    <line x1='-250' y1='300' x2='250' y2='300'/>
                    <g font-family="Arial" font-size="180" fill="currentColor">
                        <g transform="matrix( 1 0 0 -1 0 0 )">
                            <text x="-230" y="-800">ROTA</text>
                        </g>
                    </g>
                </g>
            </marker-start>
        </template>
        <template code="G*MPBCL---****X" name="Crossing Site" type="line">
            <marker-start unit='%'>
                <g stroke='currentColor' stroke-width='4px'>
                    <line y1='-0.15' y2='0' x1='-0.15' x2='0'/>
                    <line y1='0.15' y2='0' x1='-0.15' x2='0'/>
                </g>
            </marker-start>
            <segment>
                <line stroke='currentColor' stroke-width='4px' x2='1'/>
            </segment>
            <marker-end unit='%'>
                <g stroke='currentColor' stroke-width='4px'>
                    <line y1='0' y2='-0.15' x1='0' x2='0.15'/>
                    <line y1='0' y2='0.15' x1='0' x2='0.15'/>
                </g>
            </marker-end>
        </template>
        <template code="G*GPGAD---****X" name="Drop Zone (DZ)" type="polygon">
            <segment unit="%">
