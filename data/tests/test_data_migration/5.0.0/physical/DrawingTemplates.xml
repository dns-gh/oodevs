<?xml version="1.0" encoding="ISO-8859-1" standalone="no" ?>
<templates model-version="4.8.2">
    <category name='Lines'>
        <description>Tactical lines</description>
        <template name='-- oo --' type="line">
            <description>Section level line</description>
            <segment>
                <line stroke='currentColor' x1='0.02' x2='0.98' stroke-width='3px'/>
            </segment>
            <marker-mid unit='px'>
                <g fill='currentColor' stroke='none'>
                    <circle cy='-4' r='3'/>
                    <circle cy=' 4' r='3'/>
                </g>
            </marker-mid>
        </template>
        <template name='-- ooo --' type="line">
            <description>Platoon level line</description>
            <segment>
                <line stroke='currentColor' x1='0.02' x2='0.98' stroke-width='3px'/>
            </segment>
            <marker-mid unit='px'>
                <g fill='currentColor' stroke='none'>
                    <circle cy='-6' r='3'/>
                    <circle cy=' 0' r='3'/>
                    <circle cy=' 6' r='3'/>
                </g>
            </marker-mid>
        </template>
        <template name='-- i --' type="line">
            <description>Company level line</description>
            <segment>
                <line stroke='currentColor' x1='0.02' x2='0.98' stroke-width='3px'/>
            </segment>
            <marker-mid unit='px'>
                <line stroke='currentColor' stroke-width='2px' x1='5' x2='-5'/>
            </marker-mid>
        </template>
        <template name='-- ii --' type="line">
            <description>Bataillon level line</description>
            <segment>
                <line stroke='currentColor' x1='0.02' x2='0.98' stroke-width='3px'/>
            </segment>
            <marker-mid unit='px'>
                <g stroke='currentColor' stroke-width='2px'>
                    <line  y1='-4' y2='-4' x1='5' x2='-5'/>
                    <line  y1=' 4' y2=' 4' x1='5' x2='-5'/>
                </g>
            </marker-mid>
        </template>
        <template name='-- iii --' type="line">
            <description>Regiment level line</description>
            <segment>
                <line stroke='currentColor' x1='0.02' x2='0.98' stroke-width='3px'/>
            </segment>
            <marker-mid unit='px'>
              <g stroke='currentColor' stroke-width='2px'>
                  <line  y1='-6' y2='-6' x1='5' x2='-5'/>
                  <line  y1=' 0' y2=' 0' x1='5' x2='-5'/>
                  <line  y1=' 6' y2=' 6' x1='5' x2='-5'/>
              </g>
            </marker-mid>
        </template>
        <template name='-- x --' type="line">
            <description>Brigade level line</description>
            <segment>
                <line stroke='currentColor' x1='0.02' x2='0.98' stroke-width='3px'/>
            </segment>
            <marker-mid unit='px'>
                <g stroke-width='2px'>
                    <line stroke='currentColor' x1='-5' y1=' 5' x2='5' y2='-5'/>
                    <line stroke='currentColor' x1='-5' y1='-5' x2='5' y2=' 5'/>
                </g>
            </marker-mid>
        </template>
        <template name='-- xx --' type="line">
            <description>Division level line</description>
            <segment>
                <line stroke='currentColor' x1='0.02' x2='0.98' stroke-width='3px'/>
            </segment>
            <marker-mid unit='px'>
                <g stroke-width='2px'>
                    <g transform='translate(0,-6)'>
                        <line stroke='currentColor' x1='-5' y1=' 5' x2='5' y2='-5'/>
                        <line stroke='currentColor' x1='-5' y1='-5' x2='5' y2=' 5'/>
                    </g>
                    <g transform='translate(0,6)'>
                        <line stroke='currentColor' x1='-5' y1=' 5' x2='5' y2='-5'/>
                        <line stroke='currentColor' x1='-5' y1='-5' x2='5' y2=' 5'/>
                    </g>
                </g>
            </marker-mid>
        </template>
    </category>
    <category name='Tasks' id="tasks">
        <template name='Reckon' type="line">
            <segment unit='%'>
                <path fill='none' stroke='currentColor' stroke-width='4px' d='M0,0 L0.55,0.1 L0.45,-0.1 L1,0'/>
            </segment>
            <marker-end unit='px'>
                <path stroke='none' fill='currentColor' d='M-3,8 L15,0 -3,-8 0,0 z'/>
            </marker-end>
        </template>
        <template name='Delay' type="line">
            <marker-start unit='px'>
                <path fill='none' stroke='currentColor' stroke-width='3px' d='M0,-20 A11,10 0 0 0 0,0'/>
            </marker-start>
            <segment>
                <line stroke='currentColor' stroke-width='4px' x2='1'/>
            </segment>
            <marker-end unit='px'>
                <path stroke='none' fill='currentColor' d='M-3,8 L15,0 -3,-8 0,0 z'/>
            </marker-end>
        </template>
        <template name='Follow' type="line">
            <marker-start unit='px'>
                <path fill='currentColor' stroke='none' d='M0,8 V-8 H20 L28,0 20,8 H0z'/>
            </marker-start>
            <segment>
                <line stroke='currentColor' stroke-width='4px' x2='1'/>
            </segment>
            <marker-end unit='px'>
                <path stroke='none' fill='currentColor' d='M-3,8 L15,0 -3,-8 0,0 z'/>
            </marker-end>
        </template>
        <template name='Support' type="line">
            <marker-start unit='px'>
                <path fill='currentColor' stroke='none' d='M8,0 L0,-8 H20 L28,0 20,8 H0z'/>
            </marker-start>
            <segment>
                <line stroke='currentColor' stroke-width='4px' x2='1'/>
            </segment>
            <marker-end unit='px'>
                <path stroke='none' fill='currentColor' d='M-3,8 L15,0 -3,-8 0,0 z'/>
            </marker-end>
        </template>
        <template name='Primary Attack' type="line">
            <marker-start unit='px'>
                <g fill='none' stroke='currentColor' stroke-width='3px'>
                    <path d='M-20,-30 A20,20 0 0 0 0,-10'/>
                    <path d='M-20, 30 A20,20 0 0 1 0, 10'/>
                </g>
            </marker-start>
            <segment unit='px'>
                <g stroke='currentColor' stroke-width='4px'>
                    <line x2='1' y1='-10' y2='-10'/>
                    <line x2='1' y1=' 10' y2=' 10'/>
                </g>
            </segment>
            <marker-end unit='px'>
                <g>
                    <g fill='none' stroke='currentColor' stroke-width='3px'>
                        <path d='M0,-10 L-10,-20 L30,0'/>
                        <path d='M0, 10 L-10, 20 L30,0'/>
                    </g>
                    <path fill='currentColor' d='M10,0 L-10,-20 30,0 -10,20z'/>
                </g>
            </marker-end>
        </template>
        <template name='Secondary Attack' type="line">
            <marker-start unit='px'>
                <g fill='none' stroke='currentColor' stroke-width='3px'>
                    <path d='M-20,-30 A20,20 0 0 0 0,-10'/>
                    <path d='M-20, 30 A20,20 0 0 1 0, 10'/>
                </g>
            </marker-start>
            <segment unit='px'>
                <g stroke='currentColor' stroke-width='4px'>
                    <line x2='1' y1='-10' y2='-10'/>
                    <line x2='1' y1=' 10' y2=' 10'/>
                </g>
            </segment>
            <marker-end unit='px'>
                <g fill='none' stroke='currentColor' stroke-width='3px'>
                    <path d='M0,-10 l-10,-10 L30,0'/>
                    <path d='M0, 10 l-10, 10 L30,0'/>
                </g>
            </marker-end>
        </template>
        <template name="Disaggregation area" type="polygon">
            <segment unit="%">
                <line stroke='currentColor' x1='0' x2='1' stroke-width='3px'/>
            </segment>
            <marker unit="%">
                <g font-family="Arial" font-size="0.08" fill="currentColor">
                    <g transform="matrix( 1 0 0 -1 0 0 )">
                        <text x="-0.28" y="-0.02">DISAGGREGATION</text>
                        <text x="-0.08" y="0.07">AREA</text>
                    </g>
                </g>
            </marker>
        </template>
    </category>
    <category name="Markers">
        <description>Markers</description>
        <template name="Point" type="point">
            <segment/>
            <marker-start>
                <g stroke='currentColor' stroke-width='3px'>
                    <polygon fill="#ffffff" fill-opacity="0.5" points="0,0 -250,300 -250,1000 250,1000 250,300"/>
                    <line x1='-250' y1='300' x2='250' y2='300'/>
                </g>
            </marker-start>
        </template>
        <template name="Coordinating point" type="point">
            <segment/>
            <marker-start>
                <g stroke='currentColor' stroke-width='3px'>
                    <circle fill="none" cx="0" cy="0" r="500"/>
                    <line x1='-350' y1='-350' x2='350' y2=' 350'/>
                    <line x1='-350' y1=' 350' x2='350' y2='-350'/>
                </g>
            </marker-start>
        </template>
        <template name="Flag" type="point">
          <segment/>
          <marker-start>
            <path fill="white" stroke="currentColor" fill-opacity="0.7" stroke-width="3px" d="M0,0 l 0,300 l 300,150 l -300,150 z" />
          </marker-start>
        </template>
    </category>
    <category name='Misc.'>
        <description>Arrows and misc. symbols</description>
        <template name='Simple arrow' type="line">
            <segment>
                <line stroke='currentColor' x2='1'/>
            </segment>
            <marker-end unit='px'>
                <path stroke='none' fill='currentColor' d='M-3,8 L15,0 -3,-8 0,0 z'/>
            </marker-end>
        </template>
        <template name="TestPolygon" type="polygon">
            <segment>
                <line stroke="currentColor" x2="1"/>
            </segment>
        </template>
        <template name='Bold arrow' type="line">
            <segment>
                <line stroke='currentColor' stroke-width='4px' x2='1'/>
            </segment>
            <marker-end unit='px'>
                <path stroke='none' fill='currentColor' d='M-3,8 L15,0 -3,-8 0,0 z'/>
            </marker-end>
        </template>
    </category>
    <category name="Tactical graphics" id="graphics">
        <description>Objects and obstacles</description>
        <template code="G*MPOS----****X" name="Abatis" type="polygon">
            <marker-start unit='%'>
                <g stroke='currentColor' stroke-width='4px'>
                    <line y1='0' y2='0.15' x1='-0.30' x2='-0.15'/>
                    <line y1='0.15' y2='0' x1='-0.15' x2='0'/>
                </g>
            </marker-start>
            <segment>
                <line stroke='currentColor' stroke-width='4px' x2='1'/>
            </segment>
        </template>
        <template code="G*SPAR----****X" name="Forward Arming and Refueling Point" type="polygon">
            <segment unit="%">
                <line stroke='currentColor' x1='0' x2='1' stroke-width='3px'/>
            </segment>
            <marker unit="%">
                <g font-family="Arial" font-size="0.2" fill="currentColor">
                    <g transform="matrix( 1 0 0 -1 0 0 )">
                        <text x="-0.2" y="0.1">FARP</text>
                    </g>
                </g>
            </marker>
        </template>
        <template code="G*GPGAL---****X" name="Landing Zone" type="polygon">
            <segment unit="%">
                <line stroke='currentColor' x1='0' x2='1' stroke-width='3px'/>
            </segment>
