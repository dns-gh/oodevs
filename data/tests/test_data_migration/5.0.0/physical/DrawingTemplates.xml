            </segment>
            <marker unit="%">
                <g stroke="currentColor" stroke-width="4px">
                    <line x1="-0.25" x2="0.25"/>
                    <line x1="-0.15" x2="0.15" y1="-0.1" y2="0.1"/>
                </g>
            </marker>
        </template>
        <template code="G*SPPR----****X" name="Rearm, Refuel and Resupply Point (R3P)" type="point">
