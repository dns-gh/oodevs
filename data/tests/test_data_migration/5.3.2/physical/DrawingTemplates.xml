<?xml version="1.0" encoding="UTF-8" standalone="no" ?>
<templates model-version="4.8.2">
  <category name="Tactical lines">
    <description>Tactical lines</description>
    <template name="-- oo --" type="line">
      <description>Section level line</description>
      <segment>
        <line stroke="currentColor" stroke-width="3px" x1="0.02" x2="0.98"/>
      </segment>
      <marker-mid unit="px">
        <g fill="currentColor" stroke="none">
          <circle cy="-4" r="3"/>
          <circle cy=" 4" r="3"/>
        </g>
      </marker-mid>
    </template>
    <template name="-- ooo --" type="line">
      <description>Platoon level line</description>
      <segment>
        <line stroke="currentColor" stroke-width="3px" x1="0.02" x2="0.98"/>
      </segment>
      <marker-mid unit="px">
        <g fill="currentColor" stroke="none">
          <circle cy="-6" r="3"/>
          <circle cy=" 0" r="3"/>
          <circle cy=" 6" r="3"/>
        </g>
      </marker-mid>
    </template>
    <template name="-- i --" type="line">
      <description>Company level line</description>
      <segment>
        <line stroke="currentColor" stroke-width="3px" x1="0.02" x2="0.98"/>
      </segment>
      <marker-mid unit="px">
        <line stroke="currentColor" stroke-width="2px" x1="5" x2="-5"/>
      </marker-mid>
    </template>
    <template name="-- ii --" type="line">
      <description>Bataillon level line</description>
      <segment>
        <line stroke="currentColor" stroke-width="3px" x1="0.02" x2="0.98"/>
      </segment>
      <marker-mid unit="px">
        <g stroke="currentColor" stroke-width="2px">
          <line x1="5" x2="-5" y1="-4" y2="-4"/>
          <line x1="5" x2="-5" y1=" 4" y2=" 4"/>
        </g>
      </marker-mid>
    </template>
    <template name="-- iii --" type="line">
      <description>Regiment level line</description>
      <segment>
        <line stroke="currentColor" stroke-width="3px" x1="0.02" x2="0.98"/>
      </segment>
      <marker-mid unit="px">
        <g stroke="currentColor" stroke-width="2px">
          <line x1="5" x2="-5" y1="-6" y2="-6"/>
          <line x1="5" x2="-5" y1=" 0" y2=" 0"/>
          <line x1="5" x2="-5" y1=" 6" y2=" 6"/>
        </g>
      </marker-mid>
    </template>
    <template name="-- x --" type="line">
      <description>Brigade level line</description>
      <segment>
        <line stroke="currentColor" stroke-width="3px" x1="0.02" x2="0.98"/>
      </segment>
      <marker-mid unit="px">
        <g stroke-width="2px">
          <line stroke="currentColor" x1="-5" x2="5" y1=" 5" y2="-5"/>
          <line stroke="currentColor" x1="-5" x2="5" y1="-5" y2=" 5"/>
        </g>
      </marker-mid>
    </template>
    <template name="-- xx --" type="line">
      <description>Division level line</description>
      <segment>
        <line stroke="currentColor" stroke-width="3px" x1="0.02" x2="0.98"/>
      </segment>
      <marker-mid unit="px">
        <g stroke-width="2px">
          <g transform="translate(0,-6)">
            <line stroke="currentColor" x1="-5" x2="5" y1=" 5" y2="-5"/>
            <line stroke="currentColor" x1="-5" x2="5" y1="-5" y2=" 5"/>
          </g>
          <g transform="translate(0,6)">
            <line stroke="currentColor" x1="-5" x2="5" y1=" 5" y2="-5"/>
            <line stroke="currentColor" x1="-5" x2="5" y1="-5" y2=" 5"/>
          </g>
        </g>
      </marker-mid>
    </template>
  </category>
  <category id="tasks" name="Tasks">
