behavior MIS_Automate_MRT_LOG_SeDeployer( T_Mission_Automate_LOG_SeDeployer mission, id nDummy )
{
    if( DIA_ModuleBegins() )
    { 
        DEC_Debug( "MIS_Automate_MRT_LOG_SeDeployer" );
                       
        T_ListePions selPions = DEC_Automate_PionsAvecPC();
        
//        DEC_MRT_AffecteFuseaux( selPions );

        with( y in selPions )
        {
            T_Pion pion = y;
            
            T_Mission_Pion_LOG_SeDeployer missionPion = DEC_MRT_CreerMissionPion( pion, T_Mission_Pion_LOG_SeDeployer );
            if( missionPion != 0 )
            {
                if( mission.positionDeploiement_ != 0 )
                    DEC_Copie_Point( mission.positionDeploiement_, missionPion.positionDeploiement_ );
            }
        }       

        DEC_MRT_Valide();
    }
}

 behavior MIS_Automate_CDT_LOG_SeDeployer( T_Mission_Automate_LOG_SeDeployer mission, id nDummy )
{
    persistent T_CalculLignesAvantArriere tacheTmp = DEC_Geometrie_StartCalculLignesAvantEtArriere( DEC_Automate_PionsAvecPC() );
    float rProut  = DEC_Geometrie_CalculerDistancePointLigneAvant  ( tacheTmp, mission.positionDeploiement_ );
    float rProut2 = DEC_Geometrie_CalculerDistancePointLigneArriere( tacheTmp, mission.positionDeploiement_ );
    DEC_Debug( "Distance ligne avant : " + FloatToString( rProut ) + " - Distance ligne arrière : " + FloatToString( rProut2 ) );
}


