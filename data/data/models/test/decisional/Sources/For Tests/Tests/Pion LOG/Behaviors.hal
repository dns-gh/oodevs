default Default()
{  
    when updated:
    {
        persistent T_Action actionPreterVSRAM = 0;
        persistent T_Action actionPreterRemorqueurs = 0;
                   
        selection ordres_recus = DEC_GetCategory( "ordres_recus" );
        with ( x in ordres_recus)
        {
        	Rep_OrdreConduite ordre = x;
        	switch( ordre.__type_ )
        	{
                case Rep_OrderConduite_ModifierPrioritesReparations :
                {
                    Rep_OrderConduite_ModifierPrioritesReparations tmp = ordre;
                    DEC_Maintenance_ChangerPriorites( tmp.orderConduiteModifierPrioritesReparations_ );
                    DEC_RemoveFromCategory( "ordres_recus", ordre );
        		    DEC_DeleteRepresentation( ordre );
                    break;
                }
                
                case Rep_OrderConduite_ModifierPrioritesBlesses :
                {
                    Rep_OrderConduite_ModifierPrioritesBlesses tmp = ordre;
                    DEC_Sante_ChangerPriorites( tmp.orderConduiteModifierPrioritesBlesses_ );
                    DEC_RemoveFromCategory( "ordres_recus", ordre );
        		    DEC_DeleteRepresentation( ordre );
                    break;
                }                
                
                case Rep_OrderConduite_ModifierPrioritesTactiquesReparations :
                {
                    Rep_OrderConduite_ModifierPrioritesTactiquesReparations tmp = ordre;
                    DEC_Maintenance_ChangerPrioritesTactiques( tmp.orderConduiteModifierPrioritesTactiquesReparations_ );
                    DEC_RemoveFromCategory( "ordres_recus", ordre );
        		    DEC_DeleteRepresentation( ordre );
                    break;
                }
                
                case Rep_OrderConduite_ModifierPrioritesTactiquesBlesses :
                {
                    Rep_OrderConduite_ModifierPrioritesTactiquesBlesses tmp = ordre;
                    DEC_Sante_ChangerPrioritesTactiques( tmp.orderConduiteModifierPrioritesTactiquesBlesses_ );
                    DEC_RemoveFromCategory( "ordres_recus", ordre );
        		    DEC_DeleteRepresentation( ordre );
                    break;
                }
                
                
                case Rep_OrderConduite_ModifierRegimeTravailMaintenance:
                {
                    Rep_OrderConduite_ModifierRegimeTravailMaintenance tmp = ordre;
                    DEC_Maintenance_ChangerRegimeTravail( tmp.orderConduiteModifierRegimeTravailMaintenance_ );
                    DEC_RemoveFromCategory( "ordres_recus", ordre );
        		    DEC_DeleteRepresentation( ordre );
                    break;
                }
               
                
                case Rep_OrderConduite_Pion_RenforcerEnVSRAM:
                {
                    Rep_OrderConduite_Pion_RenforcerEnVSRAM tmp = ordre;
                    //DEC_Sante_PreterVSRAM( tmp.pionARenforcer_, tmp.nbrAmbulances_ );
                    DEC_RemoveFromCategory( "ordres_recus", ordre );
        		    DEC_DeleteRepresentation( ordre );
        		    
        		    bool bDummy;
        		    actionPreterVSRAM = DEC_StartPreterVSRAM( bDummy, tmp.pionARenforcer_, tmp.nbrAmbulances_ );
                    break;
                }                
                
                case Rep_OrderConduite_Pion_ReprendreAuxOrdresVSRAM:
                {                   
                    Rep_OrderConduite_Pion_ReprendreAuxOrdresVSRAM tmp = ordre;
                    DEC_RecupererVSRAM( tmp.pionRenforce_, tmp.nbrAmbulances_ );
                    DEC_RemoveFromCategory( "ordres_recus", ordre );
        		    DEC_DeleteRepresentation( ordre );
                    break;
                }       
                
                case Rep_OrderConduite_Pion_RenforcerEnRemorqueurs:
                {
                    Rep_OrderConduite_Pion_RenforcerEnRemorqueurs tmp = ordre;
                    //DEC_Sante_PreterRemorqueurs( tmp.pionARenforcer_, tmp.nbrRemorqueurs_ );
                    DEC_RemoveFromCategory( "ordres_recus", ordre );
        		    DEC_DeleteRepresentation( ordre );
        		    
        		    bool bDummy;
        		    actionPreterRemorqueurs = DEC_StartPreterRemorqueurs( bDummy, tmp.pionARenforcer_, tmp.nbrRemorqueurs_ );
                    break;
                }                
                
                case Rep_OrderConduite_Pion_ReprendreAuxOrdresRemorqueurs:
                {                   
                    Rep_OrderConduite_Pion_ReprendreAuxOrdresRemorqueurs tmp = ordre;
                    DEC_RecupererRemorqueurs( tmp.pionRenforce_, tmp.nbrRemorqueurs_ );
                    DEC_RemoveFromCategory( "ordres_recus", ordre );
        		    DEC_DeleteRepresentation( ordre );
                    break;
                }                                        
            }
        }
    }
    on spontaneous end:
    {
        if( actionPreterVSRAM != 0 )
        {
            DEC_StopAction( actionPreterVSRAM );
            actionPreterVSRAM = 0;
        }
        
        if( actionPreterRemorqueurs != 0 )
        {
            DEC_StopAction( actionPreterRemorqueurs );
            actionPreterRemorqueurs = 0;
        }       
    }
}

