<?xml version="1.0" encoding="UTF-8"?>
<interface xmlns="http://www.masagroup.net/directia/schemas/bm" name="agent.world.Follower" source-version="1.0.0" extends="agent.world.Destination">
    <methods>
        <method name="canFollowMe">
            <description>Returns 'true' if this element is able to follow  the agent, 'false' otherwise.</description>
            <return>
                <type>
                    <simple name="boolean"/>
                </type>
            </return>
        </method>
    </methods>
</interface>