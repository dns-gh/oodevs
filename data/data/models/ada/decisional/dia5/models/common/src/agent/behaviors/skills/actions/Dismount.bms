-- ****************************************************************************
-- To goes to dismount state
-- @returns: dismountActionCompleted once action is complete
-- @returns: dismountActionRunning when action is running.
-- ****************************************************************************
rootnode 
{
    feedbacks = { "dismountActionRunning", "dismountActionCompleted" },
    select = function( self )
        if not meKnowledge:dismount() then
            Feedback( self.feedbacks.dismountActionRunning )
        else
            Feedback( self.feedbacks.dismountActionCompleted )
        end
    end,
}