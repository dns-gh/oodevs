-- ****************************************************************************
-- To disable a specified system
-- @params: self.params.system, a masalife System object
-- @returns: disableActionCompleted once action is complete
-- ****************************************************************************
rootnode
{
    feedbacks = { "disableActionCompleted" },
    select = function( self )
        if meKnowledge:disable( self.params.system ) then
            Feedback( self.feedbacks.disableActionCompleted )
        end
    end
}