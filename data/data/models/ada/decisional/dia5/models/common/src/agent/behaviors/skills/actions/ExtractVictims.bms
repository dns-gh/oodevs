-- ****************************************************************************
-- To extract victims from a target agent.
-- @params: self.params.victimsUnit, a masalife ''victims'' object
-- @returns: extractionActionCompleted once action is completed
-- ****************************************************************************
rootnode
{
    feedbacks = { "extractionActionCompleted" },
 
    select = function( self )
        if meKnowledge:extractVictims( self.params.victimsUnit ) then
            Feedback( self.feedbacks.extractionActionCompleted )
        end
    end
}