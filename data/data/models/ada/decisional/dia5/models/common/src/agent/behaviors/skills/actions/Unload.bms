-- ****************************************************************************
-- To unload an entity on a given position
-- @params: self.params.entity, a masalife ''Transportable'' object
-- @returns: unloadActionCompleted once action of unloading the entity is done
-- ****************************************************************************
rootnode
{
    feedbacks = { "unloadActionCompleted" },

    select = function( self )
        if meKnowledge:unload( self.params.transportableEntity ) then
            Feedback( self.feedbacks.unloadActionCompleted )
        end
    end
}