-- ****************************************************************************
-- To self destruct
-- @returns: selfDestructionActionDone once action is done
-- ****************************************************************************
rootnode
{
    feedbacks = { "selfDestructionActionDone" },
    select = function( self )
        if meKnowledge:selfDestruct() then
            Feedback( self.feedbacks.selfDestructionActionDone )
        end
    end
}