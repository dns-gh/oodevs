-- ****************************************************************************
-- To enable a specified resource node
-- @params: self.params.system, a masalife System object
-- @returns: enableActionCompleted once action is complete
-- ****************************************************************************
rootnode
{
    feedbacks = { "enableActionCompleted" },
    select = function( self )
        if meKnowledge:enable( self.params.system ) then
            Feedback( self.feedbacks.enableActionCompleted )
        end
    end
}