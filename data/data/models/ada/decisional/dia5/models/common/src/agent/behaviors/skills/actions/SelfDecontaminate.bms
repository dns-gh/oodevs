-- ****************************************************************************
-- To self decontaminate
-- @returns: selfDecontaminationActionDone once action is done
-- ****************************************************************************
rootnode
{
    feedbacks = { "selfDecontaminationActionDone" },
    select = function( self )
        if meKnowledge:selfDecontaminate() then
            Feedback( self.feedbacks.selfDecontaminationActionDone )
        end
    end
}