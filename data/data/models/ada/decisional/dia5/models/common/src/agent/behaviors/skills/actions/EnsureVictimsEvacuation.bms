-- ****************************************************************************
-- To make victims beeing evactuated toward an logistic unit
-- @params: self.params.victimsUnit, a masalife "victims" object
-- @params: self.params.medicalUnit, a masalife "medical" object
-- @returns: evacuationActionCompleted once action is completed
-- ****************************************************************************
rootnode
{
    feedbacks = { "evacuationActionCompleted" },
    select = function( self )
        if meKnowledge:ensureVictimsEvacuation( self.params.victimsUnit, self.params.medicalUnit ) then
            Feedback( self.feedbacks.evacuationActionCompleted )
        end
    end
}