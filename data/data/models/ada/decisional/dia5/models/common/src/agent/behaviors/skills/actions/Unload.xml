<?xml version="1.0" encoding="UTF-8"?>
<skill xmlns="http://www.masagroup.net/directia/schemas/bm" name="agent.behaviors.skills.actions.Unload" source-version="1.0.0">
    <description>For an agent to enact the action of unloading a transported entity.</description>
    <parameters>
        <parameter name="transportableEntity">
            <description>The entity to unload.</description>
            <type>
                <class name="agent.ontology.interfaces.Transportable"/>
            </type>
        </parameter>
    </parameters>
    <body-type>
        <class name="agent.ontology.interfaces.Body"/>
    </body-type>
    <feedbacks>
        <feedback name="unloadActionCompleted">
            <description>Sent upon completion of the simulation action.</description>
        </feedback>
    </feedbacks>
</skill>