-- ****************************************************************************
-- To demonstrate
-- @params: nothing
-- @returns: demonstrateActionRunning while the action is running
-- ****************************************************************************
rootnode
{
    feedbacks = { "demonstrateActionRunning" },
    select = function( self )
        if meKnowledge:demonstrate() then
            Feedback( self.feedbacks.demonstrateActionRunning )
        end
    end
}