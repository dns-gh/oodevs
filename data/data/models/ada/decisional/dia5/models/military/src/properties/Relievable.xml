<?xml version="1.0" encoding="UTF-8"?>
<interface xmlns="http://www.masagroup.net/directia/schemas/bm" name="properties.Relievable" source-version="1.0.0" extends="properties.Reachable">
  <description>Représente tout élément de connaissance relievable.</description>
  <methods>
    <method name="canRelieveIt">
      <description>Renvoie vraie si les conditions de relève sont réunies.</description>
      <return>
        <type>
          <simple name="boolean"/>
        </type>
      </return>
    </method>
    <method name="relieveIt">
      <description>Action de relève de l'élément.</description>
      <return>
        <type>
          <simple name="boolean"/>
        </type>
      </return>
    </method>
  </methods>
</interface>