<?xml version="1.0" encoding="UTF-8"?>
<query xmlns="http://www.masagroup.net/directia/schemas/bm" name="queries.getPointsAroundObject" source-version="1.0.0">
    <description>Returns a set of 'Point' knolwedge found around the given 'Object'.</description>
    <parameters>
        <parameter name="object">
            <type>
                <class name="world.Object"/>
            </type>
        </parameter>
    </parameters>
    <target>
        <type>
            <class name="world.Point"/>
        </type>
    </target>
</query>