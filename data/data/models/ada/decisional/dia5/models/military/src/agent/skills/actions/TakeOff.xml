<?xml version="1.0" encoding="UTF-8"?>
<skill xmlns="http://www.masagroup.net/directia/schemas/bm" name="agent.skills.actions.TakeOff" source-version="1.0.0">
    <description>To take off at a specifiec height.</description>
    <parameters>
        <parameter name="height">
            <description>The height to reach (in meters). Default value is 2000 meters.</description>
            <type>
                <simple name="numeric"/>
            </type>
            <default>
                <number>2000.0</number>
            </default>
        </parameter>
    </parameters>
    <feedbacks>
        <feedback name="tookOff">
            <description>Set upon action completion.</description>
        </feedback>
        <feedback name="done">
            <description>Set upon action completion.</description>
        </feedback>
    </feedbacks>
</skill>