-- **************************************************************************** 
-- Land: to change the agent altitude from x meters to 0 meters.
-- ****************************************************************************
rootnode 
{
    feebacks = { "landed" },
    -- -------------------------------------------------------------------------------- 
    -- Init
    -- --------------------------------------------------------------------------------
    create = function( self )
        myself.altitude = 0
        DEC_Agent_HauteurDeVol( myself.altitude ) -- $$$ MIA TODO Action sur le body, faire un body flyingAgent et virer la méthode DEC
        integration.deactivateSensors()-- $$$ MIA TODO A virer spé drones
        Feedback( self.feedbacks.landed )
        Feedback( self.feedbacks.done ) -- if called within a mission
    end,
}