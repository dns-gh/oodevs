-- **************************************************************************** 
-- Land: to change the agent altitude from x meters to 0 meters.
-- ****************************************************************************
rootnode 
{
    -- -------------------------------------------------------------------------------- 
    -- Init
    -- --------------------------------------------------------------------------------
    create = function( self )
        if DEC_Agent_EstEnVol() then
            myself.altitude = 0
            DEC_Agent_HauteurDeVol( myself.altitude )
        end
        integration.deactivateSensors()
        Feedback( self.feedbacks.done, additionalParams )
    end,
}