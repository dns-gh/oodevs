<?xml version="1.0" encoding="UTF-8"?>
<class xmlns="http://www.masagroup.net/directia/schemas/bm" name="world.FragOrder" source-version="1.0.0">
    <methods>
        <method name="updateROE">
            <description>Appliquer le changement de ROE</description>
        </method>
        <method name="setAutomatFragOrder">
            <description>Appliquer le changement de frago lorsqu'une unité est ajouté à un automate</description>
        </method>
        <method name="startTask">
            <description>Lancer la tâche correspondant à l'application de l'ordre de conduite</description>
        </method>
        <method name="isROE">
            <description>Retourne true si l'ordre de conduite donné est un ROE</description>
            <return>
                <type>
                    <simple name="boolean"/>
                </type>
            </return>
        </method>
        <method name="isTask">
            <description>Retourne true si l'ordre de conduite est une tâche</description>
            <return>
                <type>
                    <simple name="boolean"/>
                </type>
            </return>
        </method>
        <method name="mustBePropagate">
            <description>Retourne true si l'ordre de conduite donné doit être envoyé aux subordonnés</description>
            <return>
                <type>
                    <simple name="boolean"/>
                </type>
            </return>
        </method>
        <method name="getParameters">
            <description>Retourne la table de paramètres de l'ordre de conduite</description>
        </method>
        <method name="getAmmunitionTypeForIndirectFireOrder">
            <return>
                <type>
                    <any/>
                </type>
            </return>
        </method>
        <method name="getQuantityForIndirectFireOrder">
            <return>
                <type>
                    <simple name="numeric"/>
                </type>
            </return>
        </method>
        <method name="getTargetsForIndirectFireOrder">
            <return>
                <type>
                    <class name="world.LocalizedElement"/>
                </type>
            </return>
        </method>
    </methods>
</class>