rootnode 
{
    feebacks = { "tookOff" },

    -- -------------------------------------------------------------------------------- 
    -- Init
    -- --------------------------------------------------------------------------------
    create = function( self )
        myself.altitude = self.params.height
        integration.changeHeight( myself.altitude ) -- $$$ MIA TODO Action sur le body, faire un body flyingAgent
        integration.activateSensors() -- $$$ MIA TODO A virer spé drones
        Feedback( self.feedbacks.tookOff )
        Feedback( self.feedbacks.done ) -- if called within a mission
    end,
}