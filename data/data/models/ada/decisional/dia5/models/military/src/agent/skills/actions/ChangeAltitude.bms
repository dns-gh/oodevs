-- **************************************************************************** 
-- Change altitude: to change the agent altitude from x meters to 0 meters.
-- ****************************************************************************
rootnode 
{
    -- -------------------------------------------------------------------------------- 
    -- Init
    -- --------------------------------------------------------------------------------
    create = function( self )
        myself.altitude = self.params.height
        DEC_Agent_HauteurDeVol( myself.altitude ) -- $$$ MIA TODO Action sur le body, faire un body flyingAgent et virer la méthode DEC
        Feedback( self.feedbacks.done )
    end
}