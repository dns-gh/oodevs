<?xml version="1.0" encoding="UTF-8"?>
<class xmlns="http://www.masagroup.net/directia/schemas/bm" name="agent.tasks.knowledges.Reconnoiter" source-version="1.0.0" extends="agent.tasks.knowledges.PlatoonTask">
  <description>Interface des connaissances de mission de niveau compagnie</description>
  <methods>
    <method name="getPionEfficiency">
      <description>Retourne l'efficacité voulue par l'automate basée sur la capacité des pions</description>
      <return>
        <type>
          <simple name="numeric"/>
        </type>
      </return>
    </method>
    <method name="fillParameters">
      <argument name="companyTask">
        <description>La tâche automate</description>
        <type>
          <class name="properties.Commandable"/>
        </type>
      </argument>
      <argument name="params">
        <description>Les paramètres de la tâche automate</description>
        <type>
          <list>
            <class name="directia.core.Object"/>
          </list>
        </type>
      </argument>
      <return>
        <type>
          <list>
            <class name="directia.core.Object"/>
          </list>
        </type>
      </return>
    </method>
  </methods>
</class>