<?xml version="1.0" encoding="UTF-8"?>
<class xmlns="http://www.masagroup.net/directia/schemas/bm" name="ontology.classes.PlannedWork" source-version="1.0.0" extends="ontology.classes.LocalizedElement">
    <description>Represents the idea of a human construction to come. Contains all the specifics required for construction. </description>
    <realizes>agent.ontology.interfaces.Work</realizes>
    <methods>
        <method name="getPosition">
            <return name="simPosition">
                <type>
                    <any/>
                </type>
            </return>
        </method>
        <method name="isCreated" abstract="false">
            <description>Returns true if this element is entierly built.</description>
            <return>
                <type>
                    <simple name="boolean"/>
                </type>
            </return>
        </method>
        <method name="getPositions">
            <return>
                <type>
                    <list>
                        <any/>
                    </list>
                </type>
            </return>
        </method>
        <method name="createIt" abstract="false"/>
    </methods>
</class>