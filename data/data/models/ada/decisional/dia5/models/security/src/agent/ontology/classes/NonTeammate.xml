<?xml version="1.0" encoding="UTF-8"?>
<class xmlns="http://www.masagroup.net/directia/schemas/bm" name="agent.ontology.classes.NonTeammate" source-version="1.0.0" extends="agent.ontology.classes.Agent">
    <description>Represents an unknown agent. </description>
    <realizes>agent.world.Transportable</realizes>
    <methods>
        <method name="getPosition">
            <return name="simPosition">
                <type>
                    <any/>
                </type>
            </return>
        </method>
        <method name="getPositions">
            <return>
                <type>
                    <list>
                        <any/>
                    </list>
                </type>
            </return>
        </method>
        <method name="canBeLoaded" abstract="false">
            <description>Returns true if this entity can be loaded on the agent.</description>
            <return>
                <type>
                    <simple name="boolean"/>
                </type>
            </return>
        </method>
        <method name="canBeUnloaded" abstract="false">
            <description>Returns true if THIS entity can be unloaded from the agent.</description>
            <return>
                <type>
                    <simple name="boolean"/>
                </type>
            </return>
        </method>
        <method name="isLoaded" abstract="false">
            <description>Returns 'true' if this entity is 100% loaded in the agent.</description>
            <return>
                <type>
                    <simple name="boolean"/>
                </type>
            </return>
        </method>
        <method name="loadIt" abstract="false">
            <description>Action of loading this element on the agent.</description>
            <return>
                <description>Returns 'true' on action completion,  'false' otherwise.</description>
                <type>
                    <simple name="boolean"/>
                </type>
            </return>
        </method>
        <method name="unloadIt" abstract="false">
            <description>Action of unloading this element from the agent.</description>
            <return>
                <description>Returns 'true' on action completion,  'false' otherwise.</description>
                <type>
                    <simple name="boolean"/>
                </type>
            </return>
        </method>
    </methods>
</class>