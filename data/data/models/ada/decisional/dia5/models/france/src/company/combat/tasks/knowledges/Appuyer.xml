<?xml version="1.0" encoding="UTF-8"?>
<class xmlns="http://www.masagroup.net/directia/schemas/bm" name="company.combat.tasks.knowledges.Appuyer" source-version="1.0.0" extends="commander.tasks.knowledges.CompanyTask">
    <methods>
        <method name="getEntitiesToSupport">
            <return>
                <type>
                    <list>
                        <class name="directia.core.Object"/>
                    </list>
                </type>
            </return>
        </method>
        <method name="getNbrFront">
          <return>
            <type>
              <simple name="numeric"/>
            </type>
          </return>
        </method>
        <method name="getSupportingPosition">
            <return>
                <type>
                    <class name="properties.Neutralizing"/>
                    <class name="properties.Destroying"/>
                    <class name="properties.Observing"/>
                </type>
            </return>
        </method>
        <method name="getDirection">
            <return>
                <type>
                    <class name="properties.Observable"/>
                </type>
            </return>
        </method>
    </methods>
</class>