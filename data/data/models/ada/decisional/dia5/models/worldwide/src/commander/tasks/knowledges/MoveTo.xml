<?xml version="1.0" encoding="UTF-8"?>
<class xmlns="http://www.masagroup.net/directia/schemas/bm" name="commander.tasks.knowledges.MoveTo" source-version="1.0.0" extends="commander.tasks.knowledges.CompanyTask">
    <description>The knowledge of the &quot;reach&quot; mission effect. This knowledge is used by the commander agent to perform all missions which the main effect is to reach a set of objectives.</description>
    <methods>
        <method name="getMeetingPoint">
            <return>
                <type>
                    <list>
                        <class name="directia.core.Object"/>
                    </list>
                </type>
            </return>
        </method>
        <method name="fillParameters">
            <argument name="companyTask">
                <description>The string name of the task to be performed.</description>
                <type>
                    <class name="properties.Commandable"/>
                </type>
            </argument>
            <argument name="params">
                <description>The parameters of the company task.</description>
                <type>
                    <list>
                        <class name="directia.core.Object"/>
                    </list>
                </type>
            </argument>
            <return>
                <type>
                    <list>
                        <class name="directia.core.Object"/>
                    </list>
                </type>
            </return>
        </method>
    </methods>
</class>