<?xml version="1.0" encoding="UTF-8"?>
<class xmlns="http://www.masagroup.net/directia/schemas/bm" name="agent.tasks.knowledges.ByPass" source-version="1.0.0" extends="agent.tasks.knowledges.PlatoonTask">
    <methods>
        <method name="fillParameters">
            <argument name="companyTask">
                <description>La tache automate</description>
                <type>
                    <class name="properties.Commandable"/>
                </type>
            </argument>
            <argument name="params">
                <description>Les parametres de la tache automate</description>
                <type>
                    <list>
                        <class name="directia.core.Object"/>
                    </list>
                </type>
            </argument>
            <return>
                <type>
                    <list>
                        <class name="directia.core.Object"/>
                    </list>
                </type>
            </return>
        </method>
        <method name="getObstaclesParameter">
            <description>Returns the list of the obstacles to bypass.</description>
            <return name="obstacles">
                <type>
                    <list>
                        <class name="directia.core.Object"/>
                    </list>
                </type>
            </return>
        </method>
        <method name="canDoIt">
            <description>Returns 'true' if the agent has the given entity has the physical capabilities to bypass the given obstacle.</description>
            <argument name="entity">
                <description>The unit to bypass the obstacle.</description>
                <type>
                    <class name="directia.core.Object"/>
                </type>
            </argument>
            <argument name="obstacle">
                <description>The obstacle to be bypassed.</description>
                <type>
                    <class name="world.EngineerObject"/>
                </type>
            </argument>
            <return name="hasRessource">
                <type>
                    <simple name="boolean"/>
                </type>
            </return>
        </method>
        <method name="hasDotation">
            <description>Return 'true' if the given entity has the dotation to bypass the given obstacle, 'false' otherwise. This implementation returns 'true'.</description>
            <return name="hasDotation">
                <type>
                    <simple name="boolean"/>
                </type>
            </return>
        </method>
    </methods>
</class>