<?xml version="1.0" encoding="UTF-8"?>
<class xmlns="http://www.masagroup.net/directia/schemas/bm" name="commander.tasks.knowledges.DeployOnPositions" source-version="1.0.0" extends="commander.tasks.knowledges.ReachingEffect">
    <description>The knowledge of the 'DeployOnPositions' mission. This knowledge is used by the commander agent to perform this mission. </description>
    <methods>
        <method name="getFirePositions">
            <description>Returns the deployment positions.</description>
            <return>
                <type>
                    <list>
                        <class name="directia.core.Object"/>
                    </list>
                </type>
            </return>
        </method>
        <method name="getMeetingPoint">
            <description>Returns the &quot;meetingPosition&quot; parameter.</description>
            <return>
                <type>
                    <class name="directia.core.Object"/>
                </type>
            </return>
        </method>
        <method name="getChangeAutomatically">
            <description>Returns the &quot;moveAutomatically&quot; parameter.</description>
            <return>
                <type>
                    <simple name="boolean"/>
                </type>
            </return>
        </method>
    </methods>
</class>