<?xml version="1.0" encoding="UTF-8"?>
<class xmlns="http://www.masagroup.net/directia/schemas/bm" name="commander.tasks.knowledges.ResumeConstructionWork" source-version="1.0.0" extends="commander.tasks.knowledges.CompanyTask">
    <description>Interface des connaissances de mission de niveau compagnie</description>
    <methods>
        <method name="getObstaclesPlan">
            <return>
                <type>
                    <list>
                        <class name="directia.core.Object"/>
                    </list>
                </type>
            </return>
        </method>
        <method name="getMeetingPoint">
            <return>
                <type>
                    <class name="directia.core.Object"/>
                </type>
            </return>
        </method>
        <method name="getObstaclesParameter">
            <description>Retourne la liste d'obstacles pass√© en parametre de l'automate.</description>
            <return name="obstacles">
                <type>
                    <list>
                        <class name="directia.core.Object"/>
                    </list>
                </type>
            </return>
        </method>
        <method name="getPositions">
            <return>
                <type>
                    <class name="directia.core.Object"/>
                </type>
            </return>
        </method>
        <method name="getWithImprovement">
            <description>Returns 'true' if the mission must be performed with reinforcement management, 'false' otherwise.</description>
            <return>
                <type>
                    <simple name="boolean"/>
                </type>
            </return>
        </method>
        <method name="getWithReinforcement">
            <description>Returns 'True' if the surbordinate units have to wait for reinforcement in case of non faisable work (no physical capabilities).</description>
            <return>
                <type>
                    <simple name="boolean"/>
                </type>
            </return>
        </method>
    </methods>
</class>