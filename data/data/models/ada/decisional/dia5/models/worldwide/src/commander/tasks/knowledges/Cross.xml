<?xml version="1.0" encoding="UTF-8"?>
<class xmlns="http://www.masagroup.net/directia/schemas/bm" name="commander.tasks.knowledges.Cross" source-version="1.0.0" extends="commander.tasks.knowledges.ReachingEffect">
    <description>The knowledge of the 'Cross' mission. This knowledge is used by the commander agent to perform this mission.</description>
    <methods>
        <method name="getReachable">
            <description>Returns the final objective to reach. </description>
            <return>
                <type>
                    <list>
                        <class name="directia.core.Object"/>
                    </list>
                </type>
            </return>
        </method>
        <method name="getCrossingSite">
            <description>Returns the crossing site.</description>
            <return>
                <type>
                    <list>
                        <class name="world.Object"/>
                    </list>
                </type>
            </return>
        </method>
    </methods>
</class>