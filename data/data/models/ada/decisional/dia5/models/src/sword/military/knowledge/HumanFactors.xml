<?xml version="1.0" encoding="UTF-8"?>
<interface xmlns="http://www.masagroup.net/directia/schemas/bm" name="sword.military.knowledge.HumanFactors" source-version="1.0.0">
  <methods>
    <method name="stressLevel">
      <description>Définit le niveau de stress d'une connaissance</description>
      <return>
        <type>
          <simple name="boolean"/>
        </type>
      </return>
    </method>
    <method name="moraleLevel">
      <description>Définit le niveau de moral d'une connaissance</description>
      <return>
        <type>
          <simple name="numeric"/>
        </type>
      </return>
    </method>
    <method name="adoptCoverAttitude">
      <return>
        <type>
          <simple name="boolean"/>
        </type>
      </return>
    </method>
    <method name="adoptSafetyAttitude">
      <return>
        <type>
          <simple name="boolean"/>
        </type>
      </return>
    </method>
  </methods>
  <predicates>
    <predicate name="isStressed"/>
    <predicate name="isDepressed"/>
  </predicates>
</interface>