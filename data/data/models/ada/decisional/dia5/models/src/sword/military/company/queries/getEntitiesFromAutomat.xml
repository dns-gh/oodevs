<?xml version="1.0" encoding="UTF-8"?>
<query xmlns="http://www.masagroup.net/directia/schemas/bm" name="sword.military.company.queries.getEntitiesFromAutomat" source-version="1.0.0">
  <description>Retourne les éléments de la compagnie correspondant aux critères</description>
  <parameters>
    <parameter name="role">
      <type>
        <simple name="string"/>
      </type>
    </parameter>
    <parameter name="withPC">
      <type>
        <simple name="boolean"/>
      </type>
    </parameter>
    <parameter name="automat">
      <type>
        <class name="sword.military.world.Company"/>
      </type>
    </parameter>
  </parameters>
  <target name="k">
    <type>
      <class name="sword.military.world.PlatoonAlly"/>
    </type>
  </target>
</query>