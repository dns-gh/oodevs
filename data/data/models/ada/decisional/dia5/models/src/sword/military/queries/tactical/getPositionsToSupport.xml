<?xml version="1.0" encoding="UTF-8"?>
<query xmlns="http://www.masagroup.net/directia/schemas/bm" name="sword.military.queries.tactical.getPositionsToSupport" source-version="1.0.0">
    <description>Retourne toute position permettant de soutenir une unit√©.&#xD;
    </description>
    <parameters>
        <parameter name="elementsToSupport">
            <type>
                <list>
                    <class name="sword.military.knowledge.decisional.Supportable"/>
                </list>
            </type>
        </parameter>
        <parameter name="dynamic">
            <type>
                <simple name="boolean"/>
            </type>
        </parameter>
    </parameters>
    <target name="supportingPosition">
        <type>
            <class name="sword.military.knowledge.decisional.Supporting"/>
        </type>
    </target>
</query>