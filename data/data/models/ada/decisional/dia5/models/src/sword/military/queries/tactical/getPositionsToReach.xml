<?xml version="1.0" encoding="UTF-8"?>
<query xmlns="http://www.masagroup.net/directia/schemas/bm" name="sword.military.queries.tactical.getPositionsToReach" source-version="1.0.0">
    <description>Retourne les positions bonnes pour atteindre un objectif</description>
    <parameters>
        <parameter name="elementsToReach">
            <type>
                <list>
                    <class name="sword.military.knowledge.decisional.Reachable"/>
                </list>
            </type>
        </parameter>
    </parameters>
    <target name="reachingPosition">
        <type>
            <class name="sword.military.knowledge.decisional.Reaching"/>
        </type>
    </target>
</query>