<?xml version="1.0" encoding="UTF-8"?>
<interface xmlns="http://www.masagroup.net/directia/schemas/bm" name="security.ontologies.interfaces.Stock" source-version="1.0.0" extends="sword.military.knowledge.decisional.Reachable">
  <methods>
    <method name="supply">
      <description>Alimente le stock.</description>
      <argument name="resourceTypes">
        <type>
          <list>
            <any/>
          </list>
        </type>
      </argument>
      <return>
        <description>Vrai le chargement a pu être déposé.</description>
        <type>
          <simple name="boolean"/>
        </type>
      </return>
    </method>
    <method name="extract">
      <description>Vide le stock.</description>
      <argument name="resourceTypes">
        <type>
          <list>
            <any/>
          </list>
        </type>
      </argument>
      <return>
        <description>Vrai si le chargement a pu être retiré.</description>
        <type>
          <simple name="boolean"/>
        </type>
      </return>
    </method>
    <method name="isSupplyPossible">
      <argument name="resourceTypes">
        <type>
          <list>
            <any/>
          </list>
        </type>
      </argument>
      <return>
        <type>
          <simple name="boolean"/>
        </type>
      </return>
    </method>
    <method name="isExtractPossible">
      <argument name="resourceTypes">
        <type>
          <list>
            <any/>
          </list>
        </type>
      </argument>
      <return>
        <type>
          <simple name="boolean"/>
        </type>
      </return>
    </method>
    <method name="distribute">
      <description>Distribute stock to population.</description>
    </method>
    <method name="isDistributePossible">
      <argument name="population">
        <type>
          <class name="security.ontologies.classes.Crowd"/>
        </type>
      </argument>
      <return>
        <type>
          <simple name="boolean"/>
        </type>
      </return>
    </method>
  </methods>
</interface>