<?xml version="1.0" encoding="UTF-8"?>
<class xmlns="http://www.masagroup.net/directia/schemas/bm" name="sword.military.platoon.coercition.tasks.knowledges.ReconnoiterRoute" source-version="1.0.0" extends="sword.military.platoon.tasks.knowledges.PlatoonTask">
    <description>Interface des connaissances de mission de niveau compagnie</description>
    <methods>
        <method name="fillParameters">
            <argument name="companyTask">
                <description>La tâche automate</description>
                <type>
                    <class name="sword.military.knowledge.decisional.Commandable"/>
                </type>
            </argument>
            <argument name="params">
                <description>Les paramètres de la tâche automate</description>
                <type>
                    <list>
                        <class name="directia.core.Object"/>
                    </list>
                </type>
            </argument>
            <return>
                <type>
                    <list>
                        <class name="directia.core.Object"/>
                    </list>
                </type>
            </return>
        </method>
        <method name="getPionEfficiency" abstract="false">
            <description>Retourne l'effet principal de la mission</description>
            <return>
                <type>
                    <simple name="numeric"/>
                </type>
            </return>
        </method>
    </methods>
</class>