<?xml version="1.0" encoding="UTF-8"?>
<interface xmlns="http://www.masagroup.net/directia/schemas/bm" name="security.deprecated.ontologies.interfaces.body.Installer" source-version="1.0.0" extends="directia.core.model.RoleBody">
  <description>Représente la capacité d'action de l'agent simulé pour installer un élément.</description>
  <methods>
    <method name="install">
      <description>Action d'intaller un élément.</description>
      <argument name="element">
        <description>L'élément à installer.</description>
        <type>
          <class name="security.ontologies.interfaces.Installable"/>
        </type>
      </argument>
      <return>
        <description>Vaut 'vrai' si l'action d'installation est terminée, 'faux' sinon.</description>
        <type>
          <simple name="boolean"/>
        </type>
      </return>
    </method>
  </methods>
</interface>