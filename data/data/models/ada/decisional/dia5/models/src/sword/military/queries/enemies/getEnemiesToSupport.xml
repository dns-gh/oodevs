<?xml version="1.0" encoding="UTF-8"?>
<query xmlns="http://www.masagroup.net/directia/schemas/bm" name="sword.military.queries.enemies.getEnemiesToSupport" source-version="1.0.0">
	<description>Retourne les ennemis a engager pour soutenir les élements passés en paramètre.</description>
	<parameters>
		<parameter name="elementsToSupport">
			<description>Les éléments à soutenir.</description>
			<type>
				<list>
					<class name="sword.military.knowledge.decisional.Supportable"/>
				</list>
			</type>
		</parameter>
	</parameters>
	<target>
		<type>
			<class name="sword.military.world.Platoon"/>
		</type>
	</target>
</query>