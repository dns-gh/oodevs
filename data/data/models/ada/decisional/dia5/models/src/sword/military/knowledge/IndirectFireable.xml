<?xml version="1.0" encoding="UTF-8"?>
<interface xmlns="http://www.masagroup.net/directia/schemas/bm" name="sword.military.knowledge.IndirectFireable" source-version="1.0.0">
  <methods>
    <method name="isOnRangeFor">
      <description>return if the target is on range</description>
      <argument name="munition">
        <type>
          <simple name="numeric"/>
        </type>
      </argument>
      <return>
        <type>
          <simple name="boolean"/>
        </type>
      </return>
    </method>
    <method name="applyFireOnIt">
      <description>execute fire order</description>
      <argument name="munition">
        <type>
          <simple name="numeric"/>
        </type>
      </argument>
      <argument name="interventionType">
        <type>
          <simple name="numeric"/>
        </type>
      </argument>
      <return>
        <type>
          <simple name="boolean"/>
        </type>
      </return>
    </method>
  </methods>
</interface>