<?xml version="1.0" encoding="UTF-8"?>
<class xmlns="http://www.masagroup.net/directia/schemas/bm" name="sword.military.platoon.stabilisation.tasks.knowledge.ControlCrowd" source-version="1.0.0" extends="sword.military.platoon.tasks.knowledges.PlatoonTask">
  <methods>
    <method name="getPionEfficiency" abstract="false">
      <description>Retourne l'effet principal de la mission</description>
      <return>
        <type>
          <simple name="numeric"/>
        </type>
      </return>
    </method>
    <method name="fillParameters" abstract="false">
      <argument name="companyTask">
        <description>La tache automate</description>
        <type>
          <class name="sword.military.knowledge.decisional.Commandable"/>
        </type>
      </argument>
      <argument name="objectives">
        <description>Les objectifs de la tache automate</description>
        <type>
          <list>
            <class name="directia.core.Object"/>
          </list>
        </type>
      </argument>
      <return>
        <type>
          <list>
            <class name="directia.core.Object"/>
          </list>
        </type>
      </return>
    </method>
  </methods>
</class>