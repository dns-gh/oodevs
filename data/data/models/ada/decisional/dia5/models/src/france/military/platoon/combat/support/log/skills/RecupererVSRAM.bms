-- ****************************************************************************
-- Simple internal skill activated by LOG fragorder. Ask the agent
-- to get VSRAM to an other agent.
-- ****************************************************************************
rootnode
{
    done = function( self ) end,
    
    create  = function( self )
       meKnowledge:GetVSRAM( self.params.pionRenforce, self.params.nbrAmbulances )
    end,
}
