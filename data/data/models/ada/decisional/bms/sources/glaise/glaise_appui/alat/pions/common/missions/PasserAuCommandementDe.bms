eventmanager_plugin.MIS_Pion_PasserAuCommandementDe = { "mission" }
connections = connections or {}
connections[ #connections + 1 ] = { nodes.MIS_Pion_PasserAuCommandementDe, "BEH_Ordre_Inopine", nodes.BEH_Ordre_Inopine }
