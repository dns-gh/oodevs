includedFiles = includedFiles or {}
includedFiles["sources/glaise/glaise_appui/artdsa/automates/batterie asa cp/missions/defendre site/Defendre Site - CDT.bms"] = true

--[[// -----------------------------------------------------------------------------
]]--[[// Conduite de la mission Automate ASA Defendre Site
]]--[[// -----------------------------------------------------------------------------
]]node "MIS_Automate_CDT_ASA_DefendreSite"
{
    feedbacks = { { { "done_MIS_Automate_CDT_ASA_DefendreSite" }, "MIS_Automate_CDT_ASA_DefendreSite" } },
    SendFeedback = function( self, value ) Feedback( self.feedbacks.done_MIS_Automate_CDT_ASA_DefendreSite, {value} ) end,
    activations =
    {
        { "ACT_Info_Trace", { "done_ACT_Info_Trace" } },
    },

    create = function( self )
        self._namedParams = {}
        self._namedParams.mission = self.params.mission
    end,

    activate = function( self )
        Activate( self.activations.ACT_Info_Trace, 1, { "Mission Sgpt ASA Roland Carol : Defendre Site", } )
    end,

}

eventmanager_plugin = eventmanager_plugin or {}
eventmanager_plugin.MIS_Automate_CDT_ASA_DefendreSite = { "mission" }
connections = connections or {}
connections[ #connections + 1 ] = { nodes.MIS_Automate_CDT_ASA_DefendreSite, "ACT_Info_Trace", nodes.ACT_Info_Trace }
