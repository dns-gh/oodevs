                    Activate( self.activations.ACT_Mission_Automate_ControleCreation_OK, -rForce, emptyTable )
                                        do return end

                                end
            end

        Activate( self.activations.ACT_Mission_Automate_NBC_AssignerReconnaitreA, rForce, { self._namedParams.ePhase, self._namedParams.pPion, } )
self.done_ACT_Mission_Automate_NBC_AssignerReconnaitreA = function( self, _, v ) self.eActionCreation = v[1] end
    end,

