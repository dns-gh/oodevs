node "BEH_Lima_Pion_ABC_Jalonner_GererLimas"
{
    feedbacks = { { { "done_BEH_Lima_Pion_ABC_Jalonner_GererLimas" }, "BEH_Lima_Pion_ABC_Jalonner_GererLimas" } },
    SendFeedback = function( self, value ) Feedback( self.feedbacks.done_BEH_Lima_Pion_ABC_Jalonner_GererLimas, {value} ) end,
    activations =
    {
        { "ACT_Dep_PasserSurLima", { "done_ACT_Dep_PasserSurLima" } },
    },

    create = function( self )
    end,

    activate = function( self )
        local Activate = Activate
        --[[//-----------------------------------------------------------------------------
]]--[[// Traiter les points interressants
]]--[[//-----------------------------------------------------------------------------
]]local listePointInterressant = DEC_GetCategory( "points_interressants" )
        for _,x in pairs( listePointInterressant or emptyTable ) do
        local _continue = true
do
                local pointABC = x
                if( pointABC:GetType() == "Rep_PointLima" ) then
                    do
                        --[[// cast
]]local pointLima = pointABC
                         do
                            local _continue = true
                            local switch_1 = pointLima:GetnTypeLima_()
                            local cases_switch_1 = {}
                                if switch_1 == eTypeLima_LC then
                                                                    --[[//-------------------------------------------------------------
]]--[[// Passage sur la ligne de coordination fixée par le capitaine
]]--[[//-------------------------------------------------------------
]]if( not DEC_GetMissionLimaFlag( DEC_GetLimaPoint( pointLima ) ) ) then
                                        do
                                            DEC_SetMissionLimaFlag( DEC_GetLimaPoint( pointLima ), true )
                                            --[[// DEC_RC( eRC_SurLima, eTypeLima_LC );
]]Activate( self.activations.ACT_Dep_PasserSurLima, 1, { eEtatLima_LC, } )
                                            DEC_Trace( "Passage LC" )
                                        end
                                    end

                                    DEC_RemoveFromCategory( "points_interressants", pointLima )
                                    _continue = false
                                --[[//-------------------------------------------------------------
]]--[[// Passage sur LIA --> decrocher
]]--[[//-------------------------------------------------------------
]]                                cases_switch_1[1] = true
elseif switch_1 == eTypeLima_LR
                                or switch_1 == eTypeLima_LIA then
                                    if( not DEC_GetMissionLimaFlag( DEC_GetLimaPoint( pointLima ) ) ) then
                                        do
                                            DEC_SetMissionLimaFlag( DEC_GetLimaPoint( pointLima ), true )
                                            Activate( self.activations.ACT_Dep_PasserSurLima, 1, { eEtatLima_LIA, } )
                                            --[[// DEC_RC( eRC_SurLima, eTypeLima_LIA );
]]DEC_Trace( "Passage LIA" )
                                                                                        self:SendFeedback( eActionEffectuee )
                                            do return end

                                        end
                                    end

                                    DEC_RemoveFromCategory( "points_interressants", pointLima )
                                    _continue = false
                                                                cases_switch_1[2] = true
                                end
                            end

                    end
                end

            end

        end

    end,

