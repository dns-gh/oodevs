        Activate( self.activations.BEH_Dep_ProgressionVers, 1, { self.positionArrivee, self._namedParams.eTypeProgression, self._namedParams.ePathType, } )
self.done_BEH_Dep_ProgressionVers = function( self, _, v ) self.eEtat = v[1] end
