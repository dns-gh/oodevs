node "BEH_Eni_Pion_ABC_JalonnerLesGros"
{
    feedbacks = { { { "done_BEH_Eni_Pion_ABC_JalonnerLesGros" }, "BEH_Eni_Pion_ABC_JalonnerLesGros" } },
    SendFeedback = function( self, value ) Feedback( self.feedbacks.done_BEH_Eni_Pion_ABC_JalonnerLesGros, {value} ) end,
    activations =
    {
        { "BEH_Eni_Pion_ABC_JalonnementSur", { "done_BEH_Eni_Pion_ABC_JalonnementSur" } },
        { "BEH_Dep_SePosterFaceADirectionDangereuse", { "done_BEH_Dep_SePosterFaceADirectionDangereuse" } },
    },

    create = function( self )
    end,

    activate = function( self )
        local Activate = Activate
        --[[//-----------------------------------------------------------------------------
]]--[[// On jalonne les unites eni seulement
]]--[[//-----------------------------------------------------------------------------
]]local listeConnaissances = DEC_Connaissances_UnitesEnnemiesDangereuses()
        if( not DIA_IsListEmpty( listeConnaissances ) ) then
            do
                for _,repEni in pairs( listeConnaissances or emptyTable ) do
                local _continue = true
                    do
                        local eValiditeRequete = nil
                        local rDangerosite = DEC_ConnaissanceAgent_Dangerosite( repEni, eValiditeRequete )
                        --[[// Attention au seuil
]]if( rDangerosite < 2 ) then
                            Activate( self.activations.BEH_Eni_Pion_ABC_JalonnementSur, S_Force_DangerSur( repEni ), { repEni, } )
                        end

                    end

                end

            end
--[[// Si pas d'ennemi à jalonner, surveiller
]]        else
            Activate( self.activations.BEH_Dep_SePosterFaceADirectionDangereuse, 1, emptyTable )
        end

    end,

