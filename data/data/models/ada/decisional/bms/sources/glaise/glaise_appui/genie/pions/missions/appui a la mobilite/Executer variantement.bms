includedFiles = includedFiles or {}
includedFiles["sources/glaise/glaise_appui/genie/pions/missions/appui a la mobilite/Executer variantement.bms"] = true

--[[// *****************************************************************************
]]--[[// MIS_GEN_ExecuterVariantement
]]--[[//
]]--[[// Commentaires: Refonte 27/10/2006
]]--[[//
]]--[[// *****************************************************************************
]]node "MIS_Pion_GEN_ExecuterVariantement"
{
    feedbacks = { { { "done_MIS_Pion_GEN_ExecuterVariantement" }, "MIS_Pion_GEN_ExecuterVariantement" } },
    SendFeedback = function( self, value ) Feedback( self.feedbacks.done_MIS_Pion_GEN_ExecuterVariantement, {value} ) end,
    activations =
    {
        { "BEH_Objet_Pion_GEN_EquiperExploiterPointFranchissement", { "done_BEH_Objet_Pion_GEN_EquiperExploiterPointFranchissement" } },
    },

    create = function( self )
        self._namedParams = {}
        self._namedParams.mission = self.params.mission
    end,

    activate = function( self )
        local Activate = Activate
        --[[//-----------------------------------------------------------------
]]--[[// Init
]]--[[//-----------------------------------------------------------------
]]if( ModuleBegins() ) then
            do
                DEC_Trace( "------------- Executer variantement -----------------" )
                DEC_SetMission( myself, self._namedParams.mis )
                F_Pion_SeteEtatAmbiance( myself, eEtatAmbiance_Vitesse )
            end
--[[//-----------------------------------------------------------------
]]--[[// Validité de l'objet
]]--[[//-----------------------------------------------------------------
]]        end

        if( not DEC_ConnaissanceObjet_EstValide( self._namedParams.mis.siteFranchissement_ ) ) then
            Halt( self )
            do return end
        end

        --[[//-----------------------------------------------------------------
]]--[[// Comportement générique
]]--[[//-----------------------------------------------------------------
]]Activate( self.activations.BEH_Objet_Pion_GEN_EquiperExploiterPointFranchissement, 1, { self._namedParams.mis.siteFranchissement_, self._namedParams.mis.typePontage_, self._namedParams.mis.pointRegroupement_, } )
    end,

}

eventmanager_plugin = eventmanager_plugin or {}
eventmanager_plugin.MIS_Pion_GEN_ExecuterVariantement = { "mission" }
connections = connections or {}
connections[ #connections + 1 ] = { nodes.MIS_Pion_GEN_ExecuterVariantement, "BEH_Objet_Pion_GEN_EquiperExploiterPointFranchissement", nodes.BEH_Objet_Pion_GEN_EquiperExploiterPointFranchissement }
