]]node "BEH_Conduite_Automate_ABC_FlancGarder"
{
    feedbacks = { { { "done_BEH_Conduite_Automate_ABC_FlancGarder" }, "BEH_Conduite_Automate_ABC_FlancGarder" } },
    SendFeedback = function( self, value ) Feedback( self.feedbacks.done_BEH_Conduite_Automate_ABC_FlancGarder, {value} ) end,
    activations =
    {
        { "BEH_CoordDep_Automate_ABC_AssurerLaCoordinationInterne_", { "done_BEH_CoordDep_Automate_ABC_AssurerLaCoordinationInterne_" } },
        { "BEH_CoordDep_Automate_ABC_SeCoordonnerAvecAmi_", { "done_BEH_CoordDep_Automate_ABC_SeCoordonnerAvecAmi_" } },
        { "ACT_Info_Automate_Trace", { "done_ACT_Info_Automate_Trace" } },
        { "BEH_Ordre_Automate_Attendre_", { "done_BEH_Ordre_Automate_Attendre_" } },
        { "BEH_Dispositif_Automate_ABC_GererLesAppuis_", { "done_BEH_Dispositif_Automate_ABC_GererLesAppuis_" } },
        { "BEH_Dispositif_Automate_ABC_GererPionsDetruits_", { "done_BEH_Dispositif_Automate_ABC_GererPionsDetruits_" } },
        { "BEH_Dispositif_Automate_ABC_GererLeSE_", { "done_BEH_Dispositif_Automate_ABC_GererLeSE_" } },
        { "BEH_RC_Automate_GererConditionOperationnelle", { "done_BEH_RC_Automate_GererConditionOperationnelle" } },
    },

    create = function( self )
        self._namedParams = {}
        self._namedParams.pionACouvrir = self.params[1]
    end,

    activate = function( self )
        --[[//---------------------------------------------------------------------
]]--[[// Init
]]--[[//---------------------------------------------------------------------
]]if( ModuleBegins() ) then
            DEC_Trace( "BEH_Conduite_Automate_ABC_FlancGarder" )
        end

        Activate( self.activations.BEH_CoordDep_Automate_ABC_AssurerLaCoordinationInterne_, 1, {} )--[[//-----------------------------------------------------------------------------
]]--[[// Coordination --> est ce utile ici
]]--[[//-----------------------------------------------------------------------------
]]
        if( ( DEC_GetMission( self._namedParams.pionACouvrir ) ~= 0 and DEC_GetMission( self._namedParams.pionACouvrir ) ~= nil ) ) then
            Activate( self.activations.BEH_CoordDep_Automate_ABC_SeCoordonnerAvecAmi_, 1, { self._namedParams.pionACouvrir, true, } )
        else
do
                Activate( self.activations.ACT_Info_Automate_Trace, 1, { "$$$ L'unité amie n'a pas de mission, en attente $$$", } )
                Activate( self.activations.BEH_Ordre_Automate_Attendre_, 1, {} )
            end
--[[//-----------------------------------------------------------------------------
]]--[[// Gestion appuis et releve et SE
]]--[[//-----------------------------------------------------------------------------
]]        end

        Activate( self.activations.BEH_Dispositif_Automate_ABC_GererLesAppuis_, 1, {} )
        --[[// Avec relève
]]Activate( self.activations.BEH_Dispositif_Automate_ABC_GererPionsDetruits_, 1, { true, } )
        Activate( self.activations.BEH_Dispositif_Automate_ABC_GererLeSE_, 1, {} )
        --[[//-----------------------------------------------------------------------------
]]--[[// Condition opérationnelle
]]--[[//-----------------------------------------------------------------------------
]]Activate( self.activations.BEH_RC_Automate_GererConditionOperationnelle, 1, { eEtatEchelon_Eclairage, } )
    end,

}

