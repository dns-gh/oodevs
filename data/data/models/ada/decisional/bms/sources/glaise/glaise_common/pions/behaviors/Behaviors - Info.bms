                    self.repObjet = tmp.second.first
                    self.rDist = tmp.second.second
                    tmp = tmp.first
                    Activate( self.activations.ACT_RC_SurObjet, 1, { eRC_EnPosteFaceAObstacle, self.repObjet, } )
