                local _continue = true
                    do
                        local pion = x
                        StartActivateOverride( self, self.activations.BEH_Mission_Automate_AssignerFaireMouvementA, 1, { ePhase_CDT, pion, self._namedParams.positionRegroupement, eEtatEchelon_Second, } )
                    end

                end

            end
        end

    end,

