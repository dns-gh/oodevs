connections[ #connections + 1 ] = { nodes.BEH_Medo_Automate_RENS_ROHUM_AssignerRenseignerA, "BEH_Mission_Automate_RENS_ROHUM_AssignerRenseignerA", nodes.BEH_Mission_Automate_RENS_ROHUM_AssignerRenseignerA }
