]]node "MIS_Automate_CDT_ABC_Appuyer"
{
    activations =
    {
        { "BEH_Conduite_Automate_ABC_Appuyer", {} },
    },

    create = function( self )
        self._namedParams = {}
        self._namedParams.mission = self.params.mission
    end,

    activate = function( self )
        --[[//-----------------------------------------------------------------------------
]]--[[// Init
]]--[[//-----------------------------------------------------------------------------
]]if( ModuleBegins() ) then
            DEC_SetMission( myself, self._namedParams.mission )
        end

        --[[//-----------------------------------------------------------------------------
]]--[[// Gen
]]--[[//-----------------------------------------------------------------------------
]]--[[// Cast
]]local uniteAppuyer = self._namedParams.mission.compagnie_
        Activate( self.activations.BEH_Conduite_Automate_ABC_Appuyer, 1, { uniteAppuyer, } )
    end,

}

