]]node "MIS_Automate_CDT_ASA_HAWK_DefendreSite"
{
    feedbacks = { { { "done_MIS_Automate_CDT_ASA_HAWK_DefendreSite" }, "MIS_Automate_CDT_ASA_HAWK_DefendreSite" } },
    SendFeedback = function( self, value ) Feedback( self.feedbacks.done_MIS_Automate_CDT_ASA_HAWK_DefendreSite, {value} ) end,
    activations =
    {
        { "ACT_Info_Trace", { "done_ACT_Info_Trace" } },
        { "BEH_Eni_ASA_HAWK_GererTirsDefendreSite", { "done_BEH_Eni_ASA_HAWK_GererTirsDefendreSite" } },
    },

    create = function( self )
        self._namedParams = {}
        self._namedParams.mission = self.params.mission
    end,

    activate = function( self )
        Activate( self.activations.ACT_Info_Trace, 1, { "Mission Sgpt ASA : Defendre Site", } )
        Activate( self.activations.BEH_Eni_ASA_HAWK_GererTirsDefendreSite, 1, { self._namedParams.mission.zone_, } )
    end,

}

