connections[ #connections + 1 ] = { nodes.BEH_Medo_Automate_LOG_AssignerMissionTransporterA, "ACT_Mission_Automate_ControleCreation_NOK", nodes.ACT_Mission_Automate_ControleCreation_NOK }
