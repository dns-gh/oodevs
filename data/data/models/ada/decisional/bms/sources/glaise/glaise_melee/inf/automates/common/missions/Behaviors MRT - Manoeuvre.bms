node "BEH_Medo_Automate_Melee_AssignerReconnaitrePtA"
{
    feedbacks = { { { "done_BEH_Medo_Automate_Melee_AssignerReconnaitrePtA" }, "BEH_Medo_Automate_Melee_AssignerReconnaitrePtA" } },
    SendFeedback = function( self, value ) Feedback( self.feedbacks.done_BEH_Medo_Automate_Melee_AssignerReconnaitrePtA, {value} ) end,
    activations =
    {
        { "BEH_Mission_Automate_INF_AssignerA_ReconnaitrePt", { "done_BEH_Mission_Automate_INF_AssignerA_ReconnaitrePt" } },
        { "BEH_Mission_Automate_INF_AssignerReconnaitreA", { "done_BEH_Mission_Automate_INF_AssignerReconnaitreA" } },
    },

    create = function( self )
        self._namedParams = {}
        self._namedParams.selPions_PE = self.params[1]
        self._namedParams.lstPoints = self.params[2]
    end,

    activate = function( self )
        local Activate = Activate
                self.lstObjectifs = self.lstObjectifs or DEC_Geometrie_CreerListePoints()
        if( ModuleBegins() ) then
            do
                DEC_Copie_ListePoints( self._namedParams.lstPoints, self.lstObjectifs )
                --[[//-------------------------------------------------------------------------
]]--[[// Assignement des missions aux differents pions
]]--[[//-------------------------------------------------------------------------
]]local rIt_Pion = 0
                for _,x_pion in pairs( self._namedParams.selPions_PE or emptyTable ) do
                local _continue = true
                    do
                        local rIdProche = S_Geometrie_PointPlusProchePosition_DansFuseau( DEC_Automate_PionPosition( x_pion ), self.lstObjectifs )
                        if( rIdProche < DEC_ListePoints_Size( self.lstObjectifs ) ) then
                            do
                                local ptProche = DEC_Geometrie_CreerPoint()
                                DEC_Copie_Point( DEC_ListePoints_GetAt( self.lstObjectifs, rIdProche ), ptProche )
                                DEC_ListePoints_Remove( self.lstObjectifs, rIdProche )
                                StartActivateOverride( self, self.activations.BEH_Mission_Automate_INF_AssignerA_ReconnaitrePt, 1, { ePhase_MRT, x_pion, ptProche, } )
                            end
                        else
                            StartActivateOverride( self, self.activations.BEH_Mission_Automate_INF_AssignerReconnaitreA, 1, { ePhase_MRT, x_pion, } )
                        end

                    end

                end

            end
        end

    end,

    destroy = function( self )
            end,

}

