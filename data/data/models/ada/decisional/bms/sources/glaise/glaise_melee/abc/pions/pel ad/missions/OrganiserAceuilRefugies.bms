]]node "MIS_Pion_ABC_OrganiserAccueilColonneRefugies"
{
    feedbacks = { { { "done_MIS_Pion_ABC_OrganiserAccueilColonneRefugies" }, "MIS_Pion_ABC_OrganiserAccueilColonneRefugies" } },
    SendFeedback = function( self, value ) Feedback( self.feedbacks.done_MIS_Pion_ABC_OrganiserAccueilColonneRefugies, {value} ) end,
    activations =
    {
        { "BEH_Mission_Pion_ABC_OrganiserAccueilColonneRefugies", { "done_BEH_Mission_Pion_ABC_OrganiserAccueilColonneRefugies" } },
    },

    create = function( self )
        self._namedParams = {}
        self._namedParams.mission = self.params.mission
    end,

    activate = function( self )
        local Activate = Activate
        self.eEtat = self.eEtat or nil
        --[[//-----------------------------------------------------------------------------
]]--[[// Terminaison
]]--[[//-----------------------------------------------------------------------------
]]if( self.eEtat == eActionImpossible ) then
            self:SendFeedback( eActionImpossible )
            Halt( self )
            do return end
        end

        if( self.eEtat == eActionEffectuee ) then
            self:SendFeedback( eActionEffectuee )
            Halt( self )
            do return end
        end

        --[[//-----------------------------------------------------------------------------
]]--[[// Comportement de la mission
]]--[[//-----------------------------------------------------------------------------
]]Activate( self.activations.BEH_Mission_Pion_ABC_OrganiserAccueilColonneRefugies, 1, { self._namedParams.mission, M_MODELE_PION_ABC_ACCEUIL_REFUGIES_PH(), M_MODELE_PION_ABC_ACCEUIL_REFUGIES_ID(), } )
self.done_BEH_Mission_Pion_ABC_OrganiserAccueilColonneRefugies = function( self, _, v ) self.eEtat = v[1] end
    end,

}

