includedFiles = includedFiles or {}
includedFiles["sources/glaise/glaise_appui/artdsa/automates/common/fonctions/Fonctions - Misc.bms"] = true


function S_ForceDistance( distanceAuPoint )
    local _returnValue = 0
    assert( distanceAuPoint >= 0, "distanceAuPoint : distance negative" )
    _returnValue = 1 - ( 1 / ( 1 + ( distanceAuPoint ) ) )
    do return _returnValue end
end

