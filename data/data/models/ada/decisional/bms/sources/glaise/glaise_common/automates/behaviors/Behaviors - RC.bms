--[[// *****************************************************************************
]]--[[// BEH_RC_Automate_Situation
]]--[[//
]]--[[// Commentaires: 
]]--[[//
]]--[[// *****************************************************************************
]]node "BEH_RC_Automate_Situation"
{
    feedbacks = { { { "done_BEH_RC_Automate_Situation" }, "BEH_RC_Automate_Situation" } },
    SendFeedback = function( self, value ) Feedback( self.feedbacks.done_BEH_RC_Automate_Situation, {value} ) end,
    activations =
    {
        { "BEH_RC_Automate_Situation_Ambiance", { "done_BEH_RC_Automate_Situation_Ambiance" } },
        { "BEH_RC_Automate_Situation_RapFor", { "done_BEH_RC_Automate_Situation_RapFor" } },
        { "BEH_RC_Automate_Situation_NBC", { "done_BEH_RC_Automate_Situation_NBC" } },
        { "BEH_RC_Automate_Situation_ROE", { "done_BEH_RC_Automate_Situation_ROE" } },
        { "BEH_RC_Automate_Situation_Radio", { "done_BEH_RC_Automate_Situation_Radio" } },
        { "BEH_RC_Automate_Situation_Radar", { "done_BEH_RC_Automate_Situation_Radar" } },
        { "BEH_RC_Automate_Situation_Coordination", { "done_BEH_RC_Automate_Situation_Coordination" } },
        { "BEH_RC_Automate_Situation_Neutralisation", { "done_BEH_RC_Automate_Situation_Neutralisation" } },
        { "BEH_RC_Automate_Situation_Contact", { "done_BEH_RC_Automate_Situation_Contact" } },
        { "BEH_RC_Automate_Situation_Mission", { "done_BEH_RC_Automate_Situation_Mission" } },
        { "BEH_RC_Automate_Situation_Limas", { "done_BEH_RC_Automate_Situation_Limas" } },
    },

    create = function( self )
    end,

    activate = function( self )
        local Activate = Activate
        Activate( self.activations.BEH_RC_Automate_Situation_Ambiance, 1, emptyTable )--[[//-------------------------------------------------------------------------
]]--[[// CRs en dehors du cadre de la mission
]]--[[//-------------------------------------------------------------------------
]]
        Activate( self.activations.BEH_RC_Automate_Situation_RapFor, 1, emptyTable )
        Activate( self.activations.BEH_RC_Automate_Situation_NBC, 1, emptyTable )
        Activate( self.activations.BEH_RC_Automate_Situation_ROE, 1, emptyTable )
        Activate( self.activations.BEH_RC_Automate_Situation_Radio, 1, emptyTable )
        Activate( self.activations.BEH_RC_Automate_Situation_Radar, 1, emptyTable )
        Activate( self.activations.BEH_RC_Automate_Situation_Coordination, 1, emptyTable )--[[// Ordre anibas attendre poursuivre.
]]
        Activate( self.activations.BEH_RC_Automate_Situation_Neutralisation, 1, emptyTable )
        --[[//-------------------------------------------------------------------------
]]--[[// CRs valables pendant une mission --> reinit des que nouvelle mission
]]--[[//-------------------------------------------------------------------------
]]--[[// Valable un tic
]]if( DEC_NouvelleMission() ) then
            do return end
        end

        Activate( self.activations.BEH_RC_Automate_Situation_Contact, 1, emptyTable )
        Activate( self.activations.BEH_RC_Automate_Situation_Mission, 1, emptyTable )
        Activate( self.activations.BEH_RC_Automate_Situation_Limas, 1, emptyTable )--[[// lié à la mission
]]
    end,

