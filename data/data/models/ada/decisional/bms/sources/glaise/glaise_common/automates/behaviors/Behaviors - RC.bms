node "BEH_RC_Automate_Situation_Ambiance"
{
    feedbacks = { { { "done_BEH_RC_Automate_Situation_Ambiance" }, "BEH_RC_Automate_Situation_Ambiance" } },
    SendFeedback = function( self, value ) Feedback( self.feedbacks.done_BEH_RC_Automate_Situation_Ambiance, {value} ) end,
    activations =
    {
        { "ACT_RC_Automate", { "done_ACT_RC_Automate" } },
    },

    create = function( self )
    end,

    activate = function( self )
        local Activate = Activate
        for _,x in pairs( --[[//-------------------------------------------------------------------------
]]--[[// Des qu'un pion à changé d'ambiance...
]]--[[//-------------------------------------------------------------------------
]]DEC_Automate_PionsAvecPC() or emptyTable ) do
        local _continue = true
do
                local pion = x
                if( F_Pion_GeteEtatAmbiance( pion ) == eEtatAmbiance_Surete ) then
