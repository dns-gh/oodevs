]]node "BEH_Medo_Automate_ABC_AssignerMissionJalonerA"
{
    feedbacks = { { { "done_BEH_Medo_Automate_ABC_AssignerMissionJalonerA" }, "BEH_Medo_Automate_ABC_AssignerMissionJalonerA" } },
    SendFeedback = function( self, value ) Feedback( self.feedbacks.done_BEH_Medo_Automate_ABC_AssignerMissionJalonerA, {value} ) end,
    activations =
    {
        { "BEH_Mission_Automate_ABC_AssignerJalonnerA", { "done_BEH_Mission_Automate_ABC_AssignerJalonnerA" } },
    },

    create = function( self )
        self._namedParams = {}
        self._namedParams.selPions = self.params[1]
        self._namedParams.eEtatEchelon = self.params[2]
    end,

    activate = function( self )
        local Activate = Activate
        --[[//-------------------------------------------------------------------------
]]--[[// Le fuseau de l'escadron est coupé proportionnellement
]]--[[//-------------------------------------------------------------------------
]]if( ModuleBegins() ) then
            DEC_MRT_AffecteFuseaux( self._namedParams.selPions )
        end

        for _,pion in pairs( --[[//-----------------------------------------------------------------------------
]]--[[// Affectation des missions du premier echelon
]]--[[// Tous les pion de premier echelon on la mssion reconnaitre
]]--[[//-----------------------------------------------------------------------------
]]self._namedParams.selPions or emptyTable ) do
        local _continue = true
            Activate( self.activations.BEH_Mission_Automate_ABC_AssignerJalonnerA, 1, { ePhase_MRT, pion, self._namedParams.eEtatEchelon, } )

        end

    end,

}

