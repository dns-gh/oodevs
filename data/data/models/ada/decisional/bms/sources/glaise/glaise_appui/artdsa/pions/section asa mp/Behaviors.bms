]]node "BEH_ASA_Defaut"
{
    feedbacks = { { { "done_BEH_ASA_Defaut" }, "BEH_ASA_Defaut" } },
    SendFeedback = function( self, value ) Feedback( self.feedbacks.done_BEH_ASA_Defaut, {value} ) end,
    activations =
    {
        { "BEH_Eni_GererDemandesDeTirsCentralisees", { "done_BEH_Eni_GererDemandesDeTirsCentralisees" } },
    },

    create = function( self )
    end,

    activate = function( self )
        local Activate = Activate
        if( DEC_Agent_NiveauInstallation() >= eNiveauInstallation_PosteAmenage ) then
            Activate( self.activations.BEH_Eni_GererDemandesDeTirsCentralisees, 1, {} )
        end

    end,

