node "BEH_Pion_ABC_GarderSesDistancesPourSInfiltrer"
{
    feedbacks = { { { "done_BEH_Pion_ABC_GarderSesDistancesPourSInfiltrer" }, "BEH_Pion_ABC_GarderSesDistancesPourSInfiltrer" } },
    SendFeedback = function( self, value ) Feedback( self.feedbacks.done_BEH_Pion_ABC_GarderSesDistancesPourSInfiltrer, {value} ) end,
    activations =
    {
        { "BEH_Pion_ABC_GarderSesDistancesPourSInfiltrerFaceA", { "done_BEH_Pion_ABC_GarderSesDistancesPourSInfiltrerFaceA" } },
    },

    create = function( self )
    end,

    activate = function( self )
        local Activate = Activate
        --[[// Garder ses distances sur enis dangereux
]]local listeConnaissances = DEC_Connaissances_UnitesEnnemiesDangereuses()
        for _,repEni in pairs( listeConnaissances or emptyTable ) do
        local _continue = true
            Activate( self.activations.BEH_Pion_ABC_GarderSesDistancesPourSInfiltrerFaceA, S_Force_Distance( repEni ), { repEni, } )

        end

    end,

