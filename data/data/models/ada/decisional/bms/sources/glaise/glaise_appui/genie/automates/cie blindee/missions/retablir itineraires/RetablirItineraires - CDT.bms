    activations =
    {
        { "BEH_Misc_Automate_GEN_CDT_OuvrirAxesProgressionGererRenforcementDe", { "done_BEH_Misc_Automate_GEN_CDT_OuvrirAxesProgressionGererRenforcementDe" } },
