--[[// *****************************************************************************
]]--[[// Comportement par défaut.
]]--[[//
]]--[[// Commentaires: Les helicopteres sont tous posés pour au debut. Ils n'ont pas
]]--[[// d'objectif ni de plot de rafitaillement
]]--[[//
]]--[[// toutes les actions et les comportements par défaut sont activé avec une force 
]]--[[// maximale de 0.5
]]--[[// *****************************************************************************
]]node "BEH_Defaut"
{
    feedbacks = { { { "done_BEH_Defaut" }, "BEH_Defaut" } },
    SendFeedback = function( self, value ) Feedback( self.feedbacks.done_BEH_Defaut, {value} ) end,
    activations =
    {
        { "BEH_ASA_Defaut", { "done_BEH_ASA_Defaut" } },
        { "BEH_Pion_ASA_GererInstallation", { "done_BEH_Pion_ASA_GererInstallation" } },
        { "BEH_Mot_Pion_ASA_AffecterVariablesDEtat", { "done_BEH_Mot_Pion_ASA_AffecterVariablesDEtat" } },
        { "BEH_MAJ_EtatDefaut", { "done_BEH_MAJ_EtatDefaut" } },
        { "BEH_Mot_Pion_Embarquer", { "done_BEH_Mot_Pion_Embarquer" } },
        { "BEH_Ambiance", { "done_BEH_Ambiance" } },
        { "BEH_Nbc", { "done_BEH_Nbc" } },
        { "BEH_Rens", { "done_BEH_Rens" } },
        { "BEH_ROE", { "done_BEH_ROE" } },
        { "BEH_Info_Pion_RendreCompte", { "done_BEH_Info_Pion_RendreCompte" } },
    },

    create = function( self )
        self._namedParams = {}
        self._namedParams.nDummy = self.params[1]
    end,

    activate = function( self )
        local Activate = Activate
        if( ModuleBegins() ) then
            do
                F_Pion_SetobjectifEsquive( myself, nil )
                myself.typeDeploiement_ = eEtatDeploiement_omnidirectionnel
            end
        end

        Activate( self.activations.BEH_ASA_Defaut, 1, {} )
        Activate( self.activations.BEH_Pion_ASA_GererInstallation, 1, {} )
        Activate( self.activations.BEH_Mot_Pion_ASA_AffecterVariablesDEtat, 1, {} )--[[// ---------------------------------------------------------------------
]]--[[// MAJ VE
]]--[[// ---------------------------------------------------------------------
]]
        --[[//les pions de l'asa sont toujours organisés en un echelon
]]F_Pion_SeteEtatEchelon( myself, eEtatEchelon_Premier )
        Activate( self.activations.BEH_MAJ_EtatDefaut, 1, {} )--[[// ---------------------------------------------------------------------
]]--[[// MAJ de l'état DEC par défaut 
]]--[[// ---------------------------------------------------------------------
]]
        if( not S_Misc_EstDetruitPhysique() ) then
            do
                Activate( self.activations.BEH_Mot_Pion_Embarquer, 1, {} )
                Activate( self.activations.BEH_Ambiance, 1, {} )--[[// ---------------------------------------------------------------------
]]--[[// Comportements génériques
]]--[[// ---------------------------------------------------------------------
]]
                Activate( self.activations.BEH_Nbc, 1, {} )
                Activate( self.activations.BEH_Rens, 1, {} )
                Activate( self.activations.BEH_ROE, 1, {} )
            end
--[[// -------------------------------------------------------------------------
]]--[[// RC de haut niveau (phases missions)
]]--[[// -------------------------------------------------------------------------
]]        end

        Activate( self.activations.BEH_Info_Pion_RendreCompte, 1, {} )
    end,

--[[// -----------------------------------------------------------------------------
]]--[[// GererInstallation :
]]--[[// ----------------
]]--[[//
]]--[[// Commentaires : on prend un actuateur de déplacement si on est installé.
]]--[[//
]]--[[// -----------------------------------------------------------------------------
]]}

node "BEH_Pion_ASA_GererInstallation"
{
    feedbacks = { { { "done_BEH_Pion_ASA_GererInstallation" }, "BEH_Pion_ASA_GererInstallation" } },
    SendFeedback = function( self, value ) Feedback( self.feedbacks.done_BEH_Pion_ASA_GererInstallation, {value} ) end,
    create = function( self )
    end,

    activate = function( self )
        local Activate = Activate
        if( DEC_Agent_EstInstalle() ) then
            DEC_Elongation( M_POLY_DOCTRINE_ELONGATION_DEPLOIEMENT() )
        end

        if( DEC_Agent_EstDesinstalle() ) then
            DEC_Elongation( 1 )
        end

    end,

--[[////////////////////////////// PARTIE COMPORTEMENTS VIRTUELS /////////////////////////////////////////////////////
]]--[[// *****************************************************************************
]]--[[// BEH_Dep_Virtuel_ProgressionVers
]]--[[//
]]--[[// *****************************************************************************
]]}

node "BEH_Dep_Virtuel_ProgressionVers"
{
    feedbacks = { { { "done_BEH_Dep_Virtuel_ProgressionVers" }, "BEH_Dep_Virtuel_ProgressionVers" } },
    SendFeedback = function( self, value ) Feedback( self.feedbacks.done_BEH_Dep_Virtuel_ProgressionVers, {value} ) end,
    activations =
    {
        { "BEH_Dep_ProgressionVers", { "done_BEH_Dep_ProgressionVers" } },
    },

    create = function( self )
        self._namedParams = {}
        self._namedParams.point = self.params[1]
        self._namedParams.eTypeProgression = self.params[2]
        self._namedParams.eTypeIti = self.params[3]
    end,

    activate = function( self )
        local Activate = Activate
        Activate( self.activations.BEH_Dep_ProgressionVers, 1, { self._namedParams.point, self._namedParams.eTypeProgression, self._namedParams.eTypeIti, } )
    end,

--[[// *****************************************************************************
]]--[[// BEH_Dep_Virtuel_PreparationAction
]]--[[//
]]--[[// *****************************************************************************
]]}

node "BEH_Dep_Virtuel_PreparationAction"
{
    feedbacks = { { { "done_BEH_Dep_Virtuel_PreparationAction" }, "BEH_Dep_Virtuel_PreparationAction" } },
    SendFeedback = function( self, value ) Feedback( self.feedbacks.done_BEH_Dep_Virtuel_PreparationAction, {value} ) end,
    create = function( self )
    end,

    activate = function( self )
        local Activate = Activate
                self:SendFeedback( eActionEffectuee )
        do return end

    end,

}

