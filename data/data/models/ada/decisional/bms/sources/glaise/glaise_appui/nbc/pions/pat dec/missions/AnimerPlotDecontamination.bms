]]node "MIS_Pion_NBC_AnimerUnPlotDeDecontamination"
{
    feedbacks = { { { "done_MIS_Pion_NBC_AnimerUnPlotDeDecontamination" }, "MIS_Pion_NBC_AnimerUnPlotDeDecontamination" } },
    SendFeedback = function( self, value ) Feedback( self.feedbacks.done_MIS_Pion_NBC_AnimerUnPlotDeDecontamination, {value} ) end,
    activations =
    {
        { "BEH_Mission_Pion_NBC_AnimerPlotDecontamination", { "done_BEH_Mission_Pion_NBC_AnimerPlotDecontamination" } },
    },

    create = function( self )
        self._namedParams = {}
        self._namedParams.mission = self.params.mission
    end,

    activate = function( self )
        local Activate = Activate
        self.eEtat = self.eEtat or nil
        --[[//-----------------------------------------------------------------------------
]]--[[// Terminaison
]]--[[//-----------------------------------------------------------------------------
]]if( self.eEtat == eActionImpossible ) then
            self:SendFeedback( eActionImpossible )
            Halt( self )
            do return end
        end

        if( self.eEtat == eActionEffectuee ) then
            self:SendFeedback( eActionEffectuee )
            Halt( self )
            do return end
        end

        --[[//-----------------------------------------------------------------------------
]]--[[// Comportement de la mission
]]--[[//-----------------------------------------------------------------------------
]]Activate( self.activations.BEH_Mission_Pion_NBC_AnimerPlotDecontamination, 1, { self._namedParams.mission, } )
self.done_BEH_Mission_Pion_NBC_AnimerPlotDecontamination = function( self, _, v ) self.eEtat = v[1] end
    end,

}

