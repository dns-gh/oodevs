--[[// *****************************************************************************
]]--[[// Comportement par défaut.
]]--[[//
]]--[[// Commentaires: n'est pas affecté par les 'magic move'
]]--[[//
]]--[[// *****************************************************************************
]]node "DefaultBehavior"
{
    feedbacks = { { { "done_Default" }, "Default" } },
    SendFeedback = function( self, value ) Feedback( self.feedbacks.done_Default, {value} ) end,
    activations =
    {
        { "BEH_Info_Mos", { "done_BEH_Info_Mos" } },
        { "BEH_Info_Pion_RendreCompte", { "done_BEH_Info_Pion_RendreCompte" } },
        { "BEH_Default_ReactionPopulations", { "done_BEH_Default_ReactionPopulations" } },
    },

    create = function( self )
    end,

    activate = function( self )
        local Activate = Activate
        Activate( self.activations.BEH_Info_Mos, 1, emptyTable )--[[//-----------------------------------------------------------------------------
]]--[[// INFO MOS
]]--[[//-----------------------------------------------------------------------------
]]
        Activate( self.activations.BEH_Info_Pion_RendreCompte, 1, emptyTable )
        Activate( self.activations.BEH_Default_ReactionPopulations, 1, emptyTable )
    end,

}

connections = connections or {}
connections[ #connections + 1 ] = { nodes.Default, "Default", nodes.DefaultBehavior }
