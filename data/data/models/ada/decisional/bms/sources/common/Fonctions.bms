--[[//-----------------------------------------------------------------------------
]]--[[// Recherche parmi une selection de pions, les pions ayant la mission : @type// 
]]--[[//-----------------------------------------------------------------------------
]]--[[//-----------------------------------------------------------------------------
]]--[[// Recherche parmi une selection de pions, les pions de type : @type// 
]]--[[//-----------------------------------------------------------------------------
]]function S_Cherche_Filtre_TypeMission( sPions, type )
    local _returnValue = {}
    local sFiltre = {}
    for _,x in pairs( sPions or {} ) do
    local _continue = true
do
            local pion = x
            local mission = DEC_GetMission( pion )
            if( mission ~= nil and mission:GetType() == type ) then
                DIA_PushBack( sFiltre, pion )
            end

        end

    end

    _returnValue = sFiltre
    do return _returnValue end
end

function S_Filtre_ChercheTypePion( sPions, type )
    local _returnValue = {}
    local sFiltre = {}
    for _,x in pairs( sPions or {} ) do
    local _continue = true
do
            local pion = x
            if( pion:GetType() == type ) then
                DIA_PushBack( sFiltre, pion )
            end

        end

    end

    _returnValue = sFiltre
    do return _returnValue end
end

