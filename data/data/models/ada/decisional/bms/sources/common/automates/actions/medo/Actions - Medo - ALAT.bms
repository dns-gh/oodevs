]]--[[// -----------------------------------
]]node "ACT_Mission_Automate_ALAT_AssignerAllerAllerSeRecompleterA"
{
    feedbacks = { { { "done_ACT_Mission_Automate_ALAT_AssignerAllerAllerSeRecompleterA" }, "ACT_Mission_Automate_ALAT_AssignerAllerAllerSeRecompleterA" } },
