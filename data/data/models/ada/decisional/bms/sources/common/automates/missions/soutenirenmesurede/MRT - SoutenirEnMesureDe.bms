    },

    create = function( self )
        self._namedParams = {}
        self._namedParams.listePions = self.params[1]
    end,

    activate = function( self )
        local Activate = Activate
        self.mission = self.mission or DEC_GetMission( myself )
        if( ModuleBegins() ) then
for _,pion in pairs( DEC_Automate_PionsAvecPC() or emptyTable ) do
            local _continue = true
                StartActivateOverride( self, self.activations.BEH_Mission_Automate_AssignerSoutenirEnMesureDeA, 1, { ePhase_MRT, pion, self.mission.itineraire_, } )

            end
        end

    end,

