--[[// *****************************************************************************
]]--[[// BEH_MAJ_Automate_RecevoirOrdreGE
]]--[[//
]]--[[// Commentaires:
]]--[[//
]]--[[// *****************************************************************************
]]node "BEH_MAJ_Automate_RecevoirOrdreGE"
{
    feedbacks = { { { "done_BEH_MAJ_Automate_RecevoirOrdreGE" }, "BEH_MAJ_Automate_RecevoirOrdreGE" } },
    SendFeedback = function( self, value ) Feedback( self.feedbacks.done_BEH_MAJ_Automate_RecevoirOrdreGE, {value} ) end,
    activations =
    {
        { "BEH_Ordre_Automate_Ralentir_", { "done_BEH_Ordre_Automate_Ralentir_" } },
        { "BEH_Ordre_Automate_Attendre_", { "done_BEH_Ordre_Automate_Attendre_" } },
        { "BEH_Ordre_Automate_Poursuivre_", { "done_BEH_Ordre_Automate_Poursuivre_" } },
        { "BEH_Ordre_Automate_Decrocher_", { "done_BEH_Ordre_Automate_Decrocher_" } },
        { "BEH_Ordre_Automate_TenirSurLR_", { "done_BEH_Ordre_Automate_TenirSurLR_" } },
        { "BEH_Ordre_Automate_Tenir_", { "done_BEH_Ordre_Automate_Tenir_" } },
    },

    create = function( self )
    end,

    activate = function( self )
        local Activate = Activate
        if( F_Automat_GetbOrdreRalentir( myself ) == true ) then
            Activate( self.activations.BEH_Ordre_Automate_Ralentir_, 1, emptyTable )
        end

        if( F_Automat_GetbOrdreAttendre( myself ) == true ) then
            Activate( self.activations.BEH_Ordre_Automate_Attendre_, 1, emptyTable )
        end

        if( F_Automat_GetbOrdrePoursuivre( myself ) == true ) then
            Activate( self.activations.BEH_Ordre_Automate_Poursuivre_, 1, emptyTable )
        end

        if( F_Automat_GetbOrdreDecrocher( myself ) == true ) then
            Activate( self.activations.BEH_Ordre_Automate_Decrocher_, 1, emptyTable )--[[// Mis jalonner/Freiner du GE
]]
        end

        if( F_Automat_GetbOrdreTenirSurLR( myself ) == true ) then
            Activate( self.activations.BEH_Ordre_Automate_TenirSurLR_, 1, emptyTable )--[[// Mis jalonner/Freiner du GE
]]
        end

        if( F_Automat_GetbOrdreTenir( myself ) == true ) then
            Activate( self.activations.BEH_Ordre_Automate_Tenir_, 1, emptyTable )--[[// Mis jalonner/Freiner du GE
]]
        end

    end,

--[[// *****************************************************************************
]]--[[// BEH_Ordre_Automate_RecevoirOrdreInopine
]]--[[//
