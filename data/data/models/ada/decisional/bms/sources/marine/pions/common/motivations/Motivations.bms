--[[// *****************************************************************************
]]--[[//  MOT_GarderSesDistances
]]--[[//
]]--[[//	Commentaires: 
]]--[[//
]]--[[//  Permettre de réaliser facilement les missions rétrogrades en decrochant vers
]]--[[//  un point qui est deriere nous
]]--[[//  (freiner, jalonner)
]]--[[//
]]--[[// *****************************************************************************
]]motivation_plugin = motivation_plugin or {}
motivation_plugin.stimuli = motivation_plugin.stimuli or {}
motivation_plugin.statevariables = motivation_plugin.statevariables or {}
motivation_plugin.emotions = motivation_plugin.emotions or {}
motivation_plugin.motivations = motivation_plugin.motivations or {}

motivation_plugin.motivations[ "MOT_GarderSesDistances" ] =
{
    activation = {
        VE_RapFor = 100,
    },
    amplification = {
    },
    reduction = {
    },
    max_increase_rate = 100,
    max_decrease_rate = 10,
    max_output_value = 1000,
}

node "ActivatedByMOT_GarderSesDistances"
{
    feedbacks = { { { "done_ActivatedByMOT_GarderSesDistances" }, "ActivatedByMOT_GarderSesDistances" } },
    SendFeedback = function( self, value ) Feedback( self.feedbacks.done_ActivatedByMOT_GarderSesDistances, {value} ) end,
    activations =
    {
        { "BEH_MOT_Pion_MARINE_GarderSesDistances", { "done_BEH_MOT_Pion_MARINE_GarderSesDistances" } },
    },

    create = function( self )
    end,

    activate = function( self )
        local Activate = Activate
        --[[// -----------------------------------------------------------------------------
]]--[[// Modules activateurs 
]]--[[//
]]--[[//
]]--[[//
]]--[[// Intervale de force pour la sauvegardre de soi = [0] U [1000, 10000]
]]--[[// en realité, [0] U [4800, 10000]
]]--[[// -----------------------------------------------------------------------------
]]--[[//VE_RapFor = [0]U[4.8,10] 
]]--[[// -----------------------------------------------------------------------------
]]--[[// Influence de la motivation par les stimulis. La premiere valeur indique la 
]]--[[// valeur pour laquelle la motivation ne chanhera pas, le second la valeur pour 
]]--[[// laquelle la motivation va doubler.
]]--[[// -----------------------------------------------------------------------------
]]--[[// éventuellement d'autre stim
]]--[[// -----------------------------------------------------------------------------
]]--[[// Influence de la motivation par les stimulis. La premiere valeur indique la 
]]--[[// valeur pour laquelle la motivation ne changera pas, le second la valeur pour 
]]--[[// laquelle la motivation sera nulle.
]]--[[// -----------------------------------------------------------------------------
]]--[[// + éventuellemnt d'autre stim
]]--[[// -----------------------------------------------------------------------------
]]--[[// Variation de la motivation
]]--[[// -----------------------------------------------------------------------------
]]--[[// Coef de croissance max par unite de temps
]]--[[// Coef de decroissance max par unite de temps
]]--[[// Valeur max de la motivation.
]]--[[// -----------------------------------------------------------------------------
]]--[[// Code executé quand la valeur de la motivation est supérieur à 0.
]]--[[// -----------------------------------------------------------------------------
]]if( S_Misc_EstDetruitPhysique() ) then
            do return end
        end

        --[[//ce comportement permet de décrocher à une position de sécutité
]]--[[//afin de satisfaire le besoin de garder ses distances
]]Activate( self.activations.BEH_MOT_Pion_MARINE_GarderSesDistances, GetMotivation( "MOT_GarderSesDistances" ), {} )
    end,

}

