function DEC_UserTypeList_Remove( list, element )
    local index = nil
    for i,j in ipairs( list ) do
        if j == element then index = i break end
    end
    if index then
        local i = index
        while i < #list do
            list[ i ] = list[ i + 1 ]
            i = i + 1
        end
        list[ #list ] = nil
    end
end

function DEC_ListePoints_Remove( list, index )
    if index and index < #list then
        local i = index + 1 -- $$$$ LDC Conversion from lists beginning at 0 in C++ and directia3 to 1 in lua
        while i < #list do
            list[ i ] = list[ i + 1 ]
            i = i + 1
        end
        list[ #list ] = nil
    end
end

