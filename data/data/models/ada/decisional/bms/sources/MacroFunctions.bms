DIA_Remove = function( list, item )
    local listEnd = #list
    for i = 1, listEnd do
        if list[i] == item then
            for j = i, listEnd-1 do
                list[j] = list[j+1]
            end
            list[listEnd] = nil
            return
        end
    end
end

