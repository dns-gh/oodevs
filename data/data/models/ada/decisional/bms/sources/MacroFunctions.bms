function DEC_AssignMissionParameter( missionPion, parameter, value )
    if not assignMission[ parameter ] and not assignMissionListParameter[ parameter ] then initializeAssignMissions() end
    if not assignMission[ parameter ] and not assignMissionListParameter[ parameter ] then error( "No assignment for "..tostring( parameter )..": "..tostring( DEC_AssignMissionPionParameter ).."? : ".. tostring( value ) ) end

    if assignMissionListParameter[ parameter ] then
        local listParams = {}
        for i, parameterValue in pairs( value ) do
            listParams[i] = assignMissionListParameter[ parameter ]( parameterValue )
        end
        return DEC_AssignMissionListParameter( missionPion, parameter, listParams )
    else
        return assignMission[ parameter ](missionPion, parameter, value )
    end
end

