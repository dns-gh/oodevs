--[[//------------------------------------------------------------------------------
]]--[[//Rejoins une position et cree un site de deploiement
]]--[[//------------------------------------------------------------------------------
]]node "MIS_Automate_CDT_Emergency_Alert"
{
    feedbacks = { { { "done_MIS_Automate_CDT_Emergency_Alert" }, "MIS_Automate_CDT_Emergency_Alert" } },
    SendFeedback = function( self, value ) Feedback( self.feedbacks.done_MIS_Automate_CDT_Emergency_Alert, {value} ) end,
    create = function( self )
        self._namedParams = {}
        self._namedParams.mission = self.params.mission
    end,

    activate = function( self )
        --[[//-----------------------------------------------------------------------------
]]--[[// Init
]]--[[//-----------------------------------------------------------------------------
]]if( ModuleBegins() ) then
            do
                DEC_Trace( "$ T_Mission_Automate_Emergency_Alert: starting CDT $" )
                DEC_SetMission( myself, self._namedParams.mission )
            end
        end

    end,

}

eventmanager_plugin = eventmanager_plugin or {}
eventmanager_plugin.MIS_Automate_CDT_Emergency_Alert = { "mission" }
