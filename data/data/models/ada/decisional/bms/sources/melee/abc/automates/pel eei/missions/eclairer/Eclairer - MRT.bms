includedFiles = includedFiles or {}
includedFiles["sources/melee/abc/automates/pel eei/missions/eclairer/Eclairer - MRT.bms"] = true

--[[// ******************************************************************************
]]--[[// MIS_Automate_MRT_ABC_Eclairer
]]--[[// 
]]--[[// Commentaires:
]]--[[// 
]]--[[// ******************************************************************************
]]node "MIS_Automate_MRT_ABC_Eclairer"
{
    activations =
    {
        { "BEH_Medo_Automate_ABC_DonnerLesOrdresPourMissionDynamique", { "onHalt" } },
    },

    onHalt = OnHalt,

    feedbacks = { { { "onHalt" }, "MIS_Automate_MRT_ABC_Eclairer" } },

    create = function( self )
        self._namedParams = {}
        self._namedParams.mission = self.params.mission
    end,

    activate = function( self )
        --[[//-----------------------------------------------------------------------------
]]--[[// Init
]]--[[//-----------------------------------------------------------------------------
]]if( ModuleBegins() ) then
            do
                DEC_Trace( "$ MIS AUTO Eclairer: debut MRT $" )
                                do
                    local listePionsSubordonnes = DEC_Automate_PionsAvecPC()
                    for _,x in pairs( listePionsSubordonnes ) do
                    local _continue = true
                        do
                            local pion = x
                            F_Pion_SeteEtatEchelon( pion, eEtatEchelon_Second )
                            F_Pion_SeteEtatSoutien( pion, eEtatSoutien_aucun )
                        end

                    end

                end

            end
--[[//-----------------------------------------------------------------------------
]]--[[// Gen
]]--[[//-----------------------------------------------------------------------------
]]        end

        Activate( self.activations.BEH_Medo_Automate_ABC_DonnerLesOrdresPourMissionDynamique, 1, { "T_Mission_Automate_ABC_Eclairer", } )
    end,

}

eventmanager_plugin = eventmanager_plugin or {}
eventmanager_plugin.MIS_Automate_MRT_ABC_Eclairer = { "mission" }
connections = connections or {}
connections[ #connections + 1 ] = { nodes.MIS_Automate_MRT_ABC_Eclairer, "BEH_Medo_Automate_ABC_DonnerLesOrdresPourMissionDynamique", nodes.BEH_Medo_Automate_ABC_DonnerLesOrdresPourMissionDynamique }
