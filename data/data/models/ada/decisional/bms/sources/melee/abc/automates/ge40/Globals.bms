    local _returnValue = {}
    local sFiltre = {}
    for _,x in pairs( sPions or emptyTable ) do
    local _continue = true
do
            local pion = x
            if( pion:GetType() == "Escadron_AMX" or pion:GetType() == "Escadron_XL" ) then
                DIA_PushBack( sFiltre, pion )
            end

        end

    end

    _returnValue = sFiltre
    do return _returnValue end
end

