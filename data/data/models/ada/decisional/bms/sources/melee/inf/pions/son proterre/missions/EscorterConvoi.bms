node "MIS_Pion_INF_EscorterUnite"
{
    feedbacks = { { { "done_MIS_Pion_INF_EscorterUnite" }, "MIS_Pion_INF_EscorterUnite" } },
    SendFeedback = function( self, value ) Feedback( self.feedbacks.done_MIS_Pion_INF_EscorterUnite, {value} ) end,
    activations =
    {
        { "BEH_Misc_Pion_DefinirTypeMission", { "done_BEH_Misc_Pion_DefinirTypeMission" } },
        { "BEH_MAJ_AgressiviteMission", { "done_BEH_MAJ_AgressiviteMission" } },
        { "BEH_Mission_Pion_VAB_EscorterUnConvoi", { "done_BEH_Mission_Pion_VAB_EscorterUnConvoi" } },
        { "ACT_MAJ_EtatPhaseMission", { "done_ACT_MAJ_EtatPhaseMission" } },
        { "BEH_Poste_Pion_INF_TomberEnGarde", { "done_BEH_Poste_Pion_INF_TomberEnGarde" } },
    },

    create = function( self )
        self._namedParams = {}
        self._namedParams.mission = self.params.mission
    end,

    activate = function( self )
        local Activate = Activate
        self.eCurrentPhase = self.eCurrentPhase or ePreparerMission
        self.ePhase = self.ePhase or eSeMettreEnPlace
        self.eAction = self.eAction or eActionEnCours
        if( ModuleBegins() ) then
            do
                DEC_SetMission( myself, self._namedParams.mission )
                F_Pion_SeteEtatAmbiance( myself, eEtatAmbiance_Vitesse )
                F_Pion_SeteEtatEchelon( myself, eEtatEchelon_Premier )
                --[[// --> Tiens lorsque le rapFor passe de 2.5 a 3.5
]]F_Pion_SetrTenir( myself, 1.5 )
                DEC_Trace( "Mission 'Escorter un convoi' : debut" )
            end
        end

        Activate( self.activations.BEH_Misc_Pion_DefinirTypeMission, 1, { eTypeContact_Defensif, } )
        Activate( self.activations.BEH_MAJ_AgressiviteMission, 1, { eIdentifie, M_TIR_PH_MISSION_MANOEUVRE(), } )
         do
            local _continue = true
            local switch_1 = self.ePhase
            local cases_switch_1 = {}
                if switch_1 == eSeMettreEnPlace then
                    --[[// eDefendre
]]if( self.eCurrentPhase == eFinMission ) then
                        do
                            self.ePhase = eFinMission
                                                        do return end

                        end
                    end

                    Activate( self.activations.BEH_Mission_Pion_VAB_EscorterUnConvoi, GetMotivation( "MOT_Mission" ), {} )
self.done_BEH_Mission_Pion_VAB_EscorterUnConvoi = function( self, _, v ) self.eCurrentPhase = v[1] end
                    _continue = false
                                cases_switch_1[1] = true
elseif switch_1 == eFinMission then
                    if( self.eAction == eActionEffectuee ) then
                        do
                            self.eAction = eActionEnCours
                                                        do
                                --[[//-----------------------------------------------------------------------------
]]--[[// On rend compte de la mission
]]--[[//-----------------------------------------------------------------------------
]]DEC_Trace( "Mission 'Escorter un convoi' : fin" )
                                DEC_FinMission()
                                                                self:SendFeedback( eActionEffectuee )
                                Halt( self )
                                do return end

                            end

                        end
                    end

                    Activate( self.activations.ACT_MAJ_EtatPhaseMission, 1, { eEtatPhaseMission_FinMission, } )
                    Activate( self.activations.BEH_Poste_Pion_INF_TomberEnGarde, 1, { true, } )
self.done_BEH_Poste_Pion_INF_TomberEnGarde = function( self, _, v ) self.eAction = v[1] end
                    _continue = false
                                cases_switch_1[2] = true

                else
                                end
            end

    end,

}

node "BEH_Mission_Pion_VAB_EscorterUnConvoi"
{
    feedbacks = { { { "done_BEH_Mission_Pion_VAB_EscorterUnConvoi" }, "BEH_Mission_Pion_VAB_EscorterUnConvoi" } },
    SendFeedback = function( self, value ) Feedback( self.feedbacks.done_BEH_Mission_Pion_VAB_EscorterUnConvoi, {value} ) end,
    activations =
    {
        { "BEH_Lima_Pion_RejoindreFuseau_EnAttente", { "done_BEH_Lima_Pion_RejoindreFuseau_EnAttente" } },
        { "ACT_Misc_Pion_Melee_Escorter", { "done_ACT_Misc_Pion_Melee_Escorter" } },
        { "BEH_Mot_Pion_Embarquer", { "done_BEH_Mot_Pion_Embarquer" } },
        { "ACT_MAJ_EtatPhaseMission", { "done_ACT_MAJ_EtatPhaseMission" } },
        { "ACT_Dep_ModulationVitesse", { "done_ACT_Dep_ModulationVitesse" } },
        { "BEH_Dep_Suivre", { "done_BEH_Dep_Suivre" } },
        { "ACT_Info_Trace", { "done_ACT_Info_Trace" } },
        { "BEH_Dep_Progresser", { "done_BEH_Dep_Progresser" } },
    },

    create = function( self )
    end,

    activate = function( self )
        local Activate = Activate
        self.mission = self.mission or DEC_GetMission( myself )
        self.pionConvoi = self.pionConvoi or self.mission.uniteAEscorter_
        self.eAction = self.eAction or eActionEnCours
        self.ePhase = self.ePhase or ePreparerMission
        if( ModuleBegins() ) then
            do
                StartActivateOverride( self, self.activations.BEH_Lima_Pion_RejoindreFuseau_EnAttente, 1.1, {} )
            end
        end

        Activate( self.activations.ACT_Misc_Pion_Melee_Escorter, 1, { self.pionConvoi, } )
        Activate( self.activations.BEH_Mot_Pion_Embarquer, 1, {} )
         do
            local _continue = true
            local switch_1 = self.ePhase
            local cases_switch_1 = {}
                if switch_1 == ePreparerMission then
                    --[[// Aller au plus vite			
]]Activate( self.activations.ACT_MAJ_EtatPhaseMission, 1.1, { eEtatPhaseMission_DebutMission, } )
                --[[// Progression tactique
]]                cases_switch_1[1] = true
                end
                if cases_switch_1[1] or switch_1 == eRejoindre then
                    if( F_Pion_GeteEtatAmbiance( myself ) == eEtatAmbiance_Vitesse ) then
                        Activate( self.activations.ACT_Dep_ModulationVitesse, 2, { 1, } )
                    end

                    Activate( self.activations.ACT_MAJ_EtatPhaseMission, 1, { eEtatPhaseMission_RejointUnite, } )
                    --[[// Escvorter l'unite à bonne distance
]]if( S_Geometrie_EstProchePosition( DEC_Agent_Position(), self.pionConvoi:DEC_Agent_Position(), 200 ) ) then
                        do
                            self.ePhase = eEscorter
                                                        do return end

                        end
--[[//-----------------------------------------------------------------------------
]]--[[// Rejoindre unite
]]--[[//-----------------------------------------------------------------------------      
]]                    end

                    Activate( self.activations.BEH_Dep_Suivre, 1, { self.pionConvoi, 50, } )
                    _continue = false
                                cases_switch_1[2] = true
elseif switch_1 == eEscorter then
                    Activate( self.activations.ACT_MAJ_EtatPhaseMission, 1, { eEtatPhaseMission_EscorteUnite, } )
                    Activate( self.activations.ACT_Info_Trace, 1, { "** Escorte de l'unité **", } )
                    --[[// Objectif de l'unité escorté                
]]if( F_Pion_GetitMvt( self.pionConvoi ) == nil ) then
                        do
                            Activate( self.activations.ACT_Info_Trace, 1, { "**** L'unite escorter doit avoir un objectif **** ", } )
                                                        do return end

                        end
                    end

                    if( self.eAction == eActionEffectuee ) then
                        do
                            self.eAction = eActionEnCours
                                                        self:SendFeedback( eFinMission )
                            Halt( self )
                            do return end

                        end
                    end

                    Activate( self.activations.BEH_Dep_Progresser, 1, { F_Pion_GetitMvt( self.pionConvoi ), eProgressionReco, } )
self.done_BEH_Dep_Progresser = function( self, _, v ) self.eAction = v[1] end
                    _continue = false
                                cases_switch_1[3] = true
                end
            end

    end,

}

node "BEH_Dep_Pion_INF_SeCoordonnerSurEscorte"
{
    feedbacks = { { { "done_BEH_Dep_Pion_INF_SeCoordonnerSurEscorte" }, "BEH_Dep_Pion_INF_SeCoordonnerSurEscorte" } },
    SendFeedback = function( self, value ) Feedback( self.feedbacks.done_BEH_Dep_Pion_INF_SeCoordonnerSurEscorte, {value} ) end,
    activations =
    {
        { "ACT_Dep_SePoster", { "done_ACT_Dep_SePoster" } },
    },

    create = function( self )
        self._namedParams = {}
        self._namedParams.pionAmi = self.params[1]
        self._namedParams.rDistMax = self.params[2]
        self._namedParams.rDistMin = self.params[3]
    end,

    activate = function( self )
        local Activate = Activate
        self.bActiveAttente = self.bActiveAttente or false
        self.bActiveRelai = self.bActiveRelai or false
        if( not S_Geometrie_EstProchePosition( self._namedParams.pionAmi:DEC_Agent_Position(), DEC_Agent_Position(), self._namedParams.rDistMax + self._namedParams.rDistMin ) ) then
            do return end
        end

        if( S_Geometrie_EstProchePosition( self._namedParams.pionAmi:DEC_Agent_Position(), DEC_Agent_Position(), self._namedParams.rDistMin ) ) then
            Activate( self.activations.ACT_Dep_SePoster, 1, {} )
        end

    end,

}

node "BEH_Dep_Pion_INF_Escorter"
{
    feedbacks = { { { "done_BEH_Dep_Pion_INF_Escorter" }, "BEH_Dep_Pion_INF_Escorter" } },
    SendFeedback = function( self, value ) Feedback( self.feedbacks.done_BEH_Dep_Pion_INF_Escorter, {value} ) end,
    activations =
    {
        { "BEH_Dep_ProgressionVers", { "done_BEH_Dep_ProgressionVers" } },
    },

    create = function( self )
        self._namedParams = {}
        self._namedParams.pionAmi = self.params[1]
    end,

    activate = function( self )
        local Activate = Activate
                self.eAction = self.eAction or eActionEnCours
        self.ptEnAvance = self.ptEnAvance or self._namedParams.pionAmi:DEC_Itineraire_ExtrapolerPosition( 2, true )
        if( self.eAction ~= eActionEnCours ) then
            self:SendFeedback( eActionHalt )
            Halt( self )
            do return end
        end

        Activate( self.activations.BEH_Dep_ProgressionVers, S_Force_INF_DistancePt_PourPion( self._namedParams.pionAmi, self.ptEnAvance ), { self.ptEnAvance, eProgressionReco, eTypeItiReconnaissance, } )
self.done_BEH_Dep_ProgressionVers = function( self, _, v ) self.eAction = v[1] end
    end,

    destroy = function( self )
            end,

}

