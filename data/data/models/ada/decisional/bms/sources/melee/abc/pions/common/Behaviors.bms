]]}

node "BEH_MAJ_Pion_ABC_EniDangereux"
{
    feedbacks = { { { "done_BEH_MAJ_Pion_ABC_EniDangereux" }, "BEH_MAJ_Pion_ABC_EniDangereux" } },
    SendFeedback = function( self, value ) Feedback( self.feedbacks.done_BEH_MAJ_Pion_ABC_EniDangereux, {value} ) end,
    activations =
    {
        { "ACT_MAJ_EnnemiEnCours", { "done_ACT_MAJ_EnnemiEnCours" } },
    },

    create = function( self )
    end,

    activate = function( self )
        local Activate = Activate
        local listeConnaissances = DEC_Connaissances_UnitesEnnemiesDangereuses()
        for _,repEni in pairs( listeConnaissances or emptyTable ) do
        local _continue = true
            Activate( self.activations.ACT_MAJ_EnnemiEnCours, S_Force_DangerSur( repEni ), { repEni, } )

        end

    end,

