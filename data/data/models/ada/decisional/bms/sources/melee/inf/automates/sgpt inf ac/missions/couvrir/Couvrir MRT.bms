node "MIS_Automate_MRT_INF_Couvrir"
{
    feedbacks = { { { "done_MIS_Automate_MRT_INF_Couvrir" }, "MIS_Automate_MRT_INF_Couvrir" } },
    SendFeedback = function( self, value ) Feedback( self.feedbacks.done_MIS_Automate_MRT_INF_Couvrir, {value} ) end,
    activations =
    {
        { "BEH_Medo_Automate_INF_PreparerMission_Statique", { "done_BEH_Medo_Automate_INF_PreparerMission_Statique" } },
    },

    create = function( self )
        self._namedParams = {}
        self._namedParams.mission = self.params.mission
    end,

    activate = function( self )
        local Activate = Activate
                self.eCreationMRT = self.eCreationMRT or eActionEnCours
        self.rN_PE = self.rN_PE or DEC_ListePoints_Size( self._namedParams.mission.objectifs_ )
        --[[//-----------------------------------------------------------------------------
]]--[[// Init
]]--[[//-----------------------------------------------------------------------------
]]if( ModuleBegins() ) then
            do
                DEC_SetMission( myself, self._namedParams.mission )
                DEC_Trace( "MIS_Automate_MRT_INF_Couvrir : debut MRT" )
            end
--[[//-----------------------------------------------------------------------------
]]--[[// MRT creee
]]--[[//-----------------------------------------------------------------------------
]]        end

        if( self.eCreationMRT == eActionImpossible ) then
            do
                                do
                    DEC_RC( eRC_MissionImpossible )
                end

                                self:SendFeedback( eActionImpossible )
                Halt( self )
                do return end

            end
        end

        if( self.eCreationMRT == eActionEffectuee ) then
            do
                self.eCreationMRT = eActionEnCours
                                do
                    DEC_Trace( "T_Mission_Automate_INF_Couvrir : fin MRT" )
                    DEC_MRT_Valide()
                                        self:SendFeedback( eActionEffectuee )
                    Halt( self )
                    do return end

                end

            end
--[[//-----------------------------------------------------------------------------
]]--[[// Modalité d'exécution
]]--[[//-----------------------------------------------------------------------------
]]        end

        Activate( self.activations.BEH_Medo_Automate_INF_PreparerMission_Statique, 1, { "T_Mission_Automate_INF_Couvrir", self.rN_PE, } )
self.done_BEH_Medo_Automate_INF_PreparerMission_Statique = function( self, _, v ) self.eCreationMRT = v[1] end
    end,

    destroy = function( self )
            end,

}

eventmanager_plugin = eventmanager_plugin or {}
eventmanager_plugin.MIS_Automate_MRT_INF_Couvrir = { "mission" }
connections = connections or {}
connections[ #connections + 1 ] = { nodes.MIS_Automate_MRT_INF_Couvrir, "BEH_Medo_Automate_INF_PreparerMission_Statique", nodes.BEH_Medo_Automate_INF_PreparerMission_Statique }
