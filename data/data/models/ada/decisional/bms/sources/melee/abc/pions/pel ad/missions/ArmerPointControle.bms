]]node "MIS_Pion_ABC_ArmerPointDeControle"
{
    feedbacks = { { { "done_MIS_Pion_ABC_ArmerPointDeControle" }, "MIS_Pion_ABC_ArmerPointDeControle" } },
    SendFeedback = function( self, value ) Feedback( self.feedbacks.done_MIS_Pion_ABC_ArmerPointDeControle, {value} ) end,
    activations =
    {
        { "ACT_Dep_PasserEnModeFurtif", { "done_ACT_Dep_PasserEnModeFurtif" } },
        { "BEH_Mission_Pion_ABC_ArmerPointDeControle", { "done_BEH_Mission_Pion_ABC_ArmerPointDeControle" } },
    },

    create = function( self )
        self._namedParams = {}
        self._namedParams.mission = self.params.mission
    end,

    activate = function( self )
        local Activate = Activate
        self.eEtat = self.eEtat or nil
        --[[//-----------------------------------------------------------------------------
]]--[[// Terminaison
]]--[[//-----------------------------------------------------------------------------
]]if( self.eEtat == eActionImpossible ) then
            self:SendFeedback( eActionImpossible )
            Halt( self )
            do return end
        end

        if( self.eEtat == eActionEffectuee ) then
            self:SendFeedback( eActionEffectuee )
            Halt( self )
            do return end
        end

        Activate( self.activations.ACT_Dep_PasserEnModeFurtif, 1, emptyTable )--[[//-----------------------------------------------------------------------------
]]--[[// SPE EEI
]]--[[//-----------------------------------------------------------------------------
]]
        --[[//-----------------------------------------------------------------------------
]]--[[// Comportement de la mission
]]--[[//-----------------------------------------------------------------------------
]]Activate( self.activations.BEH_Mission_Pion_ABC_ArmerPointDeControle, 1, { self._namedParams.mission, M_MODELE_PION_ABC_ARMER_UN_PIA_PH(), M_MODELE_PION_ABC_ARMER_UN_PIA_ID(), } )
self.done_BEH_Mission_Pion_ABC_ArmerPointDeControle = function( self, _, v ) self.eEtat = v[1] end
    end,

}

