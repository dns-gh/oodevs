node "ACT_GTIA_AssignerFuseau"
{
    feedbacks = { { { "done_ACT_GTIA_AssignerFuseau" }, "ACT_GTIA_AssignerFuseau" } },
    SendFeedback = function( self, value ) Feedback( self.feedbacks.done_ACT_GTIA_AssignerFuseau, {value} ) end,
    create = function( self )
        self._namedParams = {}
        self._namedParams.pion = self.params[1]
        self._namedParams.fuseau = self.params[2]
    end,

    select = function( self )
        if( DIA_GetLocalFakeActuator( "ACT_GTIA_AssignerFuseau", self._namedParams.pion ) < GetActivity() ) then
            do
                DIA_SetLocalFakeActuator( "ACT_GTIA_AssignerFuseau", self._namedParams.pion, GetActivity() )
                F_Automat_Setfuseau( self._namedParams.pion, self._namedParams.fuseau )
            end
        end

    end,

}

