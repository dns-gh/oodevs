node "BEH_GTIA_RepartirPionsDansFuseaux"
{
    feedbacks = { { { "done_BEH_GTIA_RepartirPionsDansFuseaux" }, "BEH_GTIA_RepartirPionsDansFuseaux" } },
    SendFeedback = function( self, value ) Feedback( self.feedbacks.done_BEH_GTIA_RepartirPionsDansFuseaux, {value} ) end,
    activations =
    {
        { "ACT_GTIA_AssignerFuseau", { "done_ACT_GTIA_AssignerFuseau" } },
    },

    create = function( self )
        self._namedParams = {}
        self._namedParams.listePions = self.params[1]
    end,

    activate = function( self )
        local Activate = Activate
        if( DIA_IsListEmpty( self._namedParams.listePions ) ) then
            do return end
        end

        self.fuseaux = self.fuseaux or DEC_DecouperFuseau( #( self._namedParams.listePions ) )
        local itFuseau = 0
        for _,pion in pairs( self._namedParams.listePions or emptyTable ) do
        local _continue = true
do
                Activate( self.activations.ACT_GTIA_AssignerFuseau, 1, { pion, DEC_UserTypeList_GetAt( self.fuseaux, itFuseau ), } )
                                itFuseau = itFuseau + 1
            end

        end

    end,

