node "BEH_Conduite_Automate_ABC_GererOrdre_Mortier"
{
    feedbacks = { { { "done_BEH_Conduite_Automate_ABC_GererOrdre_Mortier" }, "BEH_Conduite_Automate_ABC_GererOrdre_Mortier" } },
    SendFeedback = function( self, value ) Feedback( self.feedbacks.done_BEH_Conduite_Automate_ABC_GererOrdre_Mortier, {value} ) end,
    activations =
    {
        { "ACT_MOT_Automate_ABC_DemandeAppliqerFeux", { "done_ACT_MOT_Automate_ABC_DemandeAppliqerFeux" } },
    },

    create = function( self )
        self._namedParams = {}
        self._namedParams.selAppuis_Mortier = self.params[1]
    end,

    activate = function( self )
        local Activate = Activate
        local selPions_PE = S_Dispositif_Automate_ABC_ObtenirPionsPE_EnDifficultes()
        local rIterateur = 0
        for _,pion in pairs( selPions_PE or emptyTable ) do
        local _continue = true
if( rIterateur < #( self._namedParams.selAppuis_Mortier ) ) then
                do
                    Activate( self.activations.ACT_MOT_Automate_ABC_DemandeAppliqerFeux, 1, { DIA_GetAt( self._namedParams.selAppuis_Mortier, rIterateur ), } )
                                        rIterateur = rIterateur + 1
                end
            end

        end

    end,

