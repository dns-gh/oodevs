node "MIS_Automate_CDT_INF_MenerDefenseUsure"
{
    activations =
    {
        { "BEH_Conduite_Automate_INF_Defaut", { "onHalt" } },
        { "BEH_Conduite_Automate_INF_MenerDefenseUsure", { "onHalt" } },
        { "BEH_Conduite_Automate_INF_CadreRetrograde", { "onHalt" } },
    },

    onHalt = OnHalt,

    feedbacks = { { { "onHalt" }, "MIS_Automate_CDT_INF_MenerDefenseUsure" } },

    create = function( self )
        self._namedParams = {}
        self._namedParams.mission = self.params.mission
    end,

    activate = function( self )
                self.eActionMission = self.eActionMission or eActionEnCours
        --[[// persistent T_Point 			ptRegroupement = DEC_Geometrie_CalculerPointArrivee();	
]]--[[//-----------------------------------------------------------------------------
]]--[[// Init
]]--[[//-----------------------------------------------------------------------------
]]if( ModuleBegins() ) then
            do
                DEC_Trace( "Conduite Defense d'Usure : debut" )
                DEC_SetMission( myself, self._namedParams.mission )
            end
--[[/* MIA ADA //start behavior BEH_Conduite_Automate_GererFuseau();*/]]--[[// Defini les positions de regroupement
]]--[[//---------------------------------------------------------------------
]]--[[// behavior BEH_Misc_Automate_INF_DefinirRegroupement( ptRegroupement );
]]--[[// Conduite des pions au niveau section
]]--[[//-----------------------------------------------------------------------------		
]]        end

        if( self.eActionMission == eActionEffectuee ) then
            do
                self.eActionMission = eActionEnCours
                                do
                                        self.params.__returnValue = eActionEffectuee
                    Feedback( self.feedbacks["onHalt"], { "MIS_Automate_CDT_INF_MenerDefenseUsure" } )
Halt( self )
                    do return end

                end

            end
        end

        Activate( self.activations.BEH_Conduite_Automate_INF_Defaut, 1, setmetatable( { true, }, { __newindex = function( t, k, v ) if k == "__returnValue" then self.eActionMission = v else rawset( t, k, v ) end end } ) )
        Activate( self.activations.BEH_Conduite_Automate_INF_MenerDefenseUsure, 1, {} )
        Activate( self.activations.BEH_Conduite_Automate_INF_CadreRetrograde, 1, {} )
--[[// behavior BEH_Conduite_Automate_INF_Freiner();
]]    end,

    destroy = function( self )
            end,

}

eventmanager_plugin = eventmanager_plugin or {}
eventmanager_plugin.MIS_Automate_CDT_INF_MenerDefenseUsure = { "mission" }
connections = connections or {}
connections[ #connections + 1 ] = { nodes.MIS_Automate_CDT_INF_MenerDefenseUsure, "BEH_Conduite_Automate_INF_Defaut", nodes.BEH_Conduite_Automate_INF_Defaut }
connections[ #connections + 1 ] = { nodes.MIS_Automate_CDT_INF_MenerDefenseUsure, "BEH_Conduite_Automate_INF_MenerDefenseUsure", nodes.BEH_Conduite_Automate_INF_MenerDefenseUsure }
connections[ #connections + 1 ] = { nodes.MIS_Automate_CDT_INF_MenerDefenseUsure, "BEH_Conduite_Automate_INF_CadreRetrograde", nodes.BEH_Conduite_Automate_INF_CadreRetrograde }
