node "MIS_Automate_MRT_INF_AppuyerUnFreinage"
{
    feedbacks = { { { "done_MIS_Automate_MRT_INF_AppuyerUnFreinage" }, "MIS_Automate_MRT_INF_AppuyerUnFreinage" } },
    SendFeedback = function( self, value ) Feedback( self.feedbacks.done_MIS_Automate_MRT_INF_AppuyerUnFreinage, {value} ) end,
    activations =
    {
        { "BEH_Medo_Automate_INF_PreparerMission_Retrograde", { "done_BEH_Medo_Automate_INF_PreparerMission_Retrograde" } },
    },

    create = function( self )
        self._namedParams = {}
        self._namedParams.mission = self.params.mission
    end,

    activate = function( self )
                self.eCreationMRT = self.eCreationMRT or eActionEnCours
        local selPions = DEC_Automate_PionsSansPC()
        --[[//-----------------------------------------------------------------------------
]]--[[// Init
]]--[[//-----------------------------------------------------------------------------
]]if( ModuleBegins() ) then
            do
                DEC_SetMission( myself, self._namedParams.mission )
                DEC_Trace( "MIS_Automate_MRT_INF_Appuyer : debut MRT" )
            end
--[[//-----------------------------------------------------------------------------
]]--[[// MRT creee
]]--[[//-----------------------------------------------------------------------------
]]        end

        if( self.eCreationMRT == eActionImpossible ) then
            do
                                do
                    DEC_RC( eRC_MissionImpossible )
                end

                                self:SendFeedback( eActionImpossible )
                Halt( self )
                do return end

            end
        end

        if( self.eCreationMRT == eActionEffectuee ) then
            do
                self.eCreationMRT = eActionEnCours
                                do
                    DEC_Trace( "T_Mission_Automate_INF_Appuyer : fin MRT" )
                    DEC_MRT_Valide()
                                        self:SendFeedback( eActionEffectuee )
                    Halt( self )
                    do return end

                end

            end
--[[//-----------------------------------------------------------------------------
]]--[[// Modalité d'exécution
]]--[[//-----------------------------------------------------------------------------		
]]        end

        Activate( self.activations.BEH_Medo_Automate_INF_PreparerMission_Retrograde, 1, { "T_Mission_Automate_INF_AppuyerUnFreinage", 0, } )
self.done_BEH_Medo_Automate_INF_PreparerMission_Retrograde = function( self, _, v ) self.eCreationMRT = v[1] end
    end,

    destroy = function( self )
            end,

}

eventmanager_plugin = eventmanager_plugin or {}
eventmanager_plugin.MIS_Automate_MRT_INF_AppuyerUnFreinage = { "mission" }
connections = connections or {}
connections[ #connections + 1 ] = { nodes.MIS_Automate_MRT_INF_AppuyerUnFreinage, "BEH_Medo_Automate_INF_PreparerMission_Retrograde", nodes.BEH_Medo_Automate_INF_PreparerMission_Retrograde }
