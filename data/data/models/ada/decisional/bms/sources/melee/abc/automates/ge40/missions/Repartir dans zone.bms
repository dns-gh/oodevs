node "ACT_GTIA_AssignerZone"
{
    feedbacks = { { { "done_ACT_GTIA_AssignerZone" }, "ACT_GTIA_AssignerZone" } },
    SendFeedback = function( self, value ) Feedback( self.feedbacks.done_ACT_GTIA_AssignerZone, {value} ) end,
    create = function( self )
        self._namedParams = {}
        self._namedParams.pion = self.params[1]
        self._namedParams.zone = self.params[2]
    end,

    select = function( self )
        if( DIA_GetLocalFakeActuator( "ACT_GTIA_AssignerZone", self._namedParams.pion ) < GetActivity() ) then
            do
                DIA_SetLocalFakeActuator( "ACT_GTIA_AssignerZone", self._namedParams.pion, GetActivity() )
                F_Automat_Setzone( self._namedParams.pion, self._namedParams.zone )
            end
        end

    end,

}

