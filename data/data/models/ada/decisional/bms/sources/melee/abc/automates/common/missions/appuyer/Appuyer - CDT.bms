connections[ #connections + 1 ] = { nodes.BEH_Conduite_Automate_ABC_Appuyer, "BEH_Dispositif_Automate_ABC_GererLesAppuis_", nodes.BEH_Dispositif_Automate_ABC_GererLesAppuis_ }
