    SendFeedback = function( self, value ) Feedback( self.feedbacks.done_BEH_GTIA_RendreCompte, {value} ) end,
    activations =
    {
        { "BEH_GTIA_RendreCompteDebutFinMission", { "done_BEH_GTIA_RendreCompteDebutFinMission" } },
        { "BEH_GTIA_RendreCompteSurOrdreDeConduite", { "done_BEH_GTIA_RendreCompteSurOrdreDeConduite" } },
        { "BEH_GTIA_RendreCompteSurLima", { "done_BEH_GTIA_RendreCompteSurLima" } },
