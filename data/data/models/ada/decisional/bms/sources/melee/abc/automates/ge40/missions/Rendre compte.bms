    },

    create = function( self )
    end,

    activate = function( self )
        local Activate = Activate
        Activate( self.activations.BEH_GTIA_RendreCompteDebutFinMission, 1, emptyTable )
        Activate( self.activations.BEH_GTIA_RendreCompteSurOrdreDeConduite, 1, emptyTable )
        Activate( self.activations.BEH_GTIA_RendreCompteSurLima, 1, emptyTable )
        Activate( self.activations.BEH_GTIA_AFFICHE_temp, 1, emptyTable )--[[// TEMP rien à faire ici
]]
    end,

--[[// *****************************************************************************
]]--[[// BEH_GTIA_RendreCompteDebutFinMission
]]--[[// 
]]--[[// Commentaires: fait des CR spé aux GTIA
]]--[[//
]]--[[// *****************************************************************************
]]}

node "BEH_GTIA_RendreCompteDebutFinMission"
{
    feedbacks = { { { "done_BEH_GTIA_RendreCompteDebutFinMission" }, "BEH_GTIA_RendreCompteDebutFinMission" } },
    SendFeedback = function( self, value ) Feedback( self.feedbacks.done_BEH_GTIA_RendreCompteDebutFinMission, {value} ) end,
    activations =
    {
        { "ACT_RC_Automate", { "done_ACT_RC_Automate" } },
    },

    create = function( self )
    end,

    activate = function( self )
        local Activate = Activate
        if( ModuleBegins() ) then
            do
                Activate( self.activations.ACT_RC_Automate, 1, { eRC_DebutMission, } )
                                do return end

            end
        end

        local listePionsPE = S_Cherche_GE_Filtre_PasFinMission( S_Cherche_GE_ObtenirPionsDe( eEtatEchelon_Premier ) )
        if( DIA_IsListEmpty( listePionsPE ) ) then
            StartActivateOverride( self, self.activations.ACT_RC_Automate, 1, { eRC_FinMission, } )
        end

    end,

--[[// *****************************************************************************
]]--[[// BEH_GTIA_RendreCompteSurOrdreDeConduite
]]--[[//
]]--[[// Commentaires:
]]--[[//
]]--[[// *****************************************************************************
]]}

node "BEH_GTIA_RendreCompteSurOrdreDeConduite"
{
    feedbacks = { { { "done_BEH_GTIA_RendreCompteSurOrdreDeConduite" }, "BEH_GTIA_RendreCompteSurOrdreDeConduite" } },
    SendFeedback = function( self, value ) Feedback( self.feedbacks.done_BEH_GTIA_RendreCompteSurOrdreDeConduite, {value} ) end,
    activations =
    {
        { "ACT_RC_Automate", { "done_ACT_RC_Automate" } },
        { "ACT_Ordre_Suppression", { "done_ACT_Ordre_Suppression" } },
    },

    create = function( self )
    end,

    activate = function( self )
        local Activate = Activate
        local ordres_recus = DEC_GetCategory( "ordres_recus" )
        for _,ordre in pairs( ordres_recus or emptyTable ) do
        local _continue = true
do
                local repOrdre = ordre
                if( repOrdre:GetType() == "Rep_OrderConduite_AttendreSePoster" ) then
                    StartActivateOverride( self, self.activations.ACT_RC_Automate, 1, { eRC_AttenteOrdrePoursuivre, } )
                end

                if( repOrdre:GetType() == "Rep_OrderConduite_Deboucher" or repOrdre:GetType() == "Rep_OrderConduite_Poursuivre" ) then
                    StopActivateOverride( self, self.activations.ACT_RC_Automate, { eRC_AttenteOrdrePoursuivre, } )
                end

                if( repOrdre:GetType() == "Rep_OrderConduite_Decrocher" or g_bPhaseReceuil ) then
                    StartActivateOverride( self, self.activations.ACT_RC_Automate, 1, { eRC_Decroche, } )
                end

                Activate( self.activations.ACT_Ordre_Suppression, 1, { ordre, } )
            end

        end

    end,

--[[// *****************************************************************************
]]--[[// BEH_GTIA_RendreCompteSurLima
]]--[[//
]]--[[// Commentaires:
]]--[[//
