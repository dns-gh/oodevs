local emptyTable = emptyTable

--[[// *****************************************************************************
]]--[[// BEH_Auto_CDT_ABC_Tenir
]]--[[//
]]--[[// Commentaires:
]]--[[//
]]--[[// *****************************************************************************
]]node "BEH_Auto_CDT_ABC_Tenir"
{
    feedbacks = { { { "done_BEH_Auto_CDT_ABC_Tenir" }, "BEH_Auto_CDT_ABC_Tenir" } },
    SendFeedback = function( self, value ) Feedback( self.feedbacks.done_BEH_Auto_CDT_ABC_Tenir, {value} ) end,
    activations =
    {
        { "BEH_Dispositif_Automate_ABC_GererLesAppuis_", { "done_BEH_Dispositif_Automate_ABC_GererLesAppuis_" } },
        { "BEH_Dispositif_Automate_ABC_GererLeDispositifStatique_", { "done_BEH_Dispositif_Automate_ABC_GererLeDispositifStatique_" } },
        { "BEH_RC_Automate_GererConditionOperationnelle", { "done_BEH_RC_Automate_GererConditionOperationnelle" } },
    },

    create = function( self )
    end,

    activate = function( self )
        local Activate = Activate
        self.mission = self.mission or DEC_GetMission( myself )
        Activate( self.activations.BEH_Dispositif_Automate_ABC_GererLesAppuis_, 1, emptyTable )--[[//-----------------------------------------------------------------------------
]]--[[// Gestion appuis et releve et SE
]]--[[//-----------------------------------------------------------------------------
]]
        Activate( self.activations.BEH_Dispositif_Automate_ABC_GererLeDispositifStatique_, 1, { true, self.mission.positionInstallation_, } )
        --[[//-----------------------------------------------------------------------------
]]--[[// Condition opérationnelle
]]--[[//-----------------------------------------------------------------------------
]]Activate( self.activations.BEH_RC_Automate_GererConditionOperationnelle, 1, { eEtatEchelon_Eclairage, } )
    end,

}

