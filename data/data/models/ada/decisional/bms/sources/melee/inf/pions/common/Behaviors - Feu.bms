--[[//-----------------------------------------------------------------------------
]]--[[// Execute des tirs d'appui sur l'unite eni.
]]--[[//	Si une autre unite intervient, on lance des tirs de couverture
]]--[[//-----------------------------------------------------------------------------
]]node "BEH_Feu_Pion_INF_TirTactique_Couverture"
{
    activations =
    {
        { "ACT_Info_Trace", {} },
        { "BEH_Feu_Pion_INF_ConduireLesFeux_Sur", {} },
    },

    create = function( self )
        self._namedParams = {}
        self._namedParams.repEni = self.params[1]
    end,

    activate = function( self )
                if( ModuleBegins() ) then
            DEC_Trace( "Tir couverture : debut" )
        end

        if( not DEC_ConnaissanceAgent_EstValide( self._namedParams.repEni ) ) then
            self.params.__returnValue = eActionEffectuee
            self.deselect = function() end
            self.destroy = function() end
            Halt()
        end

        local bAttente = true
        local lstEnisPercus = DEC_Connaissances_UnitesEnnemiesVivantesPercues()
        for _,ksEni in pairs( lstEnisPercus ) do
        local _continue = true
do
                if( DEC_ConnaissanceAgent_EstDetruitTactique( ksEni ) or self._namedParams.repEni == ksEni ) then
                    _continue = false
                end

                    if _continue then
                    --[[// Valide le niveau d'identification du pion
]]--[[// --------------------------------------------------------------------
]]local strMsg = "Niveau d'identification demande ( " .. EnumToString( F_Pion_GeteNiveauAction( myself ), E_NiveauPerception ) .. " )"
                    if( not S_Eni_EstNiveauPerceptionGTIA( self._namedParams.repEni, F_Pion_GeteNiveauAction( myself ) ) ) then
                        do
                            Activate( self.activations.ACT_Info_Trace, 1, { strMsg .. " : NOK", } )
                            _continue = false
                        end
                    end

                        if _continue then
                        Activate( self.activations.ACT_Info_Trace, 1, { strMsg .. " : OK", } )
                        --[[// --------------------------------------------------------------------
]]bAttente = false
                        Activate( self.activations.BEH_Feu_Pion_INF_ConduireLesFeux_Sur, 1, { ksEni, eFormationTir_Embarque, } )
                    end
                end
            end

        end

        if( bAttente ) then
            do
                Activate( self.activations.ACT_Info_Trace, 1, { "Tir couverture : attente", } )
                                self.params.__returnValue = eActionEnPause
                return

            end
        end

                self.params.__returnValue = eActionEnCours
        return

    end,

    destroy = function( self )
                DEC_Trace( "Tir couverture : interrompu" )
    end,

--[[//-----------------------------------------------------------------------------
]]--[[// Action de fixage de l'ennemi
]]--[[// l'ennemi est fixe a partir du moment ou il est immobilise cad 
]]--[[// non opererationnel
]]--[[// On ne tire qu'une seule fois pour fixer l'ennemi
]]--[[//-----------------------------------------------------------------------------
]]}

node "BEH_Feu_Pion_INF_FixerNeutraliser"
{
    instances =
    {
        max = 1,
    },

    activations =
    {
        { "ACT_Tir_Pion_INF_Neutraliser", {} },
        { "BEH_Feu_Pion_INF_ConduireLesFeux_Sur", {} },
    },

    create = function( self )
        self._namedParams = {}
        self._namedParams.ksEni = self.params[1]
    end,

    activate = function( self )
        self.eAction = self.eAction or eActionEnCours
        --[[// surete vis a vis des tirs ennemis
]]if( ModuleBegins() ) then
            do
                Activate( self.activations.ACT_Tir_Pion_INF_Neutraliser, 1, { self._namedParams.ksEni, } )
                Activate( self.activations.BEH_Feu_Pion_INF_ConduireLesFeux_Sur, 1, setmetatable( { self._namedParams.ksEni, eFormationTir, }, { __newindex = function( t, k, v ) if k == "__returnValue" then self.eAction = v else rawset( t, k, v ) end end } ) )
            end
--[[// Lorsque l'on fixe, on ne tire qu'une seule fois.
]]        else
do
                if( self.eAction ~= eActionEffectuee ) then
                    self.params.__returnValue = self.eAction
                    return
                end

                                self.params.__returnValue = eActionEffectuee
                return

            end
        end

    end,

--[[//-----------------------------------------------------------------------------
]]--[[// Demande de tir de couverture
]]--[[// Tir sur un ennemi qui essaie de se deplacer
]]--[[// Relance l'action de tir si l'ennemi se deplace
]]--[[//-----------------------------------------------------------------------------
]]}

node "BEH_Feu_Pion_INF_TirFixer"
{
    instances =
    {
        boost = 1.1,
    },

    activations =
    {
        { "ACT_MAJ_EnnemiEnCours", {} },
        { "ACT_Info_Trace", {} },
        { "BEH_Feu_Pion_INF_FixerNeutraliser", {} },
    },

    create = function( self )
        self._namedParams = {}
        self._namedParams.repEni = self.params[1]
    end,

    activate = function( self )
        self.eAction = self.eAction or eActionEnCours
        if( ModuleBegins() ) then
            do
                DEC_Trace( "comportement fixe : debut" )
            end
        end

        if( not DEC_ConnaissanceAgent_EstValide( self._namedParams.repEni ) ) then
            self.params.__returnValue = eActionImpossible
            self.deselect = function() end
            self.destroy = function() end
            Halt()
        end

        Activate( self.activations.ACT_MAJ_EnnemiEnCours, 1, { self._namedParams.repEni, } )
        --[[// Si le pion n'a plus suffisament de munitions
]]if( self.eAction == eActionImpossible ) then
            do
                                do
                end

                                self.params.__returnValue = eActionImpossible
                self.deselect = function() end
                self.destroy = function() end
                Halt()

            end
        end

        if( self.eAction == eActionHalt or self.eAction == eActionEffectuee ) then
            self.params.__returnValue = eActionEffectuee
            self.deselect = function() end
            self.destroy = function() end
            Halt()
        end

        if( self.eAction ~= eActionEnCours and DEC_ConnaissanceAgent_EstEnMouvement( self._namedParams.repEni ) ) then
            do
                Activate( self.activations.ACT_Info_Trace, 1, { "Ennemi en deplacement", } )
                self.eAction = eActionEnCours
                                self.params.__returnValue = eActionEnCours
                return

            end
--[[// Si l'ennemi est fixe, on n'intervient pas dessus
]]        end

        if( not DEC_ConnaissanceAgent_EstEnMouvement( self._namedParams.repEni ) ) then
            do
                self.eAction = eActionEnPause
                                self.params.__returnValue = eActionEnPause
                return

            end
        end

        if( self.eAction == eActionEnCours ) then
            Activate( self.activations.BEH_Feu_Pion_INF_FixerNeutraliser, 1, setmetatable( { self._namedParams.repEni, }, { __newindex = function( t, k, v ) if k == "__returnValue" then self.eAction = v else rawset( t, k, v ) end end } ) )
        end

    end,

--[[//-----------------------------------------------------------------------------
]]--[[// Demande de tir de couverture (uniquement avec la composante VAB)
]]--[[// Tir sur un ennemi qui essaie de se deplacer
]]--[[//-----------------------------------------------------------------------------
]]}

