]]--[[// BEH_Conduite_Automate_ABC_Soutenir
]]--[[//
]]--[[// Commentaires:
]]--[[//
]]--[[// *****************************************************************************
]]node "BEH_Conduite_Automate_ABC_Soutenir"
{
    activations =
    {
        { "BEH_CoordDep_Automate_ABC_AssurerLaCoordinationInterne_", { "onHalt" } },
        { "BEH_Dispositif_Automate_ABC_GererLesAppuis_", { "onHalt" } },
        { "BEH_Dispositif_Automate_ABC_GererLeDispositif_", { "onHalt" } },
        { "BEH_CoordDep_Automate_ABC_SeCoordonnerAvecAmi_", { "onHalt" } },
        { "ACT_Info_Automate_Trace", { "onHalt" } },
        { "BEH_Ordre_Automate_Attendre_", { "onHalt" } },
        { "BEH_RC_Automate_GererConditionOperationnelle", { "onHalt" } },
    },

    onHalt = OnHalt,

    feedbacks = { { { "onHalt" }, "BEH_Conduite_Automate_ABC_Soutenir" } },

    create = function( self )
        self._namedParams = {}
        self._namedParams.uniteAAppuyer = self.params[1]
    end,

    activate = function( self )
        self.ePhase = self.ePhase or eCoordonner
        Activate( self.activations.BEH_CoordDep_Automate_ABC_AssurerLaCoordinationInterne_, 1, {} )--[[//-----------------------------------------------------------------------------
]]--[[// Coordination de l'escadron
]]--[[//-----------------------------------------------------------------------------
]]
        Activate( self.activations.BEH_Dispositif_Automate_ABC_GererLesAppuis_, 1, {} )--[[//-----------------------------------------------------------------------------
]]--[[// Gestion appuis et releve et SE
]]--[[//-----------------------------------------------------------------------------
]]
        Activate( self.activations.BEH_Dispositif_Automate_ABC_GererLeDispositif_, 1, { true, } )
        --[[//-----------------------------------------------------------------------------
]]--[[// Gestion appuis et releve et SE
]]--[[//-----------------------------------------------------------------------------
]]if( DEC_GetMission( self._namedParams.uniteAAppuyer ) ~= nil ) then
            Activate( self.activations.BEH_CoordDep_Automate_ABC_SeCoordonnerAvecAmi_, 1, { self._namedParams.uniteAAppuyer, false, } )
        else
do
                Activate( self.activations.ACT_Info_Automate_Trace, 1, { "$$$ L'unité amie n'a pas de mission, en attente $$$", } )
                Activate( self.activations.BEH_Ordre_Automate_Attendre_, 1, {} )
            end
--[[//-----------------------------------------------------------------------------
]]--[[// Condition opérationnelle
]]--[[//-----------------------------------------------------------------------------
]]        end

        Activate( self.activations.BEH_RC_Automate_GererConditionOperationnelle, 1, { eEtatEchelon_Premier, } )
    end,

}

