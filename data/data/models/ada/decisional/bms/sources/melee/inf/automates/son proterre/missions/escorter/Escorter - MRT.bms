]]--[[//
]]--[[// *****************************************************************************
]]node "MIS_Automate_MRT_INF_EscorterUnite"
{
    activations =
    {
        { "BEH_Medo_Automate_INF_DonnerLesOrdresPourMissionDynamique", {} },
    },

    create = function( self )
        self._namedParams = {}
        self._namedParams.mission = self.params.mission
    end,

    activate = function( self )
        --[[//---------------------------------------------------------------------
]]--[[// Init
]]--[[//---------------------------------------------------------------------
]]if( ModuleBegins() ) then
            do
                DEC_Trace( "$ MIS AUTO EscorterUnConvoi: debut MRT $" )
                DEC_SetMission( myself, self._namedParams.mission )
                                do
                    local listePionsSubordonnes = DEC_Automate_PionsAvecPC()
                    for _,x in pairs( listePionsSubordonnes or {} ) do
                    local _continue = true
                        do
                            local pion = x
                            F_Pion_SeteEtatEchelon( pion, eEtatEchelon_Second )
                            F_Pion_SeteEtatSoutien( pion, eEtatSoutien_aucun )
                        end

                    end

                end

            end
--[[//-----------------------------------------------------------------------------
]]--[[// Gen
]]--[[//-----------------------------------------------------------------------------
]]        end

        Activate( self.activations.BEH_Medo_Automate_INF_DonnerLesOrdresPourMissionDynamique, 1, { "T_Mission_Automate_INF_EscorterUnite", } )
    end,

--[[// *****************************************************************************
]]--[[// MIS_Automate_MRT_INF_EscorterUnites
]]--[[// 
]]--[[// Commentaires:
]]--[[//
]]--[[// *****************************************************************************
]]}

