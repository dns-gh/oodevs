connections[ #connections + 1 ] = { nodes.BEH_Conduite_Automate_ABC_Interdire, "BEH_Dispositif_Automate_ABC_GererLesAppuis_", nodes.BEH_Dispositif_Automate_ABC_GererLesAppuis_ }
connections[ #connections + 1 ] = { nodes.BEH_Conduite_Automate_ABC_Interdire, "BEH_Dispositif_Automate_ABC_GererLeDispositifStatiqueSurLima_", nodes.BEH_Dispositif_Automate_ABC_GererLeDispositifStatiqueSurLima_ }
