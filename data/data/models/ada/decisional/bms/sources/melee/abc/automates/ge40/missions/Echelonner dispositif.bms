node "ACT_GTIA_AssignerEchelonA"
{
    feedbacks = { { { "done_ACT_GTIA_AssignerEchelonA" }, "ACT_GTIA_AssignerEchelonA" } },
    SendFeedback = function( self, value ) Feedback( self.feedbacks.done_ACT_GTIA_AssignerEchelonA, {value} ) end,
    create = function( self )
        self._namedParams = {}
        self._namedParams.automat = self.params[1]
        self._namedParams.echelon = self.params[2]
    end,

    select = function( self )
        if( DIA_GetLocalFakeActuator( "ACT_GTIA_AssignerEchelonA", self._namedParams.automat ) < GetActivity() ) then
            do
                DIA_SetLocalFakeActuator( "ACT_GTIA_AssignerEchelonA", self._namedParams.automat, GetActivity() )
                F_Automat_SeteEtatEchelon( self._namedParams.automat, self._namedParams.echelon )
            end
        end

    end,

}

