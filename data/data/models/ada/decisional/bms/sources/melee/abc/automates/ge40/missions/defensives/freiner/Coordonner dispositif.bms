--[[// *****************************************************************************
]]--[[// BEH_GTIA_AssurerLaCoordinationRetrograde
]]--[[// 
]]--[[// Commentaires:
]]--[[//
]]--[[// *****************************************************************************
]]node "BEH_GTIA_AssurerLaCoordinationRetrograde"
{
    feedbacks = { { { "done_BEH_GTIA_AssurerLaCoordinationRetrograde" }, "BEH_GTIA_AssurerLaCoordinationRetrograde" } },
    SendFeedback = function( self, value ) Feedback( self.feedbacks.done_BEH_GTIA_AssurerLaCoordinationRetrograde, {value} ) end,
    activations =
    {
        { "BEH_GTIA_AssurerLaCoordinationRetrogradeIntraEchelon", { "done_BEH_GTIA_AssurerLaCoordinationRetrogradeIntraEchelon" } },
        { "BEH_GTIA_AssurerLaCoordinationRetrogradeInterEchelon", { "done_BEH_GTIA_AssurerLaCoordinationRetrogradeInterEchelon" } },
        { "BEH_GTIA_AssurerLaCoordinationRetrogradeSurRetard", { "done_BEH_GTIA_AssurerLaCoordinationRetrogradeSurRetard" } },
        { "BEH_GTIA_AssurerLaCoordinationSurLR", { "done_BEH_GTIA_AssurerLaCoordinationSurLR" } },
        { "BEH_GTIA_AssurerLaCoordinationAuContact", { "done_BEH_GTIA_AssurerLaCoordinationAuContact" } },
    },

    create = function( self )
    end,

    activate = function( self )
        local Activate = Activate
        --[[// On recupère les unités de mêlée seulement
]]local listePionsPEcl = S_Cherche_GE_Filtre_TypeAutomate_Melee( S_Cherche_GE_ObtenirPionsDe( eEtatEchelon_Eclairage ) )
        local listePionsPE = S_Cherche_GE_Filtre_TypeAutomate_Melee( S_Cherche_GE_ObtenirPionsDe( eEtatEchelon_Premier ) )
        local listePionsSE = S_Cherche_GE_Filtre_TypeAutomate_Melee( S_Cherche_GE_ObtenirPionsDe( eEtatEchelon_Second ) )
        --[[// Coordination ente les pions du même échelon --> evite de laisser des trous
]]Activate( self.activations.BEH_GTIA_AssurerLaCoordinationRetrogradeIntraEchelon, 1, { listePionsPEcl, } )
        Activate( self.activations.BEH_GTIA_AssurerLaCoordinationRetrogradeIntraEchelon, 1, { listePionsPE, } )
        Activate( self.activations.BEH_GTIA_AssurerLaCoordinationRetrogradeIntraEchelon, 1, { listePionsSE, } )
        --[[// Coordination enter PE / SE
]]if( DIA_IsListEmpty( S_Cherche_GE_Filtre_EtatSauvegarde( listePionsPE ) ) ) then
            Activate( self.activations.BEH_GTIA_AssurerLaCoordinationRetrogradeInterEchelon, 1, { listePionsPE, listePionsSE, } )
        end

        Activate( self.activations.BEH_GTIA_AssurerLaCoordinationRetrogradeSurRetard, 1, emptyTable )--[[// Coordination si retard --> décrocher
]]
        Activate( self.activations.BEH_GTIA_AssurerLaCoordinationSurLR, 1, emptyTable )--[[// Passage LR
]]
        Activate( self.activations.BEH_GTIA_AssurerLaCoordinationAuContact, 1, emptyTable )--[[// Contact
]]
    end,

--[[// *****************************************************************************
]]--[[// behavior BEH_GTIA_AssurerLaCoordinationSurLR()
]]--[[// 
]]--[[// Commentaires:
]]--[[//
]]--[[// *****************************************************************************
]]}

node "BEH_GTIA_AssurerLaCoordinationSurLR"
{
    feedbacks = { { { "done_BEH_GTIA_AssurerLaCoordinationSurLR" }, "BEH_GTIA_AssurerLaCoordinationSurLR" } },
    SendFeedback = function( self, value ) Feedback( self.feedbacks.done_BEH_GTIA_AssurerLaCoordinationSurLR, {value} ) end,
    activations =
    {
        { "ACT_GTIA_Automate_DonnerOrdreTenirSurLR", { "done_ACT_GTIA_Automate_DonnerOrdreTenirSurLR" } },
    },

    create = function( self )
    end,

    activate = function( self )
        local Activate = Activate
        for _,pion in pairs( S_Cherche_GE_ObtenirPionsDe( eEtatEchelon_Second ) or emptyTable ) do
        local _continue = true
            Activate( self.activations.ACT_GTIA_Automate_DonnerOrdreTenirSurLR, 1, { pion, } )

        end

    end,

