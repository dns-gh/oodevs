node "BEH_GTIA_AssurerLaCoordinationRetrogradeSurRetard"
{
    feedbacks = { { { "done_BEH_GTIA_AssurerLaCoordinationRetrogradeSurRetard" }, "BEH_GTIA_AssurerLaCoordinationRetrogradeSurRetard" } },
    SendFeedback = function( self, value ) Feedback( self.feedbacks.done_BEH_GTIA_AssurerLaCoordinationRetrogradeSurRetard, {value} ) end,
    activations =
    {
        { "ACT_GTIA_Automate_DonnerOrdreDecrocher", { "done_ACT_GTIA_Automate_DonnerOrdreDecrocher" } },
    },

    create = function( self )
    end,

    activate = function( self )
        local Activate = Activate
        if( S_AnalyseDelai_GE_EstEnRetard() ) then
            do
                for _,pion in pairs( S_Cherche_GE_ObtenirPionsDe( eEtatEchelon_Premier ) or emptyTable ) do
                local _continue = true
                    Activate( self.activations.ACT_GTIA_Automate_DonnerOrdreDecrocher, 1, { pion, } )

                end

                for _,pion in pairs( S_Cherche_GE_ObtenirPionsDe( eEtatEchelon_Eclairage ) or emptyTable ) do
                local _continue = true
                    Activate( self.activations.ACT_GTIA_Automate_DonnerOrdreDecrocher, 1, { pion, } )

                end

            end
        end

    end,

