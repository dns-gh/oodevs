]]}

node "BEH_GTIA_AssurerLaCoordinationAuContact"
{
    feedbacks = { { { "done_BEH_GTIA_AssurerLaCoordinationAuContact" }, "BEH_GTIA_AssurerLaCoordinationAuContact" } },
    SendFeedback = function( self, value ) Feedback( self.feedbacks.done_BEH_GTIA_AssurerLaCoordinationAuContact, {value} ) end,
    activations =
    {
        { "ACT_GTIA_Automate_DonnerOrdreTenir", { "done_ACT_GTIA_Automate_DonnerOrdreTenir" } },
    },

    create = function( self )
    end,

    activate = function( self )
        local Activate = Activate
        for _,x in pairs( S_Cherche_GE_ObtenirPionsDe( eEtatEchelon_Premier ) or emptyTable ) do
        local _continue = true
do
                local automat = x
                if( F_Automat_GeteEtatDec( automat ) ~= eEtatDec_Sauvegarde ) then
                    Activate( self.activations.ACT_GTIA_Automate_DonnerOrdreTenir, 1, { automat, } )
                end

            end

        end

    end,

