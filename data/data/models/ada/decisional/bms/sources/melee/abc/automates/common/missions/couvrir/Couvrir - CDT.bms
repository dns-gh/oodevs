--[[// *****************************************************************************
]]--[[// BEH_Conduite_Automate_ABC_Couvrir
]]--[[//
]]--[[// Commentaires: ATTENTION, ici mission statique
]]--[[//
]]--[[// *****************************************************************************
]]node "BEH_Conduite_Automate_ABC_Couvrir"
{
    activations =
    {
        { "BEH_Dispositif_Automate_ABC_GererLesAppuis_", { "onHalt" } },
        { "BEH_Dispositif_Automate_ABC_GererLeDispositifStatique_", { "onHalt" } },
        { "BEH_RC_Automate_GererConditionOperationnelle", { "onHalt" } },
    },

    onHalt = OnHalt,

    feedbacks = { { { "onHalt" }, "BEH_Conduite_Automate_ABC_Couvrir" } },

    create = function( self )
    end,

    activate = function( self )
        self.mission = self.mission or DEC_GetMission( myself )
        Activate( self.activations.BEH_Dispositif_Automate_ABC_GererLesAppuis_, 1, {} )--[[//-----------------------------------------------------------------------------
]]--[[// Gestion appuis et releve et SE
]]--[[//-----------------------------------------------------------------------------
]]
        Activate( self.activations.BEH_Dispositif_Automate_ABC_GererLeDispositifStatique_, 1, { true, self.mission.positionInstallation_, } )
        --[[//-----------------------------------------------------------------------------
]]--[[// Condition opérationnelle
]]--[[//-----------------------------------------------------------------------------
]]Activate( self.activations.BEH_RC_Automate_GererConditionOperationnelle, 1, { eEtatEchelon_Eclairage, } )
    end,

}

