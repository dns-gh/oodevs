node "BEH_Dispositif_Automate_INF_GererLeDispositif_"
{
    feedbacks = { { { "done_BEH_Dispositif_Automate_INF_GererLeDispositif_" }, "BEH_Dispositif_Automate_INF_GererLeDispositif_" } },
    SendFeedback = function( self, value ) Feedback( self.feedbacks.done_BEH_Dispositif_Automate_INF_GererLeDispositif_, {value} ) end,
    activations =
    {
        { "BEH_Dispositif_Automate_INF_GererPionsDetruits_", { "done_BEH_Dispositif_Automate_INF_GererPionsDetruits_" } },
        { "BEH_Dispositif_Automate_INF_GererLeSE_", { "done_BEH_Dispositif_Automate_INF_GererLeSE_" } },
    },

    create = function( self )
        self._namedParams = {}
        self._namedParams.bAvecReleve = self.params[1]
    end,

    activate = function( self )
        local Activate = Activate
        self.ePhase = self.ePhase or eGererSecondEchelon
         do
            local _continue = true
--[[//-----------------------------------------------------------------------------
]]--[[// Si un pion detruit de PE alors relever le pion si possible
]]--[[// Puis réévaluation du SE --> donc un pion détruit = réorganisation = delai
]]--[[//-----------------------------------------------------------------------------
]]--[[// 1 iteration ou l'on redonne les missions
]]--[[//-----------------------------------------------------------------------------
]]--[[// Organisation du SE
]]--[[//-----------------------------------------------------------------------------
]]            local switch_1 = self.ePhase
                if switch_1 == eGererPionDetruit then
                    Activate( self.activations.BEH_Dispositif_Automate_INF_GererPionsDetruits_, 1, { self._namedParams.bAvecReleve, } )
                    self.ePhase = eGererSecondEchelon
                    _continue = false
                elseif switch_1 == eGererSecondEchelon then
                    if( S_Dispositif_Automate_INF_EvtPionDetruit() ) then
                        do
                            DEC_Trace( "------- Evt Attrition ------" )
                            self.ePhase = eGererPionDetruit
                                                        do return end

                        end
                    end

                    Activate( self.activations.BEH_Dispositif_Automate_INF_GererLeSE_, 1, emptyTable )
                    _continue = false
                                end
            end

    end,

