]]node "BEH_Medo_Automate_ABC_AssignerMissionReconnaitreAxeA"
{
    feedbacks = { { { "done_BEH_Medo_Automate_ABC_AssignerMissionReconnaitreAxeA" }, "BEH_Medo_Automate_ABC_AssignerMissionReconnaitreAxeA" } },
    SendFeedback = function( self, value ) Feedback( self.feedbacks.done_BEH_Medo_Automate_ABC_AssignerMissionReconnaitreAxeA, {value} ) end,
    activations =
    {
        { "BEH_Mission_Automate_ABC_AssignerReconnaitreA", { "done_BEH_Mission_Automate_ABC_AssignerReconnaitreA" } },
    },

    create = function( self )
        self._namedParams = {}
        self._namedParams.selABC_PE = self.params[1]
        self._namedParams.eEtatEchelon = self.params[2]
    end,

    activate = function( self )
        local Activate = Activate
        --[[//-------------------------------------------------------------------------
]]--[[// Le fuseau de l'escadron est coupé proportionnellement
]]--[[//-------------------------------------------------------------------------
]]if( ModuleBegins() ) then
            DEC_MRT_AffecteFuseaux( self._namedParams.selABC_PE )
        end

        for _,pion in pairs( --[[//-----------------------------------------------------------------------------
]]--[[// Affectation des missions du premier echelon
]]--[[//-----------------------------------------------------------------------------
]]self._namedParams.selABC_PE or emptyTable ) do
        local _continue = true
            Activate( self.activations.BEH_Mission_Automate_ABC_AssignerReconnaitreA, 1, { ePhase_MRT, pion, self._namedParams.eEtatEchelon, } )

        end

    end,

}

