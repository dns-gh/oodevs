]]include "MacroFunctions.bms"

include "Humus/Humus_Melee/Abc/Automates/Pel Reco/Globals.bms"
include "Common/Automates/Perception.bms"
include "Humus/Humus_Melee/Abc/Automates/Pel Reco/Motivations.bms"
