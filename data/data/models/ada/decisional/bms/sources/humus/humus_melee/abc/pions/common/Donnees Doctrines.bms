--[[// *****************************************************************************
]]--[[// DISTANCE A LAQUELLE LES UNITES SE FIXENT
]]--[[// Distance de tube --> distance tir "efficace"
]]--[[//
]]--[[// *****************************************************************************
]]--[[// MIA à virer?
]]--[[//-----------------------------------------------------------------------------
]]--[[// Distance DANGER --> tir efficace d'un eni sur nous --> pour esquive dans 
]]--[[// Jalonner
]]--[[//-----------------------------------------------------------------------------
]]function S_Misc_Pion_ABC_ObtenirDistanceDangerSur( repEni )
    local _returnValue = 0
    local eValiditeRequete = nil
    _returnValue = DEC_Tir_PorteeMaxPourEtreTireParUnite( repEni, eValiditeRequete, M_MODELE_PION_ABC_PH_SECURITE() )
    if( eValiditeRequete == eRequeteInvalide ) then
        _returnValue = -1
    end

    do return _returnValue end
end

