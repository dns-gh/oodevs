--[[// ******************************************************************************
]]--[[// MIS_Automate_MRT_ABC_ArmerPointDeControle
]]--[[// 
]]--[[// Commentaires:
]]--[[// 
]]--[[// ******************************************************************************
]]node "MIS_Automate_MRT_ABC_ArmerPointDeControle"
{
    feedbacks = { { { "done_MIS_Automate_MRT_ABC_ArmerPointDeControle" }, "MIS_Automate_MRT_ABC_ArmerPointDeControle" } },
    SendFeedback = function( self, value ) Feedback( self.feedbacks.done_MIS_Automate_MRT_ABC_ArmerPointDeControle, {value} ) end,
    activations =
    {
        { "BEH_Medo_Automate_ABC_DonnerLesOrdresPourMissionStatique", { "done_BEH_Medo_Automate_ABC_DonnerLesOrdresPourMissionStatique" } },
    },

    create = function( self )
        self._namedParams = {}
        self._namedParams.mission = self.params.mission
    end,

    activate = function( self )
        --[[//-----------------------------------------------------------------------------
]]--[[// Init
]]--[[//-----------------------------------------------------------------------------
]]if( ModuleBegins() ) then
            do
                DEC_Trace( "$ MIS AUTO ArmerPointDeControle: debut MRT $" )
                                do
                    local listePionsSubordonnes = DEC_Automate_PionsAvecPC()
                    for _,x in pairs( listePionsSubordonnes or {} ) do
                    local _continue = true
                        do
                            local pion = x
                            F_Pion_SeteEtatEchelon( pion, eEtatEchelon_Second )
                            F_Pion_SeteEtatSoutien( pion, eEtatSoutien_aucun )
                        end

                    end

                end

            end
--[[//-----------------------------------------------------------------------------
]]--[[// Gen
]]--[[//-----------------------------------------------------------------------------
]]        end

        Activate( self.activations.BEH_Medo_Automate_ABC_DonnerLesOrdresPourMissionStatique, 1, { "T_Mission_Automate_ABC_ArmerPointDeControle", } )
    end,

}

eventmanager_plugin = eventmanager_plugin or {}
eventmanager_plugin.MIS_Automate_MRT_ABC_ArmerPointDeControle = { "mission" }
connections = connections or {}
connections[ #connections + 1 ] = { nodes.MIS_Automate_MRT_ABC_ArmerPointDeControle, "BEH_Medo_Automate_ABC_DonnerLesOrdresPourMissionStatique", nodes.BEH_Medo_Automate_ABC_DonnerLesOrdresPourMissionStatique }
