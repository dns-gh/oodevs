connections[ #connections + 1 ] = { nodes.MIS_Pion_INF_Rezzou, "ACT_Misc_Pion_INF_MAJ_PositionRegroupement", nodes.ACT_Misc_Pion_INF_MAJ_PositionRegroupement }
connections[ #connections + 1 ] = { nodes.MIS_Pion_INF_Rezzou, "BEH_Ordre_Inopine", nodes.BEH_Ordre_Inopine }
connections[ #connections + 1 ] = { nodes.MIS_Pion_INF_Rezzou, "ACT_Info_Trace", nodes.ACT_Info_Trace }
connections[ #connections + 1 ] = { nodes.MIS_Pion_INF_Rezzou, "BEH_Mission_Pion_INF_Rezzou", nodes.BEH_Mission_Pion_INF_Rezzou }
connections[ #connections + 1 ] = { nodes.MIS_Pion_INF_Rezzou, "BEH_Manoeuvre_Pion_INF_SeReplier", nodes.BEH_Manoeuvre_Pion_INF_SeReplier }
connections[ #connections + 1 ] = { nodes.MIS_Pion_INF_Rezzou, "ACT_MAJ_EtatPhaseMission", nodes.ACT_MAJ_EtatPhaseMission }
connections[ #connections + 1 ] = { nodes.MIS_Pion_INF_Rezzou, "BEH_Poste_Pion_INF_TomberEnGarde", nodes.BEH_Poste_Pion_INF_TomberEnGarde }
