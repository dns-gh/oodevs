node "BEH_CoordTir_Automate_INF_Intervention_LCAR"
{
    feedbacks = { { { "done_BEH_CoordTir_Automate_INF_Intervention_LCAR" }, "BEH_CoordTir_Automate_INF_Intervention_LCAR" } },
