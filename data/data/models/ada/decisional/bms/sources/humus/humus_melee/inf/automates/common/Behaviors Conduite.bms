--[[//-----------------------------------------------------------------------------
]]--[[// @bFinMission : Gere ou non dans le comportement la fin de mission
]]--[[//-----------------------------------------------------------------------------
]]node "BEH_Conduite_Automate_INF_Defaut"
{
    feedbacks = { { { "done_BEH_Conduite_Automate_INF_Defaut" }, "BEH_Conduite_Automate_INF_Defaut" } },
    SendFeedback = function( self, value ) Feedback( self.feedbacks.done_BEH_Conduite_Automate_INF_Defaut, {value} ) end,
    activations =
    {
        { "BEH_Situation_Automate_INF_FinDeMission", { "done_BEH_Situation_Automate_INF_FinDeMission" } },
        { "BEH_Situation_Automate_INF_Defaut", { "done_BEH_Situation_Automate_INF_Defaut" } },
        { "BEH_Conduite_Automate_INF_Gerer_UniteInconnues", { "done_BEH_Conduite_Automate_INF_Gerer_UniteInconnues" } },
    },

    create = function( self )
        self._namedParams = {}
        self._namedParams.bFinMission = self.params[1]
    end,

    activate = function( self )
        local Activate = Activate
        self.eActionMission = self.eActionMission or eActionEnCours
        self.bMissionTerminee = self.bMissionTerminee or false
        if( self.bMissionTerminee ) then
            self:SendFeedback( eActionEffectuee )
            Halt( self )
            do return end
        end

        --[[//-----------------------------------------------------------------------------
]]--[[// Gestion de la sauvegarde de la compagnie
]]--[[//-----------------------------------------------------------------------------
]]--[[// behavior BEH_Mot_Automate_GererSauvegardeFaceAuxEnis();
]]--[[//-----------------------------------------------------------------------------
]]--[[// Gestion de la fin de mission
]]--[[//-----------------------------------------------------------------------------
]]if( self.eActionMission ~= eActionEnCours ) then
            do
                for _,pion in pairs( DEC_Automate_PionsAvecPC() or emptyTable ) do
                local _continue = true
                    pion:SetStateVariable( "VE_PreparationMission", 0 )

                end

                --[[// DEC_RC( eRC_FinMission );
]]--[[// DEC_FinMission();
]]if( self._namedParams.bFinMission ) then
                    self:SendFeedback( eActionEffectuee )
                    Halt( self )
                    do return end
                end

            end
        else
            Activate( self.activations.BEH_Situation_Automate_INF_FinDeMission, 1, emptyTable )
self.done_BEH_Situation_Automate_INF_FinDeMission = function( self, _, v ) self.eActionMission = v[1] end
        end

        --[[//-----------------------------------------------------------------------------
]]--[[// Gestion des RC recus 
]]--[[//-----------------------------------------------------------------------------	
]]Activate( self.activations.BEH_Situation_Automate_INF_Defaut, 1, { self._namedParams.bFinMission, } )
        Activate( self.activations.BEH_Conduite_Automate_INF_Gerer_UniteInconnues, 1, emptyTable )--[[//-----------------------------------------------------------------------------
]]--[[// Gestion des unites inconnues n'ayant pas de mission
]]--[[//-----------------------------------------------------------------------------	
]]
    end,

}

