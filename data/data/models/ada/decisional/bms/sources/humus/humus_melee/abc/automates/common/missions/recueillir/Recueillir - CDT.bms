]]node "BEH_Conduite_Automate_ABC_Recueillir"
{
    feedbacks = { { { "done_BEH_Conduite_Automate_ABC_Recueillir" }, "BEH_Conduite_Automate_ABC_Recueillir" } },
    SendFeedback = function( self, value ) Feedback( self.feedbacks.done_BEH_Conduite_Automate_ABC_Recueillir, {value} ) end,
    activations =
    {
        { "BEH_Dispositif_Automate_ABC_GererLesAppuis_", { "done_BEH_Dispositif_Automate_ABC_GererLesAppuis_" } },
        { "BEH_Dispositif_Automate_ABC_GererLeDispositifStatiqueSurLima_", { "done_BEH_Dispositif_Automate_ABC_GererLeDispositifStatiqueSurLima_" } },
        { "BEH_Conduite_Automate_ABC_Recueillir_FaireDecrocherLePEcl", { "done_BEH_Conduite_Automate_ABC_Recueillir_FaireDecrocherLePEcl" } },
        { "BEH_RC_Automate_GererConditionOperationnelle", { "done_BEH_RC_Automate_GererConditionOperationnelle" } },
    },

    create = function( self )
    end,

    activate = function( self )
        local Activate = Activate
        self.LR = self.LR or DEC_GetLima( eTypeLima_LR )
        --[[//---------------------------------------------------------------------
]]--[[// Init
]]--[[//---------------------------------------------------------------------
]]if( ModuleBegins() ) then
            DEC_Trace( "BEH_Conduite_Automate_ABC_Recueillir" )
        end

        Activate( self.activations.BEH_Dispositif_Automate_ABC_GererLesAppuis_, 1, emptyTable )--[[//-----------------------------------------------------------------------------
]]--[[// Coordination
]]--[[//-----------------------------------------------------------------------------
]]--[[//behavior BEH_CoordDep_Automate_ABC_AssurerLaCoordinationInterne_();
]]--[[//-----------------------------------------------------------------------------
]]--[[// Gestion appuis et releve et SE
]]--[[//-----------------------------------------------------------------------------
]]
        Activate( self.activations.BEH_Dispositif_Automate_ABC_GererLeDispositifStatiqueSurLima_, 1, { true, self.LR, } )
        Activate( self.activations.BEH_Conduite_Automate_ABC_Recueillir_FaireDecrocherLePEcl, 1, emptyTable )--[[//-----------------------------------------------------------------------------
]]--[[// Gestion des unites sur la LIA
]]--[[//-----------------------------------------------------------------------------
]]
        --[[//-----------------------------------------------------------------------------
]]--[[// Condition opérationnelle
]]--[[//-----------------------------------------------------------------------------
]]Activate( self.activations.BEH_RC_Automate_GererConditionOperationnelle, 1, { eEtatEchelon_Eclairage, } )
    end,

