connections[ #connections + 1 ] = { nodes.MIS_Pion_Decrocher_Inf, "BEH_Lima_Pion_INF_GererLIA", nodes.BEH_Lima_Pion_INF_GererLIA }
connections[ #connections + 1 ] = { nodes.MIS_Pion_Decrocher_Inf, "BEH_Manoeuvre_RompreContact", nodes.BEH_Manoeuvre_RompreContact }
