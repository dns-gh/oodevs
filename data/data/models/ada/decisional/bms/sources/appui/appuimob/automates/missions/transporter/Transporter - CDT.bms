connections[ #connections + 1 ] = { nodes.MIS_Automate_CDT_LOG_TransporterUnites, "BEH_Ordre_Automate_RecevoirOrdreInopine", nodes.BEH_Ordre_Automate_RecevoirOrdreInopine }
