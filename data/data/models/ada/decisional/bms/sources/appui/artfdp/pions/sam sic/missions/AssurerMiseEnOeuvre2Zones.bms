connections[ #connections + 1 ] = { nodes.MIS_Pion_ASS_AssurerMiseEnOeuvre2Zones, "ACT_Pion_ASS_DebarquerLanceur", nodes.ACT_Pion_ASS_DebarquerLanceur }
