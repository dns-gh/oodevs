--[[// -----------------------------------------------------------------------------
]]--[[// Comportement par défaut.
]]--[[//
]]--[[// Commentaires: Les helicopteres sont tous posés pour au debut. Ils n'ont pas
]]--[[// d'objectif ni de plot de rafitaillement
]]--[[//
]]--[[// toutes les actions et les comportements par défaut sont activé avec une force 
]]--[[// maximale de 0.5
]]--[[// -----------------------------------------------------------------------------
]]node "BEH_Defaut"
{
    activations =
    {
        { "BEH_MAJ_EtatDefaut", { "onHalt" } },
        { "BEH_Mot_Pion_ALAT_AffecterVariablesDEtat", { "onHalt" } },
        { "BEH_ALAT_Ambiance", { "onHalt" } },
        { "BEH_Nbc", { "onHalt" } },
        { "BEH_ROE", { "onHalt" } },
        { "BEH_Rens", { "onHalt" } },
        { "BEH_Ordre_Pion_ALAT_ChangerParametresRecompletement", { "onHalt" } },
        { "BEH_Info_Pion_RendreCompte", { "onHalt" } },
        { "BEH_Dep_Pion_ALAT_SePoser", { "onHalt" } },
        { "BEH_Log_Pion_ALAT_Recompletement", { "onHalt" } },
        { "BEH_Lima_GererLimas", { "onHalt" } },
        { "BEH_Manoeuvre_Pion_SeCoordonner", { "onHalt" } },
        { "BEH_Manoeuvre_Pion_GestionDestruction", { "onHalt" } },
        { "BEH_Log_Pion_ALAT_AjoutNouveauPlotsDansFuseau", { "onHalt" } },
        { "BEH_Log_Pion_ALAT_GererPlotsRavitaillement", { "onHalt" } },
        { "BEH_Eni_Pion_ALAT_EviterContactsAvecEnnemisConnus", { "onHalt" } },
    },

    onHalt = OnHalt,

    feedbacks = { { { "onHalt" }, "BEH_Defaut" } },

    create = function( self )
        self._namedParams = {}
        self._namedParams.nDummy = self.params[1]
    end,

    activate = function( self )
        if( ModuleBegins() ) then
            do
                --[[//initialisation des atributs de la patrouille ALAT
]]F_Pion_SetobjectifEsquive( myself, nil )
                myself.objectifCourant_ = nil
                myself.bEnPhaseRavitaillement_ = false
                myself.rNiveauAlerteRavitaillement_ = 0
                myself.bMoteursEteints_ = true
                myself.bEnCoursExtractionPersonnel_ = false
                myself.bDemandeOrdreConduitePoursuivre_ = false
                --[[//initialisation de la position de regroupement avec notre position 
]]--[[//initiale        
]]myself.pointRegroupement_ = DEC_Geometrie_CreerPoint()
                F_Pion_SeteEtatEchelon( myself, eEtatEchelon_Premier )
                DEC_Copie_Point( myself:DEC_Agent_Position(), myself.pointRegroupement_ )
                --[[// Activation de l'écoute radar
]]DEC_Perception_ActiverRadar( eRadarType_EcouteRadar )
                --[[// Initialisation des paramètres de recompletement
]]myself.porteeAction_ = eMoyennePortee
                myself.ambianceMission_ = ePolyvalent
            end
--[[// MAJ par défaut
]]        end

        Activate( self.activations.BEH_MAJ_EtatDefaut, 1, {} )
        Activate( self.activations.BEH_Mot_Pion_ALAT_AffecterVariablesDEtat, 1, {} )
        Activate( self.activations.BEH_ALAT_Ambiance, 1, {} )
        Activate( self.activations.BEH_Nbc, 1, {} )
        Activate( self.activations.BEH_ROE, 1, {} )
        Activate( self.activations.BEH_Rens, 1, {} )
        Activate( self.activations.BEH_Ordre_Pion_ALAT_ChangerParametresRecompletement, 1, {} )
        Activate( self.activations.BEH_Info_Pion_RendreCompte, 1, {} )
        myself.plotsRavitaillement_ = S_Obj_ListeObjetsValides( myself.plotsRavitaillement_ )
        if( S_Misc_EstDetruitPhysique() ) then
            return
        end

        Activate( self.activations.BEH_Dep_Pion_ALAT_SePoser, 0.5, {} )
        if( myself.bEnPhaseRavitaillement_ ) then
            Activate( self.activations.BEH_Log_Pion_ALAT_Recompletement, 100000, {} )
        end

        Activate( self.activations.BEH_Lima_GererLimas, 1, {} )--[[// Gestion automatique du vol furtif
]]
        --[[// coordination du feu    	
]]if( not DEC_Agent_AutomateEstEmbraye() ) then
            SetStateVariable( "VE_Coordination_Tir", 1 )
        else
            Activate( self.activations.BEH_Manoeuvre_Pion_SeCoordonner, 100, {} )
        end

        --[[// gestion de la déstruction tactique
]]Activate( self.activations.BEH_Manoeuvre_Pion_GestionDestruction, 10000, {} )
        --[[// ---------------------------------------------------------------------
]]--[[// choix du plot de ravitaillement si l'automate n'a pas de mission
]]--[[//
]]--[[// Contrôle d'alerte ravitaillement ( Cf. Motivations sur Temps de Vol )
]]--[[// ---------------------------------------------------------------------
]]local automate = DEC_GetAutomate( myself )
        if( ( DEC_GetMission( automate ) == 0 or DEC_GetMission( automate ) == nil ) ) then
            do
                Activate( self.activations.BEH_Log_Pion_ALAT_AjoutNouveauPlotsDansFuseau, 1, {} )
                Activate( self.activations.BEH_Log_Pion_ALAT_GererPlotsRavitaillement, 1, {} )
            end
--[[// implémentation du pathfind adaptatif
]]--[[// ------------------------------------
]]        end

        Activate( self.activations.BEH_Eni_Pion_ALAT_EviterContactsAvecEnnemisConnus, 1, {} )
    end,

--[[//------------------------------------------------------------------------------
]]--[[// BEH_Dep_Virtuel_ProgressionVers
]]--[[//------------------------------------------------------------------------------
]]}

node "BEH_Dep_Virtuel_ProgressionVers"
{
    activations =
    {
        { "BEH_Dep_Pion_ALAT_RejoindrePoint", { "onHalt" } },
    },

    onHalt = OnHalt,

    feedbacks = { { { "onHalt" }, "BEH_Dep_Virtuel_ProgressionVers" } },

    create = function( self )
        self._namedParams = {}
        self._namedParams.point = self.params[1]
        self._namedParams.eTypeProgression = self.params[2]
        self._namedParams.eTypeIti = self.params[3]
    end,

    activate = function( self )
        Activate( self.activations.BEH_Dep_Pion_ALAT_RejoindrePoint, 1, { self._namedParams.point, self._namedParams.eTypeProgression, self._namedParams.eTypeIti, } )
    end,

--[[//------------------------------------------------------------------------------
]]--[[// BEH_Dep_Virtuel_PreparationAction
]]--[[//------------------------------------------------------------------------------
]]}

node "BEH_Dep_Virtuel_PreparationAction"
{
    activations =
    {
        { "BEH_Dep_Pion_ALAT_Voler", { "onHalt" } },
    },

    onHalt = OnHalt,

    feedbacks = { { { "onHalt" }, "BEH_Dep_Virtuel_PreparationAction" } },

    create = function( self )
    end,

    activate = function( self )
        Activate( self.activations.BEH_Dep_Pion_ALAT_Voler, 1, {} )--[[//Gestion du vol pour les hélicopteres
]]
                self.params.__returnValue = eActionEffectuee
        return

    end,

}

