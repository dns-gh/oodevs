connections[ #connections + 1 ] = { nodes.MIS_Automate_MRT_ASS_RecoEtDeploiement, "BEH_Medo_Automate_PreparerMission_RecoEtDeploiement", nodes.BEH_Medo_Automate_PreparerMission_RecoEtDeploiement }
