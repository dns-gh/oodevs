connections[ #connections + 1 ] = { nodes.MIS_Pion_LOG_ReconnaitreZoneContaminee, "BEH_Dep_ProgressionVers", nodes.BEH_Dep_ProgressionVers }
connections[ #connections + 1 ] = { nodes.MIS_Pion_LOG_ReconnaitreZoneContaminee, "BEH_LOG_Pion_FaireDesMesures", nodes.BEH_LOG_Pion_FaireDesMesures }
connections[ #connections + 1 ] = { nodes.MIS_Pion_LOG_ReconnaitreZoneContaminee, "BEH_Dep_SePosterFaceADirectionDangereuse", nodes.BEH_Dep_SePosterFaceADirectionDangereuse }
