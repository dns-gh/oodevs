connections[ #connections + 1 ] = { nodes.BEH_Medo_Automate_RENS_ROHUM_AssignerSExfiltrerA, "BEH_Mission_Automate_RENS_ROHUM_AssignerSExfiltrerA", nodes.BEH_Mission_Automate_RENS_ROHUM_AssignerSExfiltrerA }
