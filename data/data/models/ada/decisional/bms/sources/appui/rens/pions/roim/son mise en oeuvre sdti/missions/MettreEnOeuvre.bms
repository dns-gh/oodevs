includedFiles = includedFiles or {}
includedFiles["sources/appui/rens/pions/roim/son mise en oeuvre sdti/missions/MettreEnOeuvre.bms"] = true

--[[// *****************************************************************************
]]--[[// MIS_Pion_RENS_ROIM_MettreEnOeuvre
]]--[[//
]]--[[// Commentaires: 
]]--[[//
]]--[[// *****************************************************************************
]]node "MIS_Pion_RENS_ROIM_MettreEnOeuvre"
{
    feedbacks = { { { "done_MIS_Pion_RENS_ROIM_MettreEnOeuvre" }, "MIS_Pion_RENS_ROIM_MettreEnOeuvre" } },
    SendFeedback = function( self, value ) Feedback( self.feedbacks.done_MIS_Pion_RENS_ROIM_MettreEnOeuvre, {value} ) end,
    activations =
    {
        { "BEH_Mission_Pion_RENS_MettreEnOeuvre", { "done_BEH_Mission_Pion_RENS_MettreEnOeuvre" } },
    },

    create = function( self )
        self._namedParams = {}
        self._namedParams.mission = self.params.mission
    end,

    activate = function( self )
        local Activate = Activate
        self.eEtat = self.eEtat or nil
        --[[//-----------------------------------------------------------------------------
]]--[[// Terminaison
]]--[[//-----------------------------------------------------------------------------
]]if( self.eEtat == eActionImpossible ) then
            self:SendFeedback( eActionImpossible )
            Halt( self )
            do return end
        end

        if( self.eEtat == eActionEffectuee ) then
            self:SendFeedback( eActionEffectuee )
            Halt( self )
            do return end
        end

        --[[//-----------------------------------------------------------------------------
]]--[[// Furtivite
]]--[[//-----------------------------------------------------------------------------
]]--[[// action ACT_Dep_Pion_Rens_ActiverFurtivite( 0 );
]]--[[//-----------------------------------------------------------------------------
]]--[[// Comportement de la mission
]]--[[//-----------------------------------------------------------------------------
]]Activate( self.activations.BEH_Mission_Pion_RENS_MettreEnOeuvre, 1, { self._namedParams.mission.positionDeploiement_, } )
self.done_BEH_Mission_Pion_RENS_MettreEnOeuvre = function( self, _, v ) self.eEtat = v[1] end
    end,

}

eventmanager_plugin = eventmanager_plugin or {}
eventmanager_plugin.MIS_Pion_RENS_ROIM_MettreEnOeuvre = { "mission" }
connections = connections or {}
connections[ #connections + 1 ] = { nodes.MIS_Pion_RENS_ROIM_MettreEnOeuvre, "BEH_Mission_Pion_RENS_MettreEnOeuvre", nodes.BEH_Mission_Pion_RENS_MettreEnOeuvre }
