includedFiles = includedFiles or {}
includedFiles["sources/appui/artdsa/automates/sgpt satcp mistral/missions/surveiller/Surveiller - CDT.bms"] = true

--[[// -----------------------------------------------------------------------------
]]--[[// Conduite de la mission Automate ASA Surveiller
]]--[[// -----------------------------------------------------------------------------
]]node "MIS_Automate_CDT_ASA_MISTRAL_Surveiller"
{
    activations =
    {
        { "BEH_Medo_Automate_ASA_MISTRAL_AssignerMissionsSurveillerAListe", {} },
    },

    create = function( self )
        self._namedParams = {}
        self._namedParams.mission = self.params.mission
    end,

    activate = function( self )
        self.barycentreLocalisation = self.barycentreLocalisation or S_Geometrie_BarycentreZone( self._namedParams.mission.zone_ )
        self.pointDevantDispositif = self.pointDevantDispositif or DEC_Geometrie_PositionTranslateDir( self.barycentreLocalisation, DEC_GetDirectionEnnemi( self._namedParams.mission ), M_POLY_DOCTRINE_DISTANCE_MAX_ENTRE_SECTIONS() )
        self.listePositionsPourSectionsRestantes = self.listePositionsPourSectionsRestantes or DEC_Geometrie_CreerListePoints()
        self.pionPC = self.pionPC or DEC_Automate_PionPC()
        self.selPions = self.selPions or {}
        self.listeSurveiller = self.listeSurveiller or {}
        self.listeSectionsRestantes = self.listeSectionsRestantes or {}
        DIA_Clear( self.selPions )
        self.selPions = DEC_Automate_PionsSansPC()
        DIA_Clear( self.listeSurveiller )
        self.listeSurveiller = S_Cherche_Automate_Filtre_TypeMission( self.selPions, "T_Mission_Pion_ASA_MISTRAL_Surveiller" )
        DIA_Clear( self.listeSectionsRestantes )
        self.listeSectionsRestantes = S_Cherche_Automate_Filtre_PionNonDansListe( self.selPions, self.listeSurveiller )
        if( ModuleBegins() ) then
            do
                switch_1 = --[[//gestion des sections mistral qui n'ont pas de mission defendre Site
]]self._namedParams.mission.modeDeploiement_
                    if switch_1 == deploiementNasseTroisSections
                    or switch_1 == deploiementSimpleRideau
                    or switch_1 == deploiementNasseQuatreSections
                    or switch_1 == deploiementDoubleRideau then
                                            DEC_ListePoints_PushBack( self.listePositionsPourSectionsRestantes, self.barycentreLocalisation )
                        
                    elseif switch_1 == deploiementOmnidirectionnelEnCarreAutourZone
                    or switch_1 == deploiementOmnidirectionnelEnTriangleAutourZone then
                                            DEC_ListePoints_PushBack( self.listePositionsPourSectionsRestantes, self.pointDevantDispositif )
                        
                                        end

            end
        end

        if( not DIA_IsListEmpty( self.listeSectionsRestantes ) ) then
            Activate( self.activations.BEH_Medo_Automate_ASA_MISTRAL_AssignerMissionsSurveillerAListe, 1, { ePhase_CDT, self.listeSectionsRestantes, self.listePositionsPourSectionsRestantes, eEtatDeploiement_omnidirectionnel, } )
        end

    end,

}

eventmanager_plugin = eventmanager_plugin or {}
eventmanager_plugin.MIS_Automate_CDT_ASA_MISTRAL_Surveiller = { "mission" }
connections = connections or {}
connections[ #connections + 1 ] = { nodes.MIS_Automate_CDT_ASA_MISTRAL_Surveiller, "BEH_Medo_Automate_ASA_MISTRAL_AssignerMissionsSurveillerAListe", nodes.BEH_Medo_Automate_ASA_MISTRAL_AssignerMissionsSurveillerAListe }
