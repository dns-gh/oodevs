--[[// *****************************************************************************
]]--[[// MIS_Automate_CDT_RENS_ROIM_CL289_RenseignerSur
]]--[[// 
]]--[[// Commentaires:
]]--[[//
]]--[[// *****************************************************************************
]]node "MIS_Automate_CDT_RENS_ROIM_CL289_RenseignerSur"
{
    feedbacks = { { { "done_MIS_Automate_CDT_RENS_ROIM_CL289_RenseignerSur" }, "MIS_Automate_CDT_RENS_ROIM_CL289_RenseignerSur" } },
    SendFeedback = function( self, value ) Feedback( self.feedbacks.done_MIS_Automate_CDT_RENS_ROIM_CL289_RenseignerSur, {value} ) end,
    activations =
    {
        { "BEH_Dispositif_Automate_RENS_GererPionsDetruits_", { "done_BEH_Dispositif_Automate_RENS_GererPionsDetruits_" } },
        { "BEH_Dispositif_Automate_RENS_GererLeSE_", { "done_BEH_Dispositif_Automate_RENS_GererLeSE_" } },
        { "BEH_Conduite_Automate_RENS_ROIM_CL289_GererMiseEnOeuvre", { "done_BEH_Conduite_Automate_RENS_ROIM_CL289_GererMiseEnOeuvre" } },
    },

    create = function( self )
        self._namedParams = {}
        self._namedParams.mission = self.params.mission
    end,

    activate = function( self )
        --[[//-----------------------------------------------------------------------------
]]--[[// Init
]]--[[//-----------------------------------------------------------------------------
]]if( ModuleBegins() ) then
            DEC_SetMission( myself, self._namedParams.mission )
        end

        Activate( self.activations.BEH_Dispositif_Automate_RENS_GererPionsDetruits_, 1, {} )--[[//-----------------------------------------------------------------------------
]]--[[// Gestion 
]]--[[//-----------------------------------------------------------------------------
]]
        Activate( self.activations.BEH_Dispositif_Automate_RENS_GererLeSE_, 1, {} )
        Activate( self.activations.BEH_Conduite_Automate_RENS_ROIM_CL289_GererMiseEnOeuvre, 1, {} )--[[//-----------------------------------------------------------------------------
]]--[[// Gestion du drone
]]--[[//----------------------------------------------------------------------------- 
]]
    end,

--[[// *****************************************************************************
]]--[[// BEH_Conduite_Automate_RENS_ROIM_CL289_GererMiseEnOeuvre
]]--[[//
]]--[[// Commentaires:
]]--[[//
]]--[[// *****************************************************************************
]]}

node "BEH_Conduite_Automate_RENS_ROIM_CL289_GererMiseEnOeuvre"
{
    feedbacks = { { { "done_BEH_Conduite_Automate_RENS_ROIM_CL289_GererMiseEnOeuvre" }, "BEH_Conduite_Automate_RENS_ROIM_CL289_GererMiseEnOeuvre" } },
    SendFeedback = function( self, value ) Feedback( self.feedbacks.done_BEH_Conduite_Automate_RENS_ROIM_CL289_GererMiseEnOeuvre, {value} ) end,
    activations =
    {
        { "ACT_Info_Trace", { "done_ACT_Info_Trace" } },
        { "BEH_Conduite_Automate_RENS_ROIM_CL289_LancerDrone", { "done_BEH_Conduite_Automate_RENS_ROIM_CL289_LancerDrone" } },
    },

    create = function( self )
    end,

    activate = function( self )
        self.bLancementEffectue = self.bLancementEffectue or false
        local pionDrone = S_Dispositif_Automate_RENS_ObtenirPionDroneMiseEnOeuvre( "Drone_CL289" )
        --[[//-----------------------------------------------------------------------------
]]--[[// Si pas de drone disponible, attendre
]]--[[//----------------------------------------------------------------------------- 
]]if( ( pionDrone == 0 or pionDrone == nil ) ) then
            do
                Activate( self.activations.ACT_Info_Trace, 1, { "Waiting for drone to be operated", } )
                                do return end

            end
--[[//-----------------------------------------------------------------------------
]]--[[// Lancer un seul drone
]]--[[//-----------------------------------------------------------------------------
]]        end

        if( not self.bLancementEffectue ) then
            do
                StartActivateOverride( self, self.activations.BEH_Conduite_Automate_RENS_ROIM_CL289_LancerDrone, 1, { pionDrone, } )
                self.bLancementEffectue = true
            end
        end

    end,

--[[// *****************************************************************************
]]--[[// BEH_Conduite_Automate_RENS_ROIM_CL289_LancerDrone
]]--[[//
]]--[[// Commentaires:
]]--[[//
]]--[[// *****************************************************************************
]]}

node "BEH_Conduite_Automate_RENS_ROIM_CL289_LancerDrone"
{
    feedbacks = { { { "done_BEH_Conduite_Automate_RENS_ROIM_CL289_LancerDrone" }, "BEH_Conduite_Automate_RENS_ROIM_CL289_LancerDrone" } },
    SendFeedback = function( self, value ) Feedback( self.feedbacks.done_BEH_Conduite_Automate_RENS_ROIM_CL289_LancerDrone, {value} ) end,
    activations =
    {
        { "ACT_RC_Automate", { "done_ACT_RC_Automate" } },
        { "BEH_Mission_Automate_RENS_ROIM_CL289_AssignerRenseignerSur", { "done_BEH_Mission_Automate_RENS_ROIM_CL289_AssignerRenseignerSur" } },
    },

    create = function( self )
        self._namedParams = {}
        self._namedParams.pionDrone = self.params[1]
    end,

    activate = function( self )
        --[[//-----------------------------------------------------------------------------
]]--[[// Assigner mission au drone
]]--[[//-----------------------------------------------------------------------------
]]if( ModuleBegins() ) then
            do
                Activate( self.activations.ACT_RC_Automate, 1, { eRC_DroneLance, } )
                local mission = DEC_GetMission( myself )
                Activate( self.activations.BEH_Mission_Automate_RENS_ROIM_CL289_AssignerRenseignerSur, 1, { ePhase_CDT, self._namedParams.pionDrone, mission.ficelleDeVole_, mission.zonesDeRecherche_, eEtatEchelon_Premier, } )
            end
        end

    end,

}

eventmanager_plugin = eventmanager_plugin or {}
eventmanager_plugin.MIS_Automate_CDT_RENS_ROIM_CL289_RenseignerSur = { "mission" }
