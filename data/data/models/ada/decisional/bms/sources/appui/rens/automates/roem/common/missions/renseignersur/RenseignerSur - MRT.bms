]]node "BEH_Medo_Automate_RENS_ROEM_AssignerRenseignerA"
{
    activations =
    {
        { "BEH_Mission_Automate_RENS_ROEM_AssignerRenseignerA", { "onHalt" } },
    },

    onHalt = OnHalt,

    feedbacks = { { { "onHalt" }, "BEH_Medo_Automate_RENS_ROEM_AssignerRenseignerA" } },

    create = function( self )
        self._namedParams = {}
        self._namedParams.pion = self.params[1]
    end,

    activate = function( self )
        self.mission = self.mission or DEC_GetMission( myself )
        Activate( self.activations.BEH_Mission_Automate_RENS_ROEM_AssignerRenseignerA, 1, { ePhase_MRT, self._namedParams.pion, self.mission.zone_, eEtatEchelon_Premier, } )
    end,

