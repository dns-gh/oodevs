includedFiles["sources/appui/rens/pions/radint/pat viviane/missions/RenseignerSur.bms"] = true

local emptyTable = emptyTable

--[[// *****************************************************************************
]]--[[// MIS_Pion_RENS_RADINT_RenseignerSur
]]--[[//
]]--[[// Commentaires: //TEMP normalement  MIS_Pion_RENS_Radint_RenseignerSur
]]--[[//
]]--[[// *****************************************************************************
]]node "MIS_Pion_RENS_RADINT_RenseignerSur"
{
    feedbacks = { { { "done_MIS_Pion_RENS_RADINT_RenseignerSur" }, "MIS_Pion_RENS_RADINT_RenseignerSur" } },
    SendFeedback = function( self, value ) Feedback( self.feedbacks.done_MIS_Pion_RENS_RADINT_RenseignerSur, {value} ) end,
    activations =
    {
        { "BEH_Ordre_Inopine", { "done_BEH_Ordre_Inopine" } },
        { "ACT_Dep_Pion_RENS_VolerA", { "done_ACT_Dep_Pion_RENS_VolerA" } },
        { "ACT_Info_Trace", { "done_ACT_Info_Trace" } },
        { "ACT_Connaissance_Rens_EnregistrerDonnees", { "done_ACT_Connaissance_Rens_EnregistrerDonnees" } },
        { "ACT_MAJ_EtatPhaseMission", { "done_ACT_MAJ_EtatPhaseMission" } },
        { "BEH_Obs_Pion_Rens_Radint_RenseignerDans", { "done_BEH_Obs_Pion_Rens_Radint_RenseignerDans" } },
        { "ACT_Dep_SePoster", { "done_ACT_Dep_SePoster" } },
        { "BEH_Dep_Pion_RENS_MonterA", { "done_BEH_Dep_Pion_RENS_MonterA" } },
        { "ACT_Misc_Delai", { "done_ACT_Misc_Delai" } },
        { "BEH_Dep_ProgressionVers", { "done_BEH_Dep_ProgressionVers" } },
        { "BEH_Connaissance_Rens_ExploiterRens", { "done_BEH_Connaissance_Rens_ExploiterRens" } },
    },

    create = function( self )
        self._namedParams = {}
        self._namedParams.mission = self.params.mission
    end,

    activate = function( self )
        local Activate = Activate
        self.eEtat = self.eEtat or eActionEnCours
        self.ePhaseMission = self.ePhaseMission or ePreparerMission
        self.positionInitiale = self.positionInitiale or DEC_Geometrie_CreerPoint()
        self.positionObservation = self.positionObservation or DEC_Geometrie_CreerPoint()
        if( #( self._namedParams.mission.zonesDeRecherche_ ) ~= 0 ) then
            self.positionObservation = S_Geometrie_BarycentreZone( DEC_UserTypeList_GetAt( self._namedParams.mission.zonesDeRecherche_, 0 ) )
        else
do
                DEC_RC( eRC_MissionImpossible )
                                Halt( self )
                do return end

            end
        end

        self.eEtatOrdreFinMission = self.eEtatOrdreFinMission or eActionEnCours
        Activate( self.activations.BEH_Ordre_Inopine, 1, emptyTable )
self.done_BEH_Ordre_Inopine = function( self, _, v ) self.eEtatOrdreFinMission = v[1] end
        if( self.eEtatOrdreFinMission == eActionHalt ) then
            do
                self.ePhaseMission = eFinMission
                self.eEtat = eActionEnCours
                self.eEtatOrdreFinMission = eActionEnCours
            end
        end

        if( ModuleBegins() ) then
            do
                StartActivateOverride( self, self.activations.ACT_Dep_Pion_RENS_VolerA, 1, { 100, } )
                --[[// données doctrine
]]DEC_Copie_Point( DEC_Agent_Position(), self.positionInitiale )
                Activate( self.activations.ACT_Info_Trace, 1, { "Intelligence gathering mission begining", } )
                Activate( self.activations.ACT_Connaissance_Rens_EnregistrerDonnees, 1, emptyTable )--[[// Enregistrement des donnes
]]
            end
        end

         do
            local _continue = true
--[[// Preparation mission
]]--[[// Se diriger vers les ZR 
]]--[[// Observation
]]--[[// Prendre des photos pd 5 min
]]--[[// Rester sur palce dans la cas du Cougar
]]--[[// données doctrine
]]--[[// Delai mise en place
]]--[[// Progression
]]--[[// Decrocher
]]--[[// Fin de la mission Sur ordre ou si plus d'autonomie
]]--[[// Developpement photos
]]--[[// données doctrine
]]            local switch_1 = self.ePhaseMission
                if switch_1 == ePreparerMission then
                    Activate( self.activations.ACT_MAJ_EtatPhaseMission, 1, { eEtatPhaseMission_DebutMission, } )
                    self.ePhaseMission = eProgresser
                    _continue = false
                elseif switch_1 == eProgresser then
                    Activate( self.activations.ACT_MAJ_EtatPhaseMission, 1, { eEtatPhaseMission_ProgressionVersLocalisation, } )
                    if( self.eEtat == eActionEffectuee ) then
                        do
                            self.eEtat = eActionEnCours
                                                        do
                                self.ePhaseMission = eRejoindre
                                                                do return end

                            end

                        end
                    end

                    if( DEC_Perception_PointEstVisible( self.positionObservation ) ) then
                        do
                            Activate( self.activations.BEH_Obs_Pion_Rens_Radint_RenseignerDans, 1, { self._namedParams.mission.zonesDeRecherche_, } )
                            Activate( self.activations.ACT_Info_Trace, 1, { "Vertical take off", } )
                            Activate( self.activations.ACT_Dep_SePoster, 1.1, emptyTable )
                            Activate( self.activations.BEH_Dep_Pion_RENS_MonterA, 1.1, { 3000, } )
                            Activate( self.activations.ACT_Misc_Delai, 1, { 1, 5, } )
self.done_ACT_Misc_Delai = function( self, _, v ) self.eEtat = v[1] end
                        end
                    end

                    Activate( self.activations.BEH_Dep_ProgressionVers, 1, { self.positionObservation, eProgressionDirect, eTypeItiMouvement, } )
                    _continue = false
                elseif switch_1 == eRejoindre then
                    Activate( self.activations.ACT_MAJ_EtatPhaseMission, 1, { eEtatPhaseMission_Decroche, } )
                    if( self.eEtat == eActionEffectuee ) then
                        do
                            self.eEtat = eActionEnCours
                                                        do
                                self.ePhaseMission = eFinMission
                                                                do return end

                            end

                        end
                    end

                    Activate( self.activations.BEH_Dep_ProgressionVers, 1, { self.positionInitiale, eProgressionDirect, eTypeItiMouvement, } )
self.done_BEH_Dep_ProgressionVers = function( self, _, v ) self.eEtat = v[1] end
                    _continue = false
                elseif switch_1 == eFinMission then
                    if( self.eEtat == eActionEffectuee ) then
                        Activate( self.activations.ACT_MAJ_EtatPhaseMission, 1, { eEtatPhaseMission_FinMission, } )
                    end

                    Activate( self.activations.BEH_Connaissance_Rens_ExploiterRens, 1, emptyTable )
self.done_BEH_Connaissance_Rens_ExploiterRens = function( self, _, v ) self.eEtat = v[1] end
                    StopActivateOverride( self, self.activations.ACT_Dep_Pion_RENS_VolerA, { 100, } )
                    _continue = false
                
                else
                                end
            end

    end,

--[[// *****************************************************************************
]]--[[// BEH_Connaissance_Rens_ExploiterRens
]]--[[//
]]--[[// Commentaires:
]]--[[//
]]--[[// *****************************************************************************
]]}

node "BEH_Connaissance_Rens_ExploiterRens"
{
    feedbacks = { { { "done_BEH_Connaissance_Rens_ExploiterRens" }, "BEH_Connaissance_Rens_ExploiterRens" } },
    SendFeedback = function( self, value ) Feedback( self.feedbacks.done_BEH_Connaissance_Rens_ExploiterRens, {value} ) end,
    activations =
    {
        { "ACT_Info_Trace", { "done_ACT_Info_Trace" } },
        { "ACT_Connaissance_Rens_ExtraireDonnees", { "done_ACT_Connaissance_Rens_ExtraireDonnees" } },
        { "ACT_Connaissance_Rens_ExploiterDonnees", { "done_ACT_Connaissance_Rens_ExploiterDonnees" } },
    },

    create = function( self )
    end,

    activate = function( self )
        local Activate = Activate
        self.eEtat = self.eEtat or eActionEnCours
        if( self.eEtat == eActionEffectuee ) then
            do
                Activate( self.activations.ACT_Info_Trace, 1, { "End of data processing", } )
                Activate( self.activations.ACT_Connaissance_Rens_ExtraireDonnees, 1, emptyTable )
                                self:SendFeedback( eActionEffectuee )
                do return end

            end
--[[// Exploitation des donnees
]]--[[// 30 min délais doctrine
]]        end

        Activate( self.activations.ACT_Connaissance_Rens_ExploiterDonnees, 1, { 30, } )
self.done_ACT_Connaissance_Rens_ExploiterDonnees = function( self, _, v ) self.eEtat = v[1] end
    end,

--[[// *****************************************************************************
]]--[[// BEH_Obs_Pion_Rens_Radint_RenseignerDans
]]--[[//
]]--[[// Commentaires:
]]--[[//
]]--[[// *****************************************************************************
]]}

node "BEH_Obs_Pion_Rens_Radint_RenseignerDans"
{
    feedbacks = { { { "done_BEH_Obs_Pion_Rens_Radint_RenseignerDans" }, "BEH_Obs_Pion_Rens_Radint_RenseignerDans" } },
    SendFeedback = function( self, value ) Feedback( self.feedbacks.done_BEH_Obs_Pion_Rens_Radint_RenseignerDans, {value} ) end,
    activations =
    {
        { "ACT_Obs_SurveillerZone", { "done_ACT_Obs_SurveillerZone" } },
    },

    create = function( self )
        self._namedParams = {}
        self._namedParams.zonesDeRecherches = self.params[1]
    end,

    activate = function( self )
        local Activate = Activate
        for _,zone in pairs( --[[//---------------------------------------------------------------------
]]--[[// Observation ( si station => exploitation du Rens possible)
]]--[[//---------------------------------------------------------------------
]]self._namedParams.zonesDeRecherches or emptyTable ) do
        local _continue = true
            Activate( self.activations.ACT_Obs_SurveillerZone, 1, { zone, } )

        end

    end,

}

