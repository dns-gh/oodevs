--[[//-----------------------------------------------------------------------------
]]--[[// Fonctions de scripts communes aux SMO
]]--[[//-----------------------------------------------------------------------------
]]--[[//-----------------------------------------------------------------------------
]]--[[// Obtenir la station SMO
]]--[[//-----------------------------------------------------------------------------
]]function S_Misc_Pion_Rens_ObtenirStationAssignes( )
    local _returnValue = {}
    local selTemp = {}
    local listePions = DEC_Pion_PionsAvecPC()
    for _,x in pairs( listePions or emptyTable ) do
    local _continue = true
do
            local pion = x
            if( pion:GetType() == "Stations_Horizon" and F_Pion_GeteEtatDestruction( pion ) == eEtatDestruction_None ) then
                DIA_PushFront( selTemp, pion )
            end

        end

    end

    _returnValue = selTemp
    do return _returnValue end
end

