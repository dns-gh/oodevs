includedFiles = includedFiles or {}
includedFiles["sources/appui/artdsa/automates/common/Behaviors.bms"] = true

--[[//génériques automates scipio
]]include "Common/Automates/Behaviors.bms"
include "Common/Automates/Behaviors/Medo/Behaviors - Medo - ASA.bms"


--[[// *****************************************************************************
]]--[[// Comportement par défaut.
]]--[[//
]]--[[// Commentaires: Les helicopteres sont tous posés pour au debut. Ils n'ont pas
]]--[[//d'objectif ni de plot de rafitaillement
]]--[[//
]]--[[// *****************************************************************************
]]node "BEH_Defaut"
{
    activations =
    {
        { "BEH_RC_Automate_Situation", { "onHalt" } },
        { "BEH_ROE_Automate_GererOrdre", { "onHalt" } },
        { "BEH_Nbc_Automate_GererAlerteNiv4", { "onHalt" } },
        { "BEH_Ordre_Automate_RecevoirOrdreInopine", { "onHalt" } },
        { "BEH_Rens_Automate_GererSilenceRadio", { "onHalt" } },
        { "BEH_Ordre_Automate_RecevoirOrdreSurSilenceRadar", { "onHalt" } },
        { "BEH_Automate_ASA_Defaut", { "onHalt" } },
    },

    onHalt = OnHalt,

    feedbacks = { { { "onHalt" }, "BEH_Defaut" } },

    create = function( self )
        self._namedParams = {}
        self._namedParams.nDummy = self.params[1]
    end,

    activate = function( self )
        Activate( self.activations.BEH_RC_Automate_Situation, 1, {} )--[[// ---------------------------------------------------------------------
]]--[[// CR automates sur eni et situation mission
]]--[[// ---------------------------------------------------------------------
]]
        --[[// -------------------------------------------------------------------------
]]--[[// si l'automate est pas embrayé
]]--[[// -------------------------------------------------------------------------
]]if( DEC_Automate_EstEmbraye() ) then
            do
                Activate( self.activations.BEH_ROE_Automate_GererOrdre, 1, {} )--[[//----------------------------------------------------------------------
]]--[[// ROE --> Tir libre, sur riposte, interdit
]]--[[//----------------------------------------------------------------------
]]
                Activate( self.activations.BEH_Nbc_Automate_GererAlerteNiv4, 1, {} )--[[// ---------------------------------------------------------------------
]]--[[// Mettre les combinaisons Nbc si alerte de niveau 4
]]--[[// ---------------------------------------------------------------------
]]
                Activate( self.activations.BEH_Ordre_Automate_RecevoirOrdreInopine, 1, {} )--[[// ---------------------------------------------------------------------
]]--[[// Gestion des ordres inopines
]]--[[// ---------------------------------------------------------------------
]]
                Activate( self.activations.BEH_Rens_Automate_GererSilenceRadio, 1, {} )--[[// ---------------------------------------------------------------------
]]--[[// Gestion de l'ordre de conduite Silence Radio/Radar
]]--[[// ---------------------------------------------------------------------
]]
                Activate( self.activations.BEH_Ordre_Automate_RecevoirOrdreSurSilenceRadar, 1, {} )
            end
--[[//appel au comportement par défaut spécifique aux automates de sous groupements
]]--[[//ASA
]]        end

        Activate( self.activations.BEH_Automate_ASA_Defaut, 1, {} )
    end,

}

connections = connections or {}
connections[ #connections + 1 ] = { nodes.BEH_Defaut, "BEH_RC_Automate_Situation", nodes.BEH_RC_Automate_Situation }
connections[ #connections + 1 ] = { nodes.BEH_Defaut, "BEH_ROE_Automate_GererOrdre", nodes.BEH_ROE_Automate_GererOrdre }
connections[ #connections + 1 ] = { nodes.BEH_Defaut, "BEH_Nbc_Automate_GererAlerteNiv4", nodes.BEH_Nbc_Automate_GererAlerteNiv4 }
connections[ #connections + 1 ] = { nodes.BEH_Defaut, "BEH_Ordre_Automate_RecevoirOrdreInopine", nodes.BEH_Ordre_Automate_RecevoirOrdreInopine }
connections[ #connections + 1 ] = { nodes.BEH_Defaut, "BEH_Rens_Automate_GererSilenceRadio", nodes.BEH_Rens_Automate_GererSilenceRadio }
connections[ #connections + 1 ] = { nodes.BEH_Defaut, "BEH_Ordre_Automate_RecevoirOrdreSurSilenceRadar", nodes.BEH_Ordre_Automate_RecevoirOrdreSurSilenceRadar }
connections[ #connections + 1 ] = { nodes.BEH_Defaut, "BEH_Automate_ASA_Defaut", nodes.BEH_Automate_ASA_Defaut }
