        local Activate = Activate
        if( ModuleBegins() ) then
            StartActivateOverride( self, self.activations.BEH_Mission_Automate_AssignerSuivreA, 1, { ePhase_CDT, self._namedParams.pPion, self._namedParams.pAAppuyer, } )
        end

    end,

}

