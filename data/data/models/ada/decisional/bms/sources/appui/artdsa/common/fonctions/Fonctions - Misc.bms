--[[// *************************************************************************
]]--[[// Fonction permétant de dire si un agent derive d'une section asa
]]--[[//
]]--[[// ATTENTION - A METTRE A JOUR SI ON AJOUTE UN NOUVEAU PION ASA
]]--[[// *************************************************************************
]]function S_EstSectionASA( agent )
    local _returnValue = false
    _returnValue = ( agent:GetType() == "VeryShortRangeSurfaceToAirBatteryCP" ) or ( agent:GetType() == "ShortRangeSurfaceToAirDefenseBatteryCP" ) or ( agent:GetType() == "TheaterHighAltitudeAeraDefenseCP" ) or ( agent:GetType() == "VeryShortRangeSurfaceToAirDefenseTroop" ) or ( agent:GetType() == "TheaterHighAltitudeDefenseTroop" ) or ( agent:GetType() == "ShortRangeSurfaceToAirDefenseTroop" )
    do return _returnValue end
end

function S_Liste_UnitesReconnues( listeConnaissances )
    local _returnValue = {}
    DIA_Clear( _returnValue )
    for _,x in pairs( listeConnaissances ) do
    local _continue = true
do
            if( S_Eni_EstNiveauPerceptionGTIA( x, eReconnu ) ) then
                DEC_UserTypeList_PushBack( _returnValue, x )
            end

        end

    end

    do return _returnValue end
end

