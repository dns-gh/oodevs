includedFiles = includedFiles or {}
includedFiles["sources/appui/alat/automates/common/missions/detruire neutraliser dans zone/Detruire Neutraliser dans Zone - MRT.bms"] = true

--[[// -----------------------------------------------------------------------------
]]--[[// Detruire Neutraliser dans zone :
]]--[[// -----------------------------------------------------------------------------
]]node "MIS_Automate_MRT_ALAT_DetruireNeutraliserDansZone"
{
    feedbacks = { { { "onHalt" }, "MIS_Automate_MRT_ALAT_DetruireNeutraliserDansZone" } },

    create = function( self )
        self._namedParams = {}
        self._namedParams.mission = self.params.mission
    end,

    activate = function( self )
        self.selPions = self.selPions or S_Cherche_Automate_PionsOperationnels( DEC_Automate_PionsSansPC() )
        self.listeHAC = self.listeHAC or S_Cherche_Automate_Filtre_TypePion( self.selPions, "AntitankHelicopterPatrol" )
        self.listeHAP = self.listeHAP or S_Cherche_Automate_Filtre_TypePion( self.selPions, "EscortAndCloseSupportHelicopterPatrol" )
        self.listeDetruire = self.listeDetruire or S_Liste_Concatene( self.listeHAC, self.listeHAP )
        if( ModuleBegins() ) then
            do
                myself.sPlotsDeRavitaillement_ = self._namedParams.mission.plotsRavitaillement_
                DEC_SetMission( myself, self._namedParams.mission )
                if( DIA_IsListEmpty( self.listeDetruire ) ) then
                    do
                        DEC_Warning( eRC_MissionImpossibleUnitesSubordonneesNonOperationnelles )
                                                self.params.__returnValue = eActionImpossible
                        Feedback( self.feedbacks["onHalt"], { "MIS_Automate_MRT_ALAT_DetruireNeutraliserDansZone" } )
Halt( self )
                        do return end

                    end
                end

            end
        end

        DEC_MRT_Valide()
                self.params.__returnValue = eActionEffectuee
        Feedback( self.feedbacks["onHalt"], { "MIS_Automate_MRT_ALAT_DetruireNeutraliserDansZone" } )
Halt( self )
        do return end

    end,

}

eventmanager_plugin = eventmanager_plugin or {}
eventmanager_plugin.MIS_Automate_MRT_ALAT_DetruireNeutraliserDansZone = { "mission" }
