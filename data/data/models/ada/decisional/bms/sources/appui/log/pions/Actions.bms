--[[// *****************************************************************************
]]--[[// ACT_Misc_Pion_LOG_Charger
]]--[[//
]]--[[// Commentaires:
]]--[[//
]]--[[// *****************************************************************************
]]node "ACT_Misc_Pion_LOG_Charger"
{
    feedbacks = { { { "done_ACT_Misc_Pion_LOG_Charger" }, "ACT_Misc_Pion_LOG_Charger" } },
    SendFeedback = function( self, value ) Feedback( self.feedbacks.done_ACT_Misc_Pion_LOG_Charger, {value} ) end,
    create = function( self )
    end,

    select = function( self )
                self.supplyAction = self.supplyAction or nil
        self.bSupplyActionReturnCode = self.bSupplyActionReturnCode or false
        --[[//------------------------------------------------------------------------
]]--[[// Init
]]--[[//------------------------------------------------------------------------
]]if( ModuleBegins() ) then
            do
                DEC_RC( eRC_Convoi_ChargementEnCours )
                self.supplyAction = DEC_Ravitaillement_Convoi_StartCharger(  )
                actionCallbacks[ self.supplyAction ] = function( arg ) self.bSupplyActionReturnCode = arg end

            end
        end

        if( self.bSupplyActionReturnCode ) then
            do
                DEC_RC( eRC_Convoi_ChargementEffectue )
                                self:SendFeedback( eActionEffectuee )
                do return end

            end
        end

                self:SendFeedback( eActionEnCours )
        do return end

    end,

    deselect = function( self )
            end,

    destroy = function( self )
                self.supplyAction = DEC_StopAction( self.supplyAction )
    end,

}

