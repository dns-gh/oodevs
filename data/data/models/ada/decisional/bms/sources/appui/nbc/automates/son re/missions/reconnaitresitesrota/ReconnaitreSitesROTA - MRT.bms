--[[// *****************************************************************************
]]--[[// MIS_Automate_MRT_NBC_ReconnaitreDesSitesROTA
]]--[[// 
]]--[[// Commentaires:
]]--[[//
]]--[[// *****************************************************************************
]]node "MIS_Automate_MRT_NBC_ReconnaitreDesSitesROTA"
{
    activations =
    {
        { "BEH_Medo_Automate_NBC_DonnerLesOrdresPourMission", {} },
    },

    create = function( self )
        self._namedParams = {}
        self._namedParams.mission = self.params.mission
    end,

    activate = function( self )
        --[[//-----------------------------------------------------------------------------
]]--[[// Init
]]--[[//-----------------------------------------------------------------------------
]]if( ModuleBegins() ) then
            do
                DEC_Trace( "$ MIS AUTO reco site ROTA: debut MRT $" )
                DEC_SetMission( myself, self._namedParams.mission )
                local listePionsSubordonnes = DEC_Automate_PionsSansPC()
                for _,x in pairs( listePionsSubordonnes ) do
                local _continue = true
                    do
                        local pion = x
                        F_Pion_SeteEtatEchelon( pion, eEtatEchelon_Second )
                        F_Pion_SeteEtatSoutien( pion, eEtatSoutien_aucun )
                    end

                end

            end
--[[//-----------------------------------------------------------------------------
]]--[[// Gen
]]--[[//-----------------------------------------------------------------------------
]]        end

        Activate( self.activations.BEH_Medo_Automate_NBC_DonnerLesOrdresPourMission, 1, { "T_Mission_Automate_NBC_ReconnaitreDesSitesROTA", } )
    end,

}

eventmanager_plugin = eventmanager_plugin or {}
eventmanager_plugin.MIS_Automate_MRT_NBC_ReconnaitreDesSitesROTA = { "mission" }
connections = connections or {}
connections[ #connections + 1 ] = { nodes.MIS_Automate_MRT_NBC_ReconnaitreDesSitesROTA, "BEH_Medo_Automate_NBC_DonnerLesOrdresPourMission", nodes.BEH_Medo_Automate_NBC_DonnerLesOrdresPourMission }
