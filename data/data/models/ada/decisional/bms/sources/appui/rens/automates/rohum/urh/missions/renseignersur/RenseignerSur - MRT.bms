includedFiles = includedFiles or {}
includedFiles["sources/appui/rens/automates/rohum/urh/missions/renseignersur/RenseignerSur - MRT.bms"] = true

--[[// *****************************************************************************
]]--[[// MIS_Automate_MRT_RENS_ROHUM_RenseignerSur
]]--[[// 
]]--[[// Commentaires:
]]--[[//
]]--[[// *****************************************************************************
]]node "MIS_Automate_MRT_RENS_ROHUM_RenseignerSur"
{
    activations =
    {
        { "BEH_Medo_Automate_RENS_ROHUM_AssignerMissions", {} },
    },

    create = function( self )
        self._namedParams = {}
        self._namedParams.mission = self.params.mission
    end,

    activate = function( self )
        --[[//-----------------------------------------------------------------------------
]]--[[// Init
]]--[[//-----------------------------------------------------------------------------
]]if( ModuleBegins() ) then
            do
                DEC_Trace( "$ MIS_Automate_MRT_RENS_ROHUM_RenseignerSur: debut MRT $" )
                DEC_SetMission( myself, self._namedParams.mission )
                                do
                    local listePionsSubordonnes = DEC_Automate_PionsSansPC()
                    for _,x in pairs( listePionsSubordonnes or {} ) do
                    local _continue = true
                        do
                            local pion = x
                            F_Pion_SeteEtatEchelon( pion, eEtatEchelon_Second )
                            F_Pion_SeteEtatSoutien( pion, eEtatSoutien_aucun )
                        end

                    end

                end

            end
--[[//-----------------------------------------------------------------------------
]]--[[// Gen
]]--[[//-----------------------------------------------------------------------------	
]]        end

        Activate( self.activations.BEH_Medo_Automate_RENS_ROHUM_AssignerMissions, 1, { "T_Mission_Automate_RENS_ROHUM_RenseignerSur", } )
    end,

}

eventmanager_plugin = eventmanager_plugin or {}
eventmanager_plugin.MIS_Automate_MRT_RENS_ROHUM_RenseignerSur = { "mission" }
connections = connections or {}
connections[ #connections + 1 ] = { nodes.MIS_Automate_MRT_RENS_ROHUM_RenseignerSur, "BEH_Medo_Automate_RENS_ROHUM_AssignerMissions", nodes.BEH_Medo_Automate_RENS_ROHUM_AssignerMissions }
