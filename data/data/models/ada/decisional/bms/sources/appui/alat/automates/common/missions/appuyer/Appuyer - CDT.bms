connections[ #connections + 1 ] = { nodes.MIS_Automate_CDT_ALAT_Appuyer, "BEH_Log_Automate_ALAT_GererHelitransportsPlotsEtIMEX", nodes.BEH_Log_Automate_ALAT_GererHelitransportsPlotsEtIMEX }
