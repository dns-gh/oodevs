    SendFeedback = function( self, value ) Feedback( self.feedbacks.done_BEH_Medo_Automate_RENS_ROIM_AssignerMettreEnOeuvreA, {value} ) end,
    activations =
    {
        { "ACT_Info_Trace", { "done_ACT_Info_Trace" } },
        { "BEH_Mission_Automate_RENS_ROIM_AssignerMettreEnOeuvreA", { "done_BEH_Mission_Automate_RENS_ROIM_AssignerMettreEnOeuvreA" } },
    },

    create = function( self )
        self._namedParams = {}
        self._namedParams.selSMO = self.params[1]
        self._namedParams.positionDeploiement = self.params[2]
        self._namedParams.eEtatEchelon = self.params[3]
    end,

    activate = function( self )
        local Activate = Activate
        for _,pion in pairs( --[[//-----------------------------------------------------------------------------
]]--[[// Affectation des missions à l'echelon
]]--[[//-----------------------------------------------------------------------------
]]self._namedParams.selSMO or emptyTable ) do
        local _continue = true
do
                Activate( self.activations.ACT_Info_Trace, 1, { "Operating mission assigned", } )
                Activate( self.activations.BEH_Mission_Automate_RENS_ROIM_AssignerMettreEnOeuvreA, 1, { ePhase_MRT, pion, self._namedParams.positionDeploiement, self._namedParams.eEtatEchelon, } )
            end

        end

    end,

}

