includedFiles = includedFiles or {}
includedFiles["sources/appui/nbc/automates/son reco/missions/reconnaitrezone/ReconnaitreZone - MRT.bms"] = true

--[[// *****************************************************************************
]]--[[// MIS_Automate_MRT_NBC_ReconnaitreUneZone
]]--[[// 
]]--[[// Commentaires:
]]--[[//
]]--[[// *****************************************************************************
]]node "MIS_Automate_MRT_NBC_ReconnaitreUneZone"
{
    feedbacks = { { { "done_MIS_Automate_MRT_NBC_ReconnaitreUneZone" }, "MIS_Automate_MRT_NBC_ReconnaitreUneZone" } },
    SendFeedback = function( self, value ) Feedback( self.feedbacks.done_MIS_Automate_MRT_NBC_ReconnaitreUneZone, {value} ) end,
    activations =
    {
        { "BEH_Medo_Automate_NBC_DonnerLesOrdresPourMission", { "done_BEH_Medo_Automate_NBC_DonnerLesOrdresPourMission" } },
    },

    create = function( self )
        self._namedParams = {}
        self._namedParams.mission = self.params.mission
    end,

    activate = function( self )
        local Activate = Activate
        --[[//-----------------------------------------------------------------------------
]]--[[// Init
]]--[[//-----------------------------------------------------------------------------
]]if( ModuleBegins() ) then
            do
                DEC_Trace( "$ MIS AUTO Reconnaitre zone: debut MRT $" )
                DEC_SetMission( myself, self._namedParams.mission )
                local listePionsSubordonnes = DEC_Automate_PionsSansPC()
                for _,x in pairs( listePionsSubordonnes or {} ) do
                local _continue = true
                    do
                        local pion = x
                        F_Pion_SeteEtatEchelon( pion, eEtatEchelon_Second )
                        F_Pion_SeteEtatSoutien( pion, eEtatSoutien_aucun )
                    end

                end

            end
--[[//-----------------------------------------------------------------------------
]]--[[// Gen
]]--[[//-----------------------------------------------------------------------------
]]        end

        Activate( self.activations.BEH_Medo_Automate_NBC_DonnerLesOrdresPourMission, 1, { "T_Mission_Automate_NBC_ReconnaitreUneZone", } )
    end,

}

eventmanager_plugin = eventmanager_plugin or {}
eventmanager_plugin.MIS_Automate_MRT_NBC_ReconnaitreUneZone = { "mission" }
connections = connections or {}
connections[ #connections + 1 ] = { nodes.MIS_Automate_MRT_NBC_ReconnaitreUneZone, "BEH_Medo_Automate_NBC_DonnerLesOrdresPourMission", nodes.BEH_Medo_Automate_NBC_DonnerLesOrdresPourMission }
