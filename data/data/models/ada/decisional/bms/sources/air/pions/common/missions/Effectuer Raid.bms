connections[ #connections + 1 ] = { nodes.MIS_Pion_JOINT_AIR_EffectuerRaid, "BEH_Eni_Pion_AIR_EffectuerRaid", nodes.BEH_Eni_Pion_AIR_EffectuerRaid }
