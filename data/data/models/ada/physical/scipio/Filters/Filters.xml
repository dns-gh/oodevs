<?xml version="1.0" encoding="UTF-8" standalone="no" ?>
<sections model-version="4.8.2" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../schemas/4.3.3/physical/Filters.xsd">
  <section id="export">
    <descriptions>
      <description name="Export..." lang="en"/>
      <description name="Exporter..." lang="fr"/>
    </descriptions>
    <filters>
      <filter output="html" target="exercise" xsl="htmlBriefing">
        <descriptions>
          <description name="Scenario extractor" lang="en"><![CDATA[Extracts briefing, terrain and model information from exercise meta data.<br>Outputs an HTML file.]]></description>
          <description name="Export de scénario" lang="fr"><![CDATA[Extrait les informations de briefing, terrain et modèle à partir de l'exercice.<br>Le résultat est un fichier HTML.]]></description>
        </descriptions>
      </filter>
      <filter output="xml" target="orbat" xsl="simpleOrbat">
        <descriptions>
          <description name="Simplified orbat export" lang="en"><![CDATA[Extracts teams, automats, units, populations and objects from orbat.<br>Outputs an XML file.]]></description>
          <description name="Export d'OdB simplifié" lang="fr"><![CDATA[Extrait les camps, automates, unités, populations et objets de l'OdB.<br>Le résultat est un fichier XML.]]></description>
        </descriptions>
      </filter>
      <filter command="ExportDQP.exe" directory="D:\THALES\SCIPIO\PrepaSE\bin">
        <arguments>
          <argument name="0"/>
          <argument name="-exercise" value="$exercise$"/>
          <argument name="-root-dir" value="$rootdir$"/>
        </arguments>
        <descriptions>
          <description name="Test DQP export" lang="en">HTML Description</description>
          <description name="Test d'export DQP" lang="fr">HTML Description</description>
        </descriptions>
      </filter>
      <filter command="ExportDQP.exe" directory="D:\THALES\SCIPIO\PrepaSE\bin">
        <arguments>
          <argument name="1"/>
          <argument name="-exercise" value="$exercise$"/>
          <argument name="-root-dir" value="$rootdir$"/>
        </arguments>
        <descriptions>
          <description name="Export DQP" lang="en">HTML Description</description>
          <description name="Exporter DQP" lang="fr">HTML Description</description>
        </descriptions>
      </filter>
    </filters>
  </section>
  <section id="import">
    <descriptions>
      <description name="Import..." lang="en"/>
      <description name="Importer..." lang="fr"/>
    </descriptions>
    <filters>
      <filter command="ImportDQP.exe" reload-exercise="true" directory="D:\THALES\SCIPIO\PrepaSE\bin">
        <arguments>
          <argument name="-exercise" value="$exercise$"/>
          <argument name="-root-dir" value="$rootdir$"/>
        </arguments>
        <descriptions>
          <description name="Import DQP" lang="en">HTML Description</description>
          <description name="Importer DQP" lang="fr">HTML Description</description>
        </descriptions>
      </filter>
      <filter command="Melmil.exe" reload-exercise="true" directory="D:\THALES\SCIPIO\PrepaSE\bin">
        <arguments>
          <argument name="-exercise" value="$exercise$"/>
          <argument name="-root-dir" value="$rootdir$"/>
        </arguments>
        <descriptions>
          <description name="Import Melmil" lang="en">HTML Description</description>
          <description name="Importer Melmil" lang="fr">HTML Description</description>
        </descriptions>
      </filter>
    </filters>
  </section>
</sections>
