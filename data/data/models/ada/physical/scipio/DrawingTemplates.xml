<?xml version="1.0" encoding="ISO-8859-1" standalone="no" ?>
<templates model-version="4.8.3">
    <category name='Lignes'>
        <description>Lignes tactiques</description>
        <template name='-- oo --' type="line">
            <description>Ligne de niveau section</description>
            <segment>
                <line stroke='currentColor' x1='0.02' x2='0.98' stroke-width='3px'/>
            </segment>
            <marker-mid unit='px'>
                <g fill='currentColor' stroke='none'>
                    <circle cy='-4' r='3'/>
                    <circle cy=' 4' r='3'/>
                </g>
            </marker-mid>
        </template>
        <template name='-- ooo --' type="line">
            <description>Ligne de niveau automate</description>
            <segment>
                <line stroke='currentColor' x1='0.02' x2='0.98' stroke-width='3px'/>
            </segment>
            <marker-mid unit='px'>
                <g fill='currentColor' stroke='none'>
                    <circle cy='-6' r='3'/>
                    <circle cy=' 0' r='3'/>
                    <circle cy=' 6' r='3'/>
                </g>
            </marker-mid>
        </template>
        <template name='-- i --' type="line">
            <description>Ligne de niveau compagnie</description>
            <segment>
                <line stroke='currentColor' x1='0.02' x2='0.98' stroke-width='3px'/>
            </segment>
            <marker-mid unit='px'>
                <line stroke='currentColor' stroke-width='2px' x1='5' x2='-5'/>
            </marker-mid>
        </template>
        <template name='-- ii --' type="line">
            <description>Ligne de niveau bataillon</description>
            <segment>
                <line stroke='currentColor' x1='0.02' x2='0.98' stroke-width='3px'/>
            </segment>
            <marker-mid unit='px'>
                <g stroke='currentColor' stroke-width='2px'>
                    <line  y1='-4' y2='-4' x1='5' x2='-5'/>
                    <line  y1=' 4' y2=' 4' x1='5' x2='-5'/>
                </g>
            </marker-mid>
        </template>
        <template name='-- iii --' type="line">
            <description>Ligne de niveau regiment</description>
            <segment>
                <line stroke='currentColor' x1='0.02' x2='0.98' stroke-width='3px'/>
            </segment>
            <marker-mid unit='px'>
              <g stroke='currentColor' stroke-width='2px'>
                  <line  y1='-6' y2='-6' x1='5' x2='-5'/>
                  <line  y1=' 0' y2=' 0' x1='5' x2='-5'/>
                  <line  y1=' 6' y2=' 6' x1='5' x2='-5'/>
              </g>
            </marker-mid>
        </template>
        <template name='-- x --' type="line">
            <description>Ligne de niveau brigade</description>
            <segment>
                <line stroke='currentColor' x1='0.02' x2='0.98' stroke-width='3px'/>
            </segment>
            <marker-mid unit='px'>
                <g stroke-width='2px'>
                    <line stroke='currentColor' x1='-5' y1=' 5' x2='5' y2='-5'/>
                    <line stroke='currentColor' x1='-5' y1='-5' x2='5' y2=' 5'/>
                </g>
            </marker-mid>
        </template>
        <template name='-- xx --' type="line">
            <description>Ligne de niveau division</description>
            <segment>
                <line stroke='currentColor' x1='0.02' x2='0.98' stroke-width='3px'/>
            </segment>
            <marker-mid unit='px'>
                <g stroke-width='2px'>
                    <g transform='translate(0,-6)'>
                        <line stroke='currentColor' x1='-5' y1=' 5' x2='5' y2='-5'/>
                        <line stroke='currentColor' x1='-5' y1='-5' x2='5' y2=' 5'/>
                    </g>
                    <g transform='translate(0,6)'>
                        <line stroke='currentColor' x1='-5' y1=' 5' x2='5' y2='-5'/>
                        <line stroke='currentColor' x1='-5' y1='-5' x2='5' y2=' 5'/>
                    </g>
                </g>
            </marker-mid>
        </template>
    </category>
