<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<sections xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../schemas/4.3.1/physical/Filters.xsd">
  <section id="export">
    <descriptions>
      <description xml:lang="en" name="Export..."/>
      <description xml:lang="fr" name="Exporter..."/>
    </descriptions>
    <filters>
      <filter target="exercise" xsl="htmlBriefing" output="html">
        <descriptions>
          <description xml:lang="en" name="Scenario extractor"><![CDATA[Extracts briefing, terrain and model information from exercise meta data.<br>Outputs an HTML file.]]></description>
          <description xml:lang="fr" name="Export de scénario"><![CDATA[Extrait les informations de briefing, terrain et modèle à partir de l'exercice.<br>Le résultat est un fichier HTML.]]></description>
        </descriptions>
      </filter>
      <filter target="orbat" xsl="simpleOrbat" output="xml">
        <descriptions>
          <description xml:lang="en" name="Simplified orbat export"><![CDATA[Extracts teams, automats, units, populations and objects from orbat.<br>Outputs an XML file.]]></description>
          <description xml:lang="fr" name="Export d'OdB simplifié"><![CDATA[Extrait les camps, automates, unités, populations et objets de l'OdB.<br>Le résultat est un fichier XML.]]></description>
        </descriptions>
      </filter>
    </filters>
  </section>
</sections>
