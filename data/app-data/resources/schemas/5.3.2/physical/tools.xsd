	<xs:simpleType name="T_ReportClientFunctionTrigger">
		<xs:restriction base="xs:string">
			<xs:enumeration value="FirstFireStarted">
				<xs:annotation>
					<xs:documentation source="Fonction permettant le passage d'une demande de tir à l'état premier coup parti (CR n°168)" xml:lang="fr"/>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="ExecutedFire">
				<xs:annotation>
					<xs:documentation source="Fonction permettant le passage d'une demande de tir à l'état  executé (CR n°169)" xml:lang="fr"/>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="OutOfRangeFire">
				<xs:annotation>
					<xs:documentation source="Fonction permettant le passage d'une demande de tir à l'état refusé avec comme cause un tir hors de portée (CR n°170)" xml:lang="fr"/>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="NoAmoLeftFire">
				<xs:annotation>
					<xs:documentation source="Fonction permettant le passage d'une demande de tir à l'état refusé avec comme cause plus de munitions (CR n°171)" xml:lang="fr"/>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="CanceledFire">
				<xs:annotation>
					<xs:documentation source="Fonction permettant le passage d'une demande de tir à l'état refusé avec comme cause Tir annulé unité de tir indisponible (CR n°171)" xml:lang="fr"/>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="IndirectFireObservation">
				<xs:annotation>
					<xs:documentation source="Fonction permettant la création d'une demande de tir depuis la position observée du tireur (CR n°177)" xml:lang="fr"/>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="MissionEndAlert">
				<xs:annotation>
					<xs:documentation source="Fonction permettant l'affichage d'un indicateur visuel CR suite à un CR fin de mission (CR n°12)" xml:lang="fr"/>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="FiredAlert">
				<xs:annotation>
					<xs:documentation source="Fonction permettant l'affichage d'un indicateur visuel ATTACKED suite à un CR pris a partie par tir direct (CR n°36) ou pris sous tir artillerie (CR n°37) ou prise a partie par population (CR n°226) ou agression par population (CR n°250)" xml:lang="fr"/>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="NBCAlert">
				<xs:annotation>
					<xs:documentation source="Fonction permettant l'affichage d'un indicateur visuel NBC suite à un CR contamine nbc (CR n°148) ou alerte nbc niveau4 (CR n°152)" xml:lang="fr"/>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="CrowAlert">
				<xs:annotation>
					<xs:documentation source="Fonction permettant l'affichage d'un indicateur visuel POPULATION suite à un CR population detectee (CR n°225) ou agression de population (CR n°249)" xml:lang="fr"/>
				</xs:annotation>
			</xs:enumeration>
			<!--- Gestion des tirs -->
			<!--- Gestion des indicateurs visuels -->
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="T_ReportCategory">
		<xs:annotation>
			<xs:documentation/>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="information"/>
			<xs:enumeration value="operational"/>
			<xs:enumeration value="exceptional"/>
			<xs:enumeration value="warning"/>
		</xs:restriction>
