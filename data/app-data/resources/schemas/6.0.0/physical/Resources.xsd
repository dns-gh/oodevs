								</xs:annotation>
								<xs:complexType>
									<xs:sequence>
										<xs:element name="indirect-fire" minOccurs="0" maxOccurs="unbounded">
											<xs:complexType>
												<xs:sequence>
													<xs:element name="ph" minOccurs="0" maxOccurs="unbounded">
														<xs:complexType>
															<xs:attribute name="target-posture" type="xs:string" use="required"/>
															<xs:attribute name="value" type="T_NonNegativeDouble" use="required"/>
														</xs:complexType>
													</xs:element>
												</xs:sequence>
												<xs:attribute name="type" type="xs:string" use="required"/>
												<xs:attribute name="object-type" type="xs:string" use="optional"/>
												<xs:attribute name="neutralization-ratio" type="T_NonNegativeDouble" use="optional"/>
												<xs:attribute name="dispersion-factor" type="T_ZeroToOneDouble" use="optional"/>
												<xs:attribute name="setup-time" type="T_Time" use="optional"/>
												<xs:attribute name="life-time" type="T_Time" use="optional"/>
												<xs:attribute name="mine-count" type="xs:nonNegativeInteger" use="optional"/>
											</xs:complexType>
										</xs:element>
									</xs:sequence>
									<xs:attribute name="intervention-type" type="xs:nonNegativeInteger" use="required">
										<xs:annotation>
											<xs:documentation xml:lang="en">TODO</xs:documentation>
											<xs:documentation xml:lang="fr">TODO</xs:documentation>
										</xs:annotation>
									</xs:attribute>
									<xs:attribute name="x-dispersion" type="T_NonNegativeDouble" use="required">
										<xs:annotation>
											<xs:documentation xml:lang="en">TODO</xs:documentation>
											<xs:documentation xml:lang="fr">Rayon r1 de l’ellipse d’effet du tir</xs:documentation>
										</xs:annotation>
									</xs:attribute>
									<xs:attribute name="y-dispersion" type="T_NonNegativeDouble" use="required">
										<xs:annotation>
											<xs:documentation xml:lang="en">TODO</xs:documentation>
											<xs:documentation xml:lang="fr">Rayon r2 de l’ellipse d’effet du tir</xs:documentation>
										</xs:annotation>
									</xs:attribute>
									<xs:attribute name="detection-range" type="T_NonNegativeDouble" use="optional">
										<xs:annotation>
											<xs:documentation xml:lang="en">TODO</xs:documentation>
											<xs:documentation xml:lang="fr">Portée en mètres à laquelle le tir peut être detecté (entendu).</xs:documentation>
										</xs:annotation>
									</xs:attribute>
								</xs:complexType>
							</xs:element>
						</xs:sequence>
						<xs:attribute name="network-usable" type="xs:boolean" use="required">
							<xs:annotation>
								<xs:documentation xml:lang="en">Usable within a resource network</xs:documentation>
								<xs:documentation xml:lang="fr">Utilisable dans un réseau de ressource</xs:documentation>
							</xs:annotation>
						</xs:attribute>
						<xs:attribute name="category" type="T_ResourceCategory" use="required">
							<xs:annotation>
								<xs:documentation>Catégorie physique de dotation (Munition, Carburant, Mine etc.)</xs:documentation>
							</xs:annotation>
						</xs:attribute>
						<xs:attribute name="name" type="xs:string" use="required">
							<xs:annotation>
								<xs:documentation xml:lang="en">Identifying resource name</xs:documentation>
								<xs:documentation xml:lang="fr">Nom identifiant la dotation</xs:documentation>
							</xs:annotation>
						</xs:attribute>
						<xs:attribute name="type" type="xs:string">
							<xs:annotation>
								<xs:documentation>Type de la dotation</xs:documentation>
							</xs:annotation>
						</xs:attribute>
						<xs:attribute name="id" type="xs:nonNegativeInteger" use="required">
							<xs:annotation>
								<xs:documentation xml:lang="en">TODO</xs:documentation>
								<xs:documentation xml:lang="fr">Identifiant numérique de la dotation</xs:documentation>
							</xs:annotation>
						</xs:attribute>
						<xs:attribute name="package-size" type="T_NonNegativeDouble" use="required">
							<xs:annotation>
								<xs:documentation xml:lang="en">TODO</xs:documentation>
								<xs:documentation xml:lang="fr">Nombre d'unités dans un package de cette dotation</xs:documentation>
							</xs:annotation>
						</xs:attribute>
						<xs:attribute name="package-mass" type="T_NonNegativeDouble" use="required">
							<xs:annotation>
								<xs:documentation xml:lang="en">TODO</xs:documentation>
								<xs:documentation xml:lang="fr">Masse unitaire</xs:documentation>
							</xs:annotation>
						</xs:attribute>
						<xs:attribute name="package-volume" type="T_NonNegativeDouble" use="required">
							<xs:annotation>
								<xs:documentation xml:lang="en">TODO</xs:documentation>
								<xs:documentation xml:lang="fr">Volume unitaire</xs:documentation>
							</xs:annotation>
						</xs:attribute>
						<xs:attribute name="nature" type="xs:string"/>
						<xs:attribute name="id-nature" type="xs:positiveInteger" use="required">
							<xs:annotation>
								<xs:documentation>Id de la nature de la dotation</xs:documentation>
							</xs:annotation>
						</xs:attribute>
						<xs:attribute name="ied" type="xs:boolean" use="optional">
							<xs:annotation>
								<xs:documentation xml:lang="en">Used for explosions (terrorist attack, etc.)</xs:documentation>
								<xs:documentation xml:lang="fr">Utilisé pour les explosions (attaques terroristes, etc.)</xs:documentation>
							</xs:annotation>
						</xs:attribute>
						<xs:attribute name="codeEMAT6" type="xs:string" use="optional">
							<xs:annotation>
								<xs:documentation xml:lang="en">TODO</xs:documentation>
								<xs:documentation xml:lang="fr">code EMAT6 (codification armée de terre française)</xs:documentation>
							</xs:annotation>
						</xs:attribute>
						<xs:attribute name="codeEMAT8" type="xs:string" use="optional">
							<xs:annotation>
								<xs:documentation xml:lang="en">TODO</xs:documentation>
								<xs:documentation xml:lang="fr">code EMAT8 (codification armée de terre française)</xs:documentation>
							</xs:annotation>
						</xs:attribute>
						<xs:attribute name="codeLFRIL" type="xs:string" use="optional">
