<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified" attributeFormDefault="unqualified" version="4.8.2">
	<xs:include schemaLocation="tools.xsd"/>
	<xs:element name="resources">
		<xs:annotation>
			<xs:documentation xml:lang="en">TODO</xs:documentation>
			<xs:documentation xml:lang="fr">Définit la liste des types de dotations.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="resource" minOccurs="0" maxOccurs="unbounded">
					<xs:annotation>
						<xs:documentation xml:lang="en">TODO</xs:documentation>
						<xs:documentation xml:lang="fr">Définit un type de dotation</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:sequence>
							<xs:element name="illuminating" minOccurs="0">
								<xs:annotation>
									<xs:documentation xml:lang="en">TODO</xs:documentation>
									<xs:documentation xml:lang="fr">Capacité d'illumination d'une cible</xs:documentation>
								</xs:annotation>
								<xs:complexType>
									<xs:attribute name="range" type="T_NonNegativeDouble" use="required">
										<xs:annotation>
											<xs:documentation xml:lang="en">TODO</xs:documentation>
											<xs:documentation xml:lang="fr">Indique la portée de tir pour marquer une cible</xs:documentation>
										</xs:annotation>
									</xs:attribute>
									<xs:attribute name="maintain" type="xs:boolean" use="required">
										<xs:annotation>
											<xs:documentation xml:lang="en">TODO</xs:documentation>
											<xs:documentation xml:lang="fr">Indique s'il faut maintenir la visée</xs:documentation>
										</xs:annotation>
									</xs:attribute>
								</xs:complexType>
							</xs:element>
							<xs:element name="guided" minOccurs="0">
								<xs:annotation>
									<xs:documentation xml:lang="en">TODO</xs:documentation>
									<xs:documentation xml:lang="fr">Capacité Ã  tirer des cibles guidées</xs:documentation>
								</xs:annotation>
								<xs:complexType>
									<xs:attribute name="maintain" type="xs:boolean" use="required">
										<xs:annotation>
											<xs:documentation xml:lang="en">TODO</xs:documentation>
											<xs:documentation xml:lang="fr">Indique s'il faut maintenir la visée</xs:documentation>
										</xs:annotation>
									</xs:attribute>
									<xs:attribute name="range" type="T_NonNegativeDouble" use="required">
										<xs:annotation>
											<xs:documentation xml:lang="en">TODO</xs:documentation>
											<xs:documentation xml:lang="fr">Indique la distance maximale entre la cible et l'illuminateur</xs:documentation>
										</xs:annotation>
									</xs:attribute>
								</xs:complexType>
							</xs:element>
							<xs:element name="attritions" minOccurs="0">
								<xs:annotation>
									<xs:documentation xml:lang="en">TODO</xs:documentation>
									<xs:documentation xml:lang="fr">Dégâts causé par la munition, données par type de protection de la cible</xs:documentation>
								</xs:annotation>
								<xs:complexType>
									<xs:sequence>
										<xs:element name="attrition" minOccurs="0" maxOccurs="unbounded">
											<xs:annotation>
												<xs:documentation xml:lang="en">TODO</xs:documentation>
												<xs:documentation xml:lang="fr">Dégâts causé par la munition, pour le type de protection donné</xs:documentation>
											</xs:annotation>
											<xs:complexType>
												<xs:attribute name="protection" type="xs:string" use="required">
													<xs:annotation>
														<xs:documentation xml:lang="en">TODO</xs:documentation>
														<xs:documentation xml:lang="fr">Protection de la cible</xs:documentation>
													</xs:annotation>
												</xs:attribute>
												<xs:attribute name="destruction" type="T_NonNegativeDouble" use="required">
													<xs:annotation>
														<xs:documentation xml:lang="en">TODO</xs:documentation>
														<xs:documentation xml:lang="fr">Probabilité de détruire la cible</xs:documentation>
													</xs:annotation>
												</xs:attribute>
												<xs:attribute name="repairable-with-evacuation" type="T_NonNegativeDouble" use="required">
													<xs:annotation>
														<xs:documentation xml:lang="en">TODO</xs:documentation>
														<xs:documentation xml:lang="fr">Probabilité d’endommager la cible en la rendant réparable sous réserve d'évacuation</xs:documentation>
													</xs:annotation>
												</xs:attribute>
												<xs:attribute name="repairable-without-evacuation" type="T_NonNegativeDouble" use="required">
													<xs:annotation>
														<xs:documentation xml:lang="en">TODO</xs:documentation>
														<xs:documentation xml:lang="fr">Probabilité d’endommager la cible en la rendant réparable sans nécessité d'évacuation</xs:documentation>
													</xs:annotation>
												</xs:attribute>
											</xs:complexType>
										</xs:element>
									</xs:sequence>
								</xs:complexType>
							</xs:element>
							<xs:element name="urban-modifiers" minOccurs="0">
								<xs:annotation>
									<xs:documentation xml:lang="en">Modifiers depending on ammunitions and urban block material</xs:documentation>
									<xs:documentation xml:lang="fr">Modifieurs dépendants des munitions et des matériaux des blocs urbains</xs:documentation>
								</xs:annotation>
								<xs:complexType>
									<xs:sequence>
										<xs:element name="urban-modifier" minOccurs="0" maxOccurs="unbounded">
											<xs:annotation>
												<xs:documentation xml:lang="en">Specific modifier</xs:documentation>
												<xs:documentation xml:lang="fr">Modifieur particulier</xs:documentation>
											</xs:annotation>
											<xs:complexType>
												<xs:attribute name="material-type" type="xs:string" use="required">
													<xs:annotation>
														<xs:documentation xml:lang="en">Urban block material type</xs:documentation>
														<xs:documentation xml:lang="fr">Type de matériau du bloc urbain</xs:documentation>
													</xs:annotation>
												</xs:attribute>
												<xs:attribute name="value" type="T_NonNegativeDouble" use="required">
													<xs:annotation>
														<xs:documentation xml:lang="en">Alleviation value</xs:documentation>
														<xs:documentation xml:lang="fr">Valeur d'atténuation</xs:documentation>
													</xs:annotation>
												</xs:attribute>
											</xs:complexType>
										</xs:element>
									</xs:sequence>
								</xs:complexType>
							</xs:element>
							<xs:sequence/>
							<xs:element name="indirect-fires" minOccurs="0">
								<xs:annotation>
									<xs:documentation xml:lang="en">Indirect fire specific data, depending on the fire effect</xs:documentation>
