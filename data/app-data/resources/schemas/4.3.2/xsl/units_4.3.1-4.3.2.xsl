<?xml version="1.0" encoding="UTF-8"?><xsl:stylesheet version="1.0" xmlns:xsl="http://www.w3.org/1999/XSL/Transform">    <xsl:output method="xml" indent="yes" encoding="UTF-8"/>    <xsl:template match="node()|@*">        <xsl:copy>            <xsl:apply-templates select="node()|@*"/>        </xsl:copy>    </xsl:template>    <xsl:template match="unit/stocks/stock">        <xsl:copy>            <xsl:attribute name="category">                <xsl:choose>                    <xsl:when test="@category = 'Unite feu TD'">                        <xsl:text>Unite tranche D</xsl:text>                    </xsl:when>                    <xsl:when test="@category = 'Unite feu -TD'">                        <xsl:text>Unite non tranche D</xsl:text>                    </xsl:when>                    <xsl:otherwise>                        <xsl:value-of select="@category"/>                    </xsl:otherwise>                </xsl:choose>            </xsl:attribute>            <xsl:attribute name="threshold">                <xsl:value-of select="@threshold"/>            </xsl:attribute>        </xsl:copy>    </xsl:template>    </xsl:stylesheet>