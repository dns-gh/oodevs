<?xml version="1.0" encoding="UTF-8"?><xsl:stylesheet version="1.0" xmlns:xsl="http://www.w3.org/1999/XSL/Transform">    <xsl:output method="xml" indent="yes" encoding="UTF-8"/>        <xsl:template name="convertTerrain">        <xsl:param name="type"/>        <xsl:choose>            <xsl:when test="$type = 'inconnu'">unknown</xsl:when>            <xsl:when test="$type = 'foret'">forest</xsl:when>            <xsl:when test="$type = 'plantation'">orchard</xsl:when>            <xsl:when test="$type = 'marais'">swamp</xsl:when>            <xsl:when test="$type = 'montagne'">mountain</xsl:when>            <xsl:when test="$type = 'dune'">dune</xsl:when>            <xsl:when test="$type = 'urbain'">urban area</xsl:when>            <xsl:when test="$type = 'etendue eau'">lake</xsl:when>            <xsl:when test="$type = 'glace'">glacier</xsl:when>            <xsl:when test="$type = 'lisiere'">forest edge</xsl:when>            <xsl:when test="$type = 'banlieue'">suburb</xsl:when>            <xsl:when test="$type = 'cote'">waterfront</xsl:when>            <xsl:when test="$type = 'bord de plantation'">orchard edge</xsl:when>            <xsl:when test="$type = 'bord de marais'">swamp edge</xsl:when>            <xsl:when test="$type = 'bord de dune'">dune edge</xsl:when>            <xsl:when test="$type = 'bord de glace'">glacier edge</xsl:when>            <xsl:when test="$type = 'chemin'">country road</xsl:when>            <xsl:when test="$type = 'autoroute'">highway</xsl:when>            <xsl:when test="$type = 'nationale'">main road</xsl:when>            <xsl:when test="$type = 'departementale'">secondary road</xsl:when>            <xsl:when test="$type = 'ruisseau'">stream</xsl:when>            <xsl:when test="$type = 'riviere'">river</xsl:when>            <xsl:when test="$type = 'riviere importante'">main river</xsl:when>            <xsl:when test="$type = 'pont'">bridge</xsl:when>            <xsl:when test="$type = 'falaise'">cliff</xsl:when>            <xsl:when test="$type = 'voie ferree'">railroad</xsl:when>            <xsl:when test="$type = 'carrefour'">crossroad</xsl:when>            <xsl:when test="$type = 'montagne'">mountain</xsl:when>            <xsl:when test="$type = 'bord de montagne'">mountain edge</xsl:when>            <xsl:when test="$type = 'lima'">lima</xsl:when>            <xsl:otherwise>error</xsl:otherwise>        </xsl:choose>    </xsl:template>  </xsl:stylesheet>