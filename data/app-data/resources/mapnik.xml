<?xml version="1.0" encoding="utf-8"?>

<!DOCTYPE Map [
  <!ENTITY maxscale_zoom0 "<MaxScaleDenominator>250000000000</MaxScaleDenominator>">
  <!ENTITY minscale_zoom1 "<MinScaleDenominator>200000000</MinScaleDenominator>">
  <!ENTITY maxscale_zoom2 "<MaxScaleDenominator>200000000</MaxScaleDenominator>">
  <!ENTITY maxscale_zoom1 "<MaxScaleDenominator>500000000</MaxScaleDenominator>">
  <!ENTITY minscale_zoom2 "<MinScaleDenominator>100000000</MinScaleDenominator>">
  <!ENTITY maxscale_zoom3 "<MaxScaleDenominator>100000000</MaxScaleDenominator>">
  <!ENTITY minscale_zoom3 "<MinScaleDenominator>50000000</MinScaleDenominator>">
  <!ENTITY maxscale_zoom4 "<MaxScaleDenominator>50000000</MaxScaleDenominator>">
  <!ENTITY minscale_zoom4 "<MinScaleDenominator>25000000</MinScaleDenominator>">
  <!ENTITY maxscale_zoom5 "<MaxScaleDenominator>25000000</MaxScaleDenominator>">
  <!ENTITY minscale_zoom5 "<MinScaleDenominator>12500000</MinScaleDenominator>">
  <!ENTITY maxscale_zoom6 "<MaxScaleDenominator>12500000</MaxScaleDenominator>">
  <!ENTITY minscale_zoom6 "<MinScaleDenominator>6500000</MinScaleDenominator>">
  <!ENTITY maxscale_zoom7 "<MaxScaleDenominator>6500000</MaxScaleDenominator>">
  <!ENTITY minscale_zoom7 "<MinScaleDenominator>3000000</MinScaleDenominator>">
  <!ENTITY maxscale_zoom8 "<MaxScaleDenominator>3000000</MaxScaleDenominator>">
  <!ENTITY minscale_zoom8 "<MinScaleDenominator>1500000</MinScaleDenominator>">
  <!ENTITY maxscale_zoom9 "<MaxScaleDenominator>1500000</MaxScaleDenominator>">
  <!ENTITY minscale_zoom9 "<MinScaleDenominator>750000</MinScaleDenominator>">
  <!ENTITY maxscale_zoom10 "<MaxScaleDenominator>750000</MaxScaleDenominator>">
  <!ENTITY minscale_zoom10 "<MinScaleDenominator>400000</MinScaleDenominator>">
  <!ENTITY maxscale_zoom11 "<MaxScaleDenominator>400000</MaxScaleDenominator>">
  <!ENTITY minscale_zoom11 "<MinScaleDenominator>200000</MinScaleDenominator>">
  <!ENTITY maxscale_zoom12 "<MaxScaleDenominator>200000</MaxScaleDenominator>">
  <!ENTITY minscale_zoom12 "<MinScaleDenominator>100000</MinScaleDenominator>">
  <!ENTITY maxscale_zoom13 "<MaxScaleDenominator>100000</MaxScaleDenominator>">
  <!ENTITY minscale_zoom13 "<MinScaleDenominator>50000</MinScaleDenominator>">
  <!ENTITY maxscale_zoom14 "<MaxScaleDenominator>50000</MaxScaleDenominator>">
  <!ENTITY minscale_zoom14 "<MinScaleDenominator>25000</MinScaleDenominator>">
  <!ENTITY maxscale_zoom15 "<MaxScaleDenominator>25000</MaxScaleDenominator>">
  <!ENTITY minscale_zoom15 "<MinScaleDenominator>12500</MinScaleDenominator>">
  <!ENTITY maxscale_zoom16 "<MaxScaleDenominator>12500</MaxScaleDenominator>">
  <!ENTITY minscale_zoom16 "<MinScaleDenominator>5000</MinScaleDenominator>">
  <!ENTITY maxscale_zoom17 "<MaxScaleDenominator>5000</MaxScaleDenominator>">
  <!ENTITY minscale_zoom17 "<MinScaleDenominator>2500</MinScaleDenominator>">
  <!ENTITY maxscale_zoom18 "<MaxScaleDenominator>2500</MaxScaleDenominator>">
  <!ENTITY minscale_zoom18 "<MinScaleDenominator>1000</MinScaleDenominator>">
]>

<Map background-color="lightgrey">

  <FontSet name="book-fonts">
    <Font face-name="DejaVu Sans Book" />
  </FontSet>
  <FontSet name="bold-fonts">
      <Font face-name="DejaVu Sans Bold" />
    <Font face-name="unifont Medium" />
  </FontSet>

  <Style name="water">
    <Rule>
      &maxscale_zoom6;
      <PolygonSymbolizer gamma=".75" fill="#b5d0d0"/>
    </Rule>
  </Style>

  <Style name="forest">
    <Rule>
      &maxscale_zoom8;
<!--
      &minscale_zoom13;
-->
      <PolygonSymbolizer fill="#336600"/>
    </Rule>
<!--
    <Rule>
      &maxscale_zoom14;
      <PolygonPatternSymbolizer file="image/forest.png"/>
    </Rule>
-->
  </Style>

  <Style name="plantation">
    <Rule>
      &maxscale_zoom8;
      <PolygonSymbolizer fill="#5C8A00"/>
    </Rule>
  </Style>

  <Style name="swamp">
    <Rule>
      &maxscale_zoom8;
      <PolygonSymbolizer fill="#8A5C2E"/>
    </Rule>
  </Style>

  <Style name="river">
    <Rule>
      &maxscale_zoom13;
      &minscale_zoom13;
      <LineSymbolizer stroke-linejoin="round" stroke="#b5d0d0" stroke-width="3" stroke-linecap="round"/>
    </Rule>
    <Rule>
      &maxscale_zoom14;
      &minscale_zoom14;
      <LineSymbolizer stroke-linejoin="round" stroke="#b5d0d0" stroke-width="5" stroke-linecap="round"/>
    </Rule>
    <Rule>
      &maxscale_zoom15;
      &minscale_zoom16;
      <LineSymbolizer stroke-linejoin="round" stroke="#b5d0d0" stroke-width="6" stroke-linecap="round"/>
    </Rule>
    <Rule>
      &maxscale_zoom17;
      &minscale_zoom17;
      <LineSymbolizer stroke-linejoin="round" stroke="#b5d0d0" stroke-width="10" stroke-linecap="round"/>
    </Rule>
    <Rule>
      &maxscale_zoom18;
      <LineSymbolizer stroke-linejoin="round" stroke="#b5d0d0" stroke-width="12" stroke-linecap="round"/>
    </Rule>
  </Style>

  <Style name="urban">
    <Rule>
      <LineSymbolizer stroke="grey" stroke-width="1"/>
      <PolygonSymbolizer fill="#ededed"/>
    </Rule>
  </Style>

  <Style name="block">
    <Rule>
      <LineSymbolizer stroke="#999" stroke-width="1"/>
      <PolygonSymbolizer fill="#D3D3D3"/>
    </Rule>
  </Style>

  <Style name="mountain">
    <Rule>
      <Filter>[MASACODE] = 10007</Filter>
      <PolygonSymbolizer fill="#4e3d28"/>
    </Rule>
  </Style>

  <Style name="dune">
    <Rule>
      <Filter>[MASACODE] = 10005</Filter>
      <PolygonSymbolizer fill="#FFEDA5"/>
    </Rule>
  </Style>

  <Style name="cliff">
    <Rule>
      &maxscale_zoom13;
      &minscale_zoom14;
      <LineSymbolizer stroke-width="2" stroke="black" stroke-linejoin="round"/>
    </Rule>
    <Rule>
      &maxscale_zoom15;
      &minscale_zoom16;
      <LineSymbolizer stroke-width="4" stroke="black" stroke-linejoin="round"/>
    </Rule>
    <Rule>
      &maxscale_zoom17;
      <LineSymbolizer stroke-width="6" stroke="black" stroke-linejoin="round"/>
    </Rule>
  </Style>

  <Style name="tunnel">
    <Rule>
    </Rule>
    <Rule>
      &maxscale_zoom14;
      &minscale_zoom15;
      <LineSymbolizer stroke-width="2" stroke="#ffffff" stroke-linejoin="round" stroke-linecap="round" stroke-opacity="0.4"/>
      <LineSymbolizer stroke-width="1" stroke="#996600" stroke-linecap="round" stroke-linejoin="round" stroke-dasharray="3,4"/>
    </Rule>
    <Rule>
      &maxscale_zoom15;
      &minscale_zoom16;
      <LineSymbolizer stroke-width="3" stroke="#ffffff" stroke-linejoin="round" stroke-linecap="round" stroke-opacity="0.4"/>
      <LineSymbolizer stroke-width="2" stroke="#996600" stroke-linecap="round" stroke-linejoin="round" stroke-dasharray="3,4"/>
    </Rule>
    <Rule>
      &maxscale_zoom17;
      <LineSymbolizer stroke-width="3" stroke="#996600" stroke-linecap="round" stroke-linejoin="round" stroke-dasharray="6,8"/>
    </Rule>
  </Style>

  <Style name="bridge">
    <Rule>
      &maxscale_zoom13;
      &minscale_zoom13;
      <LineSymbolizer stroke-linejoin="round" stroke="black" stroke-width="3"/>
    </Rule>
    <Rule>
      &maxscale_zoom14;
      &minscale_zoom15;
      <LineSymbolizer stroke-linejoin="round" stroke="black" stroke-width="10"/>
    </Rule>
    <Rule>
      &maxscale_zoom15;
      &minscale_zoom16;
      <LineSymbolizer stroke-linejoin="round" stroke="black" stroke-width="12"/>
    </Rule>
    <Rule>
      &maxscale_zoom17;
      &minscale_zoom17;
      <LineSymbolizer stroke-linejoin="round" stroke="black" stroke-width="17"/>
    </Rule>
    <Rule>
      &maxscale_zoom18;
      <LineSymbolizer stroke-linejoin="round" stroke="black" stroke-width="23"/>
    </Rule>
  </Style>

  <Style name="railroad">
    <Rule>
      &maxscale_zoom13;
      &minscale_zoom14;
      <LineSymbolizer stroke-width="3" stroke="#999999" stroke-linejoin="round"/>
      <LineSymbolizer stroke-width="1" stroke="#ffffff" stroke-linejoin="round" stroke-dasharray="0,11,8,1"/>
    </Rule>
    <Rule>
      &maxscale_zoom15;
      &minscale_zoom16;
      <LineSymbolizer stroke-width="5" stroke="#999999" stroke-linejoin="round"/>
      <LineSymbolizer stroke-width="3" stroke="#ffffff" stroke-linejoin="round" stroke-dasharray="0,11,8,1"/>
    </Rule>
    <Rule>
      &maxscale_zoom17;
      <LineSymbolizer stroke-width="8" stroke="#999999" stroke-linejoin="round"/>
      <LineSymbolizer stroke-width="5" stroke="#ffffff" stroke-linejoin="round" stroke-dasharray="0,11,8,1"/>
    </Rule>
  </Style>

  <!--Style name="road-text">
    <Rule>
      <Filter>[MASACODE] = 20001</Filter>
      &maxscale_zoom13;
      <ShieldSymbolizer size="10" fill="#fff" placement="line" file="image/mot_shield.png" spacing="750" minimum-distance="30" fontset-name="bold-fonts">[NAME]</ShieldSymbolizer>
    </Rule>
    <Rule>
      <Filter>[MASACODE] = 20003</Filter>
      &maxscale_zoom13;
      &minscale_zoom13;
      <TextSymbolizer size="15" fill="black" placement="line" fontset-name="book-fonts" halo-radius="1" halo-fill="#fed7a5">[NAME]</TextSymbolizer>
    </Rule>
  </Style-->

  <Style name="road-casing">
  <!-- small -->
    <Rule>
      <Filter>[MASACODE] = 20004</Filter>
      &maxscale_zoom14;
      &minscale_zoom15;
      <LineSymbolizer stroke-linejoin="round" stroke="grey" stroke-width="3" stroke-linecap="round"/>
    </Rule>
    <Rule>
      <Filter>[MASACODE] = 20004</Filter>
      &maxscale_zoom15;
      &minscale_zoom16;
      <LineSymbolizer stroke-linejoin="round" stroke="grey" stroke-width="4" stroke-linecap="round"/>
    </Rule>
    <Rule>
      <Filter>[MASACODE] = 20004</Filter>
      &maxscale_zoom17;
      &minscale_zoom17;
      <LineSymbolizer stroke-linejoin="round" stroke="grey" stroke-width="6" stroke-linecap="round"/>
    </Rule>
    <Rule>
      <Filter>[MASACODE] = 20004</Filter>
      &maxscale_zoom18;
      <LineSymbolizer stroke-linejoin="round" stroke="grey" stroke-width="9" stroke-linecap="round"/>
    </Rule>

  <!-- medium -->
    <Rule>
      <Filter>[MASACODE] = 20003</Filter>
      &maxscale_zoom14;
      &minscale_zoom15;
      <LineSymbolizer stroke-linejoin="round" stroke="grey" stroke-width="4" stroke-linecap="round"/>
    </Rule>
    <Rule>
      <Filter>[MASACODE] = 20003</Filter>
      &maxscale_zoom15;
      &minscale_zoom16;
      <LineSymbolizer stroke-linejoin="round" stroke="grey" stroke-width="6" stroke-linecap="round"/>
    </Rule>
    <Rule>
      <Filter>[MASACODE] = 20003</Filter>
      &maxscale_zoom17;
      &minscale_zoom17;
      <LineSymbolizer stroke-linejoin="round" stroke="grey" stroke-width="11" stroke-linecap="round"/>
    </Rule>
    <Rule>
      <Filter>[MASACODE] = 20003</Filter>
      &maxscale_zoom18;
      <LineSymbolizer stroke-linejoin="round" stroke="grey" stroke-width="13" stroke-linecap="round"/>
    </Rule>

  <!-- large -->
    <Rule>
      <Filter>[MASACODE] = 20002</Filter>
      &maxscale_zoom13;
      &minscale_zoom13;
      <LineSymbolizer stroke-linejoin="round" stroke="grey" stroke-width="5" stroke-linecap="round"/>
    </Rule>
    <Rule>
      <Filter>[MASACODE] = 20002</Filter>
      &maxscale_zoom14;
      &minscale_zoom14;
      <LineSymbolizer stroke-linejoin="round" stroke="grey" stroke-width="6" stroke-linecap="round"/>
    </Rule>
    <Rule>
      <Filter>[MASACODE] = 20002</Filter>
      &maxscale_zoom15;
      &minscale_zoom16;
      <LineSymbolizer stroke-linejoin="round" stroke="grey" stroke-width="8" stroke-linecap="round"/>
    </Rule>
    <Rule>
      <Filter>[MASACODE] = 20002</Filter>
      &maxscale_zoom17;
      &minscale_zoom17;
      <LineSymbolizer stroke-linejoin="round" stroke="grey" stroke-width="13" stroke-linecap="round"/>
    </Rule>
    <Rule>
      <Filter>[MASACODE] = 20002</Filter>
      &maxscale_zoom18;
      <LineSymbolizer stroke-linejoin="round" stroke="grey" stroke-width="19" stroke-linecap="round"/>
    </Rule>

  <!-- motorway -->
    <Rule>
      <Filter>[MASACODE] = 20001</Filter>
      &maxscale_zoom13;
      &minscale_zoom13;
      <LineSymbolizer stroke-linejoin="round" stroke="grey" stroke-width="5" stroke-linecap="round"/>
    </Rule>
    <Rule>
      <Filter>[MASACODE] = 20001</Filter>
      &maxscale_zoom14;
      &minscale_zoom14;
      <LineSymbolizer stroke-linejoin="round" stroke="grey" stroke-width="6" stroke-linecap="round"/>
    </Rule>
    <Rule>
      <Filter>[MASACODE] = 20001</Filter>
      &maxscale_zoom15;
      &minscale_zoom16;
      <LineSymbolizer stroke-linejoin="round" stroke="grey" stroke-width="8" stroke-linecap="round"/>
    </Rule>
    <Rule>
      <Filter>[MASACODE] = 20001</Filter>
      &maxscale_zoom17;
      &minscale_zoom17;
      <LineSymbolizer stroke-linejoin="round" stroke="grey" stroke-width="13" stroke-linecap="round"/>
    </Rule>
    <Rule>
      <Filter>[MASACODE] = 20001</Filter>
      &maxscale_zoom18;
      <LineSymbolizer stroke-linejoin="round" stroke="grey" stroke-width="19" stroke-linecap="round"/>
    </Rule>
  </Style>

  <Style name="road-small">
    <Rule>
      <Filter>[MASACODE] = 20004</Filter>
      <LineSymbolizer stroke-linejoin="round" stroke="#fed7a5" stroke-width="1" stroke-linecap="round"/>
      &maxscale_zoom14;
      &minscale_zoom15;
    </Rule>
    <Rule>
      <Filter>[MASACODE] = 20004</Filter>
      &maxscale_zoom15;
      &minscale_zoom16;
      <LineSymbolizer stroke-linejoin="round" stroke="#fed7a5" stroke-width="2" stroke-linecap="round"/>
    </Rule>
    <Rule>
      <Filter>[MASACODE] = 20004</Filter>
      &maxscale_zoom17;
      &minscale_zoom17;
      <LineSymbolizer stroke-linejoin="round" stroke="#fed7a5" stroke-width="4" stroke-linecap="round"/>
    </Rule>
    <Rule>
      <Filter>[MASACODE] = 20004</Filter>
      &maxscale_zoom18;
      <LineSymbolizer stroke-linejoin="round" stroke="#fed7a5" stroke-width="7" stroke-linecap="round"/>
    </Rule>
  </Style>

  <Style name="road-medium">
    <Rule>
      <Filter>[MASACODE] = 20003</Filter>
      <LineSymbolizer stroke-linejoin="round" stroke="#fed7a5" stroke-width="2" stroke-linecap="round"/>
      &maxscale_zoom14;
      &minscale_zoom15;
    </Rule>
    <Rule>
      <Filter>[MASACODE] = 20003</Filter>
      &maxscale_zoom15;
      &minscale_zoom16;
      <LineSymbolizer stroke-linejoin="round" stroke="#fed7a5" stroke-width="4" stroke-linecap="round"/>
    </Rule>
    <Rule>
      <Filter>[MASACODE] = 20003</Filter>
      &maxscale_zoom17;
      &minscale_zoom17;
      <LineSymbolizer stroke-linejoin="round" stroke="#fed7a5" stroke-width="9" stroke-linecap="round"/>
    </Rule>
    <Rule>
      <Filter>[MASACODE] = 20003</Filter>
      &maxscale_zoom18;
      <LineSymbolizer stroke-linejoin="round" stroke="#fed7a5" stroke-width="11" stroke-linecap="round"/>
    </Rule>
  </Style>

  <Style name="road-large">
    <Rule>
      <Filter>[MASACODE] = 20002</Filter>
      <LineSymbolizer stroke-linejoin="round" stroke="#f4c3c4" stroke-width="3" stroke-linecap="round"/>
      &maxscale_zoom13;
      &minscale_zoom13;
    </Rule>
    <Rule>
      <Filter>[MASACODE] = 20002</Filter>
      &maxscale_zoom14;
      &minscale_zoom14;
      <LineSymbolizer stroke-linejoin="round" stroke="#f4c3c4" stroke-width="4" stroke-linecap="round"/>
    </Rule>
    <Rule>
      <Filter>[MASACODE] = 20002</Filter>
      &maxscale_zoom15;
      &minscale_zoom16;
      <LineSymbolizer stroke-linejoin="round" stroke="#f4c3c4" stroke-width="6" stroke-linecap="round"/>
    </Rule>
    <Rule>
      <Filter>[MASACODE] = 20002</Filter>
      &maxscale_zoom17;
      &minscale_zoom17;
      <LineSymbolizer stroke-linejoin="round" stroke="#f4c3c4" stroke-width="11" stroke-linecap="round"/>
    </Rule>
    <Rule>
      <Filter>[MASACODE] = 20002</Filter>
      &maxscale_zoom18;
      <LineSymbolizer stroke-linejoin="round" stroke="#f4c3c4" stroke-width="17" stroke-linecap="round"/>
    </Rule>
  </Style>

  <Style name="road-motorway">
    <Rule>
      <Filter>[MASACODE] = 20001</Filter>
      <LineSymbolizer stroke-linejoin="round" stroke="#809bc0" stroke-width="3" stroke-linecap="round"/>
      &maxscale_zoom13;
      &minscale_zoom13;
    </Rule>
    <Rule>
      <Filter>[MASACODE] = 20001</Filter>
      &maxscale_zoom14;
      &minscale_zoom14;
      <LineSymbolizer stroke-linejoin="round" stroke="#809bc0" stroke-width="4" stroke-linecap="round"/>
    </Rule>
    <Rule>
      <Filter>[MASACODE] = 20001</Filter>
      &maxscale_zoom15;
      &minscale_zoom16;
      <LineSymbolizer stroke-linejoin="round" stroke="#809bc0" stroke-width="6" stroke-linecap="round"/>
    </Rule>
    <Rule>
      <Filter>[MASACODE] = 20001</Filter>
      &maxscale_zoom17;
      &minscale_zoom17;
      <LineSymbolizer stroke-linejoin="round" stroke="#809bc0" stroke-width="11" stroke-linecap="round"/>
    </Rule>
    <Rule>
      <Filter>[MASACODE] = 20001</Filter>
      &maxscale_zoom18;
      <LineSymbolizer stroke-linejoin="round" stroke="#809bc0" stroke-width="17" stroke-linecap="round"/>
    </Rule>
  </Style>

  <Layer name="forest" srs="+proj=longlat +ellps=WGS84 +datum=WGS84 +no_defs" >
      <StyleName>forest</StyleName>
      <Datasource>
        <Parameter name="type">shape</Parameter>
        <Parameter name="file">forest.shp</Parameter>
      </Datasource>
  </Layer>

  <Layer name="plantation" srs="+proj=longlat +ellps=WGS84 +datum=WGS84 +no_defs" >
      <StyleName>plantation</StyleName>
      <Datasource>
        <Parameter name="type">shape</Parameter>
        <Parameter name="file">plantation.shp</Parameter>
      </Datasource>
  </Layer>

  <Layer name="swamp" srs="+proj=longlat +ellps=WGS84 +datum=WGS84 +no_defs" >
      <StyleName>swamp</StyleName>
      <Datasource>
        <Parameter name="type">shape</Parameter>
        <Parameter name="file">swamp.shp</Parameter>
      </Datasource>
  </Layer>

  <Layer name="urban" srs="+proj=longlat +ellps=WGS84 +datum=WGS84 +no_defs">
      <StyleName>urban</StyleName>
      <Datasource>
        <Parameter name="type">shape</Parameter>
        <Parameter name="file">urban.shp</Parameter>
      </Datasource>
  </Layer>

  <Layer name="mountain" srs="+proj=longlat +ellps=WGS84 +datum=WGS84 +no_defs">
      <StyleName>mountain</StyleName>
      <StyleName>dune</StyleName>
      <Datasource>
        <Parameter name="type">shape</Parameter>
        <Parameter name="file">mountain.shp</Parameter>
      </Datasource>
  </Layer>

  <Layer name="cliff" srs="+proj=longlat +ellps=WGS84 +datum=WGS84 +no_defs">
      <StyleName>cliff</StyleName>
      <Datasource>
        <Parameter name="type">shape</Parameter>
        <Parameter name="file">cliff.shp</Parameter>
      </Datasource>
  </Layer>

  <Layer name="water" srs="+proj=longlat +ellps=WGS84 +datum=WGS84 +no_defs">
      <StyleName>water</StyleName>
      <Datasource>
        <Parameter name="type">shape</Parameter>
        <Parameter name="file">water.shp</Parameter>
      </Datasource>
  </Layer>

  <Layer name="river" srs="+proj=longlat +ellps=WGS84 +datum=WGS84 +no_defs">
      <StyleName>river</StyleName>
      <Datasource>
        <Parameter name="type">shape</Parameter>
        <Parameter name="file">river.shp</Parameter>
      </Datasource>
  </Layer>

  <Layer name="bridge" srs="+proj=longlat +ellps=WGS84 +datum=WGS84 +no_defs">
      <StyleName>bridge</StyleName>
      <Datasource>
        <Parameter name="type">shape</Parameter>
        <Parameter name="file">bridge.shp</Parameter>
      </Datasource>
  </Layer>

  <Layer name="road" srs="+proj=longlat +ellps=WGS84 +datum=WGS84 +no_defs">
      <StyleName>road-casing</StyleName>
      <StyleName>road-small</StyleName>
      <StyleName>road-medium</StyleName>
      <StyleName>road-large</StyleName>
      <Datasource>
        <Parameter name="type">shape</Parameter>
        <Parameter name="file">road.shp</Parameter>
      </Datasource>
  </Layer>

  <Layer name="tunnel" srs="+proj=longlat +ellps=WGS84 +datum=WGS84 +no_defs">
      <StyleName>tunnel</StyleName>
      <Datasource>
        <Parameter name="type">shape</Parameter>
        <Parameter name="file">tunnel.shp</Parameter>
      </Datasource>
  </Layer>

  <Layer name="railroad" srs="+proj=longlat +ellps=WGS84 +datum=WGS84 +no_defs">
      <StyleName>railroad</StyleName>
      <Datasource>
        <Parameter name="type">shape</Parameter>
        <Parameter name="file">railroad.shp</Parameter>
      </Datasource>
  </Layer>

  <Layer name="road" srs="+proj=longlat +ellps=WGS84 +datum=WGS84 +no_defs">
      <StyleName>road-motorway</StyleName>
      <Datasource>
        <Parameter name="type">shape</Parameter>
        <Parameter name="file">road.shp</Parameter>
      </Datasource>
  </Layer>

  <Layer name="road-text" srs="+proj=longlat +ellps=WGS84 +datum=WGS84 +no_defs">
    <StyleName>road-text</StyleName>
    <Datasource>
        <Parameter name="type">shape</Parameter>
        <Parameter name="file">road.shp</Parameter>
    </Datasource>
  </Layer>

</Map>
