<Map background-color="lightgrey">

  <FontSet name="book-fonts">
    <Font face-name="DejaVu Sans Book"/>
  </FontSet>
  <FontSet name="bold-fonts">
    <Font face-name="DejaVu Sans Bold"/>
  </FontSet>

  <Style name="water">
    <Rule>
      <PolygonSymbolizer gamma=".75" fill="#b3d1ff"/>
    </Rule>
  </Style>

  <Style name="forest">
    <Rule>
      &minscale_zoom13;
      <PolygonSymbolizer fill="#8dc56c"/>
    </Rule>
    <Rule>
      &maxscale_zoom14;
      <PolygonPatternSymbolizer file="symbols/forest.png"/>
    </Rule>
  </Style>

  <Style name="plantation">
    <Rule>
      &minscale_zoom13;
      <PolygonSymbolizer fill="#9ed88f"/>
    </Rule>
    <Rule>
      &maxscale_zoom14;
      <PolygonPatternSymbolizer file="symbols/orchard.png"/>
    </Rule>
  </Style>

 <Style name="swamp-underlay">
    <Rule>
