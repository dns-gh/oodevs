      <LineSymbolizer stroke-linejoin="round" stroke="#b3d1ff" stroke-width="12" stroke-linecap="round"/>
    </Rule>
  </Style>

  <Style name="urban">
    <Rule>
      <LineSymbolizer stroke="grey" stroke-width="1"/>
      <PolygonSymbolizer fill="#ededed"/>
    </Rule>
  </Style>

  <Style name="urban-text">
    <Rule>
      <TextSymbolizer size="14" fill="#777777" fontset-name="book-fonts" halo-radius="1" wrap-width="20" ><![CDATA[[NAME]]]></TextSymbolizer>
    </Rule>
  </Style>

  <Style name="block">
