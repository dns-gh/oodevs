<?xml version="1.0" encoding="utf-8"?>
<Project xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup
    Condition="'$(ProtocingBeforeTargets)' == '' and '$(ProtocingAfterTargets)' == '' and '$(ConfigurationType)' != 'Makefile'">
    <ProtocingBeforeTargets>Midl</ProtocingBeforeTargets>
    <ProtocingAfterTargets>CustomBuild</ProtocingAfterTargets>
  </PropertyGroup>
  <PropertyGroup>
    <ProtocingDependsOn
      Condition="'$(ConfigurationType)' != 'Makefile'">_SelectedFiles;$(ProtocingDependsOn)</ProtocingDependsOn>
  </PropertyGroup>
  <ItemDefinitionGroup>
    <Protocing>
      <CommandLineTemplate>..\..\bin\python\python $(ProjectDir)../cppproto.py %(FullPath) $(IntDir) $(IntDir)proto</CommandLineTemplate>
      <Outputs>$(IntDir)proto/%(Filename).pb.cc;$(IntDir)proto/%(Filename).pb.h;%(Outputs)</Outputs>
      <ExecutionDescription>protoc%27ing %(Filename).proto</ExecutionDescription>
    </Protocing>
  </ItemDefinitionGroup>
</Project>